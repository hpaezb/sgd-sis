<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"          xmlns:fmt="http://java.sun.com/jsp/jstl/fmt">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventAsignacionDetalle();});</script>                                    <div class="ui-datatable-fixed" id="divListaAsignaciones">        <input type="hidden" id="idCoAsignacionFuncionario"/>        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                            <th width="5%"></th>                            <th width="10%">Fecha de Registro</th>                            <th width="20%">Dependencia</th>                            <th width="25%">Empleado Encargado</th>                            <th width="10%">Tipo de Encargatura</th>                            <th width="10%">Nro Doc.Encargatura</th>                            <th width="5%">Fecha de<br/> Inicio</th>                            <th width="5%">Fecha de<br/> Fin</th>                            <th width="10%">Estado</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:300px;'>               <table class="ui-datatable-data" id="tblDepDet" style="width:100%;">                <tbody>                    <c:forEach var="item" items="${lista}" varStatus="status">                                           <tr>                            <td style="display:none;">${item.coAsignacionFuncionario}</td>                            <td width="5%">                                                                    <!--<button title ="Eliminar" class="btn btn-default btn-xs" onclick="fu_eliminaAsignacionEmpleado('${item.coAsignacionFuncionario}');"><span class="glyphicon glyphicon-remove"><jsp:text/></span></button>-->                                    <button title ="Editar" class="btn btn-default btn-xs" onclick="fu_goDialogEditarAsignacionFuncionario('${item.coAsignacionFuncionario}');"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                           </td>                            <td width="10%">                                <fmt:formatDate type="date" value="${item.feUseCre}" pattern="dd/MM/yyyy" />                            </td>                                                        <td width="20%"><c:out value="${item.deDependencia}"/></td>                            <td width="25%"><c:out value="${item.deEmpleado}"/></td>                            <td width="10%"><c:out value="${item.deTipoEncargo}"/></td>                            <td width="10%"><c:out value="${item.deDocAsigna}"/></td>                            <td width="5%">                                <fmt:formatDate type="date" value="${item.feInicio}" pattern="dd/MM/yyyy" />                            </td>                            <td width="5%">                                                                <c:if test="${item.coTipoEncargo eq '1'}">                                    --                                </c:if>                                <c:if test="${item.coTipoEncargo !='1'}">                                    <fmt:formatDate type="date" value="${item.feFin}" pattern="dd/MM/yyyy" />                                </c:if>                            </td>                            <td width="10%"><c:out value="${item.deAsignacion}"/></td>                        </tr>                    </c:forEach>                </tbody>			            </table>        </div>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>  </jsp:root>