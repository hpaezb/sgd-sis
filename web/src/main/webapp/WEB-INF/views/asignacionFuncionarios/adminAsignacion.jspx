<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_inicializaAsignacionFuncionarios('#frmBuscarAsignacion');});</script>  	        <div class="ui-panel ui-corner-all" style="" id="divDependenciaBody">        <div class="ui-panel-titlebar ui-widget-header ui-corner-all">            <span>Asignación de Funcionarios a Unidades Orgánicas</span>        </div>        <div class="ui-panel-body">            <fieldset>                <div class="btn-group btn-group-lg">                    <button id="btn-nuevo" title="Nuevo" class="btn btn-primary btn-lg" onclick="fu_goDialogAsignacionFuncionario();" type="button"><span class="glyphicon glyphicon-file"></span> Nuevo</button>                    <button id="btn-editar" title="Editar" class="btn btn-primary btn-lg" onclick="fu_llamarEditar();" type="button"><span class="glyphicon glyphicon-edit"></span> Editar</button>                    <button id="btn-salir" title="Salir" value = "Salir" class="btn btn-primary btn-lg" onclick="cerrarPantallaConfigTabla();" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>            <fieldset>                <form:form modelAttribute="asignacionFuncionarioBean" id="frmBuscarAsignacion">                                        <form:hidden path="feRegistraInicio" id="feRegistraInicio"/>                      <form:hidden path="feRegistraFin" id="feRegistraFin"/>                    <form:hidden path="coDependencia" id="coDependencia"></form:hidden>                    <legend>Buscar</legend>                    <table>                        <tbody>                            <tr>                                <td>Dependencia :</td>                                <td width="500px">                                    <div class="input-group anchoTotal">                                        <input readonly="readonly" class="form-control" id="deDependencia" type="text" />                                        <a href="javascript:;" id="btn-SelUbi" title="Seleccionar Dependencia" class="input-group-addon btn-default btnPequenoSGD" onclick="fn_buscaDependenciaAsignacion();" ><span class="glyphicon glyphicon-search"></span></a>                                    </div>                                  </td>                                <td></td>                                <td>                                                                        <div id="fechaFiltro" class="cajaFechas">                                        <div class="cajaIcono"><span class="glyphicon glyphicon-calendar fa fa-calendar"><jsp:text/></span></div>                                        <div name="labelDate" class="cajaEtiqueta"><jsp:text/></div>                                        <div class="cajaTriangulo"><b class="caret"><jsp:text/></b></div>                                    </div>                                </td>                                <td>                                    <button id="btn-buscar" title="Buscar" class="btn btn-default btn-sm" onclick="fn_filtrarAsignacionFuncionario('#frmBuscarAsignacion');" type="button"><span class="glyphicon glyphicon-filter"></span> Buscar</button>                                    <button title ="Limpiar datos de Filtro" class="btn btn-default btn-sm" type="button" onclick="fn_limpiarFormAsignacion('#frmBuscarAsignacion');return false;">                                        <span class="glyphicon glyphicon-remove"><jsp:text/></span>                                    </button>                                </td>                            </tr>                        </tbody>                    </table>                </form:form>            </fieldset>            <fieldset>                <legend>Asignaciones de Funcionarios</legend>                <div id="divDependenciaDetalle">                                        <jsp:include page="adminAsignacionLista.jspx"/>                </div>            </fieldset>        </div>    </div>    <script languaje="javascript">jQuery(document).ready(function() {fn_filtrarAsignacionFuncionario('#frmBuscarAsignacion');});</script>                        <div id="divDependenciaEdit"><jsp:text/></div>                    </jsp:root>