<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <form:form modelAttribute="configDatePicker">        <input type="hidden" id="anioDataPickerInicial" value="${anioDataPickerInicial}"/>        <input type="hidden" id="anioDataPickerFinal" value="${anioDataPickerFinal}"/>        <div class="seleccionaFecha openCachito" style="">            <div class="selectContent">                <ul style="padding-left: 0px;">                    <li>Hoy</li>                    <li>Ayer</li>                    <li>Mes actual</li>                    <li>Últimos 30 días</li>                    <li>Año Actual</li>                    <li>Año Pasado</li>                    <li>Año/Mes</li>                    <li>Rango de fechas</li>                    <li>Todos</li>                </ul>                <div style="width: 160px;">                    <div style="float: left;margin-right: 12px;">                        <label>DESDE</label>                        <input readonly="readonly" id="DPfechaInicial" class="input-mini" type="text" maxlength="10"/>                    </div>                    <div>                        <label>HASTA</label>                        <input readonly="readonly" id="DPfechaFinal" class="input-mini" type="text" maxlength="10"/>                    </div>                    <button id="DPAceptar" type="button" class="btn btn-sm btn-success" style="margin-right: 5px;">Aceptar</button>                    <button id="DPCancelar" type="button" class="btn btn-sm btn-default">Cancelar</button>                </div>            </div>            <div class="rangoFechas" style="display: none;">                <div id="divFechaInicial" class="fechaIni"><jsp:text/></div>                <div id="divFechaFinal" class="fechaFin"><jsp:text/></div>            </div>            <div class="anioMes" style="display: none;">                <div style="margin: 5px;">                    <div class="comboFechas" >                        <p>AÑO:</p>                        <form:select path="anio" id="DPAnio" onchange="setNumeroDiasYRangoFechas();" >                            <form:options items="${conAnioList}" itemLabel="deAnio" itemValue="coAnio" cssStyle="width:85px;" />                        </form:select>                    </div>                    <div class="comboFechas">                        <p>MES:</p>                        <form:select path="mes" id="DPMes" onchange="setNumeroDiasYRangoFechas();" >                            <form:options items="${conMesList}" itemLabel="deMes" itemValue="coMes" cssStyle="width:85px;" />                        </form:select>                    </div>                </div>            </div>        </div>        <script>            $(document).ready(function(){                fn_cargarParametrosIniciales();            });        </script>    </form:form></jsp:root>