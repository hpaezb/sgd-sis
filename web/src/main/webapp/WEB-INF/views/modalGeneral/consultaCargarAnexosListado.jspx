<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <span id="btn-ReemplazarDocIE" class="btn btn-default btn-file" style="width: 4px; height: 11px;">            <span class="glyphicon glyphicon-transfer"></span><input id="fileuploadReIE" type="file" name="files[]" data-url="" multiple="" style="width: 28px; height: 23px; top: -26px;right: 12px;"/>        </span>        <button id="btn-ReemplazarDoc" title="Reemplazar documento" class="btn btn-default btn-xs" onclick="fn_reemplazarDocumento();" type="button"><span class="glyphicon glyphicon-transfer"></span></button>        <button id="btn-eliminar" title="Eliminar documento" class="btn btn-default btn-xs" onclick="fn_eliminarDocumento();" type="button"><span class="glyphicon glyphicon-trash"></span></button>                <button id="btn-firmar" title="Firmar documento" class="btn btn-success btn-xs" onclick="fn_firmarDocumentoAnexo(this);" type="button"><span class="glyphicon glyphicon-pencil"></span></button>            </div>    <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                <thead>                    <tr>                        <!--<th width="10%">Inf. <br/>Public.</th>-->                        <th width="37%">Descripci√≥n</th>                        <th width="37%">Nombre de anexo</th>                        <th width="10%">Req. Firmar</th>                        <th width="16%">Opciones</th>                    </tr>                </thead>            </table>        </div>        <script>            //width: 27px; height: 26px; top: -20px; bottom: 20px; right: 1px;            //width: 27px; border-width: 1px; padding: 0px; height: 24px;            //visibility: hidden;display: none;            arrayDetalles = [];            arrayNuAne = [];        </script>        <div class="bx_sb ui-datatable-scrollable-body" style='height:115px;'>            <table class="ui-datatable-edit-data" id="tblDocAnexos" cellpadding="0" cellspacing="0" style="width:100%;">                <tbody>                    <c:forEach var="refDocAdm" items="${docAnexoList}" varStatus="status">                        <tr id="Anexo${refDocAdm.nuAne}" >                            <!--<td width="4%" align="center">                                <c:choose>                                        <c:when test="${refDocAdm.tiPublic eq '1'}">                                            <input type="checkbox" name="tiPublic" checked="true"/>                                        </c:when>                                        <c:otherwise>                                            <input type="checkbox" name="tiPublic"/>                                        </c:otherwise>                                </c:choose>                                                             </td>-->                                                        <td width="36%">                                <input name="txtDetalleAnexo" title="No se puede grabar campos vacios." required="" type="text" value="${refDocAdm.deDet}" style="width:97%;"/>                                <script>arrayDetalles.push(["${refDocAdm.deDet}",${refDocAdm.reqFirma}]);                                    arrayNuAne.push("${refDocAdm.nuAne}");</script>                            </td>                            <td width="36%">                                <input name="txtNombreAnexo" type="text" value="${refDocAdm.deRutOri}" readonly="readonly" style="width:97%;"/>                            </td>                            <td width="4%" align="center">                                <c:choose>                                        <c:when test="${refDocAdm.reqFirma eq '1'}">                                            <input type="checkbox" name="reqFirma" checked="true"/>                                        </c:when>                                        <c:otherwise>                                            <input type="checkbox" name="reqFirma"/>                                        </c:otherwise>                                </c:choose>                                                             </td>                            <td width="24%" align="">                                <div id="btn-opciones">                                    <button id="btn-VerDocumento" title="Ver documento" class="btn btn-default btn-xs" type="button" onclick="fn_verDocAnexo('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuAne="${refDocAdm.nuAne}" ><span class="glyphicon glyphicon-folder-open"></span></button>                                                                    </div>                            </td>                        </tr>                    </c:forEach>                </tbody>            </table>        </div>                        </div>    <input type="hidden" name="oNuAnn" id="oNuAnn" />    <input type="hidden" name="oNuEmi" id="oNuEmi" />    <input type="hidden" name="oNuAne" id="oNuAne" />        <input id="fileSizeMaxCargo" name="fileSizeMaxCargo" type="hidden" value="${fileSizeMaxCargo}"/>    <input id="rutaDocFirmaAnexo" name="rutaDocFirmaAnexo" type="hidden" value=""/>    <input id="nuSecuenciaFirmaAnexo" name="nuSecuenciaFirmaAnexo" type="hidden" value=""/>    <input id="noPrefijoAnexo" name="noPrefijoAnexo" type="hidden" value=""/>    <script>        fn_eventDocAnexos();        $("#windowConsultaCargarAnexo").show('fast');        jQuery(".tree ul.dynatree-container").css('width', '100%').css('width', '-=20px');        jQuery(".tree ul.dynatree-container").css('height', '100%').css('height', '-=20px');    </script></jsp:root>