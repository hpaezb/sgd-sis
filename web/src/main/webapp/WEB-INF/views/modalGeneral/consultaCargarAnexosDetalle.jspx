<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <input type="hidden" id="rutactx" name="rutactx" value="${rutactx}"/>    <input type="hidden" id="nuAnnDocGenerado" name="nuAnnDocGenerado" value="${docRemitoBean.nuAnn}"/>    <input type="hidden" id="nuEmiDocGEnerado" name="nuEmiDocGenerado" value="${docRemitoBean.nuEmi}"/>    <fieldset>        <legend>Detalle</legend>        <table >            <tbody>                <tr>                    <td>Año:</td>                    <td><input id="txtAnio" size="3" style="width: 135px;" type="text" readonly="readonly" value="${docRemitoBean.nuAnn}" ok=""/></td>                    <td>Fecha:</td>                    <td ><input id="txtFecha" size="33"  type="text" style="width: 150px;" readonly="readonly" value="${docRemitoBean.feEmi}"/></td>                    <td><jsp:text/></td>                </tr>                <tr class="hide">                    <td>Local emisor:</td>                    <td colspan="4"><input id="txtLocalEmisor" size="74" style="width: 490px;" type="text" readonly="readonly" value=""/></td>                </tr>                <tr>                    <td>Dependencia:</td>                    <td colspan="4" ><input id="txtDependencia" size="73" style="width: 490px;" type="text" readonly="readonly" value="${docRemitoBean.deDepEmi}"/></td>                </tr>                <tr>                    <td>Tipo de documento:</td>                    <td ><input id="txtTipoDoc" size="33"  type="text" style="width: 150px;" readonly="readonly" value="${docRemitoBean.deTiDoc}"/></td>                    <td>Número:</td>                    <td><input id="txtNumeroDoc" size="33"  type="text" style="width: 150px;" readonly="readonly" value="${docRemitoBean.nuDoc}"/></td>                    <td >                        <div id="btn-abrirDocumento">                            <button title ="Ver Documento" class="btn btn-default btn-sm" onclick="fn_verDocumento('${docRemitoBean.nuAnn}', '${docRemitoBean.nuEmi}');" type="button">                                <span class="glyphicon glyphicon-folder-open"/> Ver documento                             </button>                        </div>                    </td>                </tr>                <tr>                    <td>Asunto:</td>                    <td colspan="4"><textarea id="txtAsunto" cols="70" style="width: 490px; height: 41px;" rows="4" readonly="readonly">${docRemitoBean.deAsu}</textarea></td>                </tr>            </tbody>        </table>    </fieldset>    <fieldset>        <legend>Documentos anexos</legend>        <div id="divHerramientasAnexar">            <table>                <tr>                    <td>                        <div class="btn-group btn-group-sm">                            <span id="btn-fileuploadIE" class="btn btn-default btn-file">                                <span class="glyphicon glyphicon-open"></span> Carga Anexoss <input id="fileuploadIE" type="file" name="files[]" data-url="${rutactx}/srDocObjeto.do?accion=goUploadIE&amp;pkEmi=${docRemitoBean.nuAnn}${docRemitoBean.nuEmi}" multiple=""/>                            </span>                            <button id="btn-fileupload" title="Cargar anexo" class="btn btn-default btn-lg" onclick="fn_cargarAnexos();" type="button"><span class="glyphicon glyphicon-open"></span> Cargar anexo</button>                            <button id="btn-guardar" title="Guardar cambios" class="btn btn-default btn-lg" onclick="fn_GuardarCambios();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>                            <button id="btn-actualizar" title="Actualizar" class="btn btn-default btn-lg" onclick="fn_actualizar('${nodoPadre_pkEmiDoc}');" type="button"><span class="glyphicon glyphicon-refresh"></span> Actualizar</button>                            <button id="btn-descargaZip" title="Descargar todos los Archivos en Zip" class="btn btn-default btn-lg" onclick="fn_DescargarArchivosZip();" type="button"><span class="glyphicon glyphicon-save"></span>Descargar Zip</button>                        </div>                    </td>                    <td>                        <div>                            <input id="fileupload" type="file" name="files[]" data-url="${rutactx}/srDocObjeto.do?accion=goUpload&amp;pkEmi=${docRemitoBean.nuAnn}${docRemitoBean.nuEmi}" multiple="" style="display: none;"/>                            <div id="progress" class="progress" style="margin-top:-10px;position:absolute;width:274px;">                                <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">                                    <span style="top: 3px; position: relative;"><jsp:text/></span>                                </div>                            </div>                        </div>                    </td>                </tr>            </table>        </div>        <div id="divError" class="alert alert-danger" style="display:none">            <ul id="divErrorLista"><jsp:text/></ul>        </div>        <script>            var arrayDetalles = Array();            var arrayNuAne = Array();            var arrayDocEliminar = Array();            fn_cargarAnexosIE();            var nodoPadre="${nodoPadre_pkEmiDoc}";            bloquearAbrirDocumento(nodoPadre);            $("#divError").hide();            actualizarListadoAnexos();            $("#rutactx").attr("value", "${rutactx}");            $("#progress").hide();            $("#txtAsunto").showBigTextBox();        </script>        <div id="divListadoArchivosAnexos"><jsp:text/></div>    </fieldset></jsp:root>