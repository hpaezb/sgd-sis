<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:500px; height:400px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro de Avances"/>        <tiles:putAttribute name="body">            <script>                //$(document).ready(function() {                 //   $("#divConfigAvance input:radio").prop("disabled", true);                //});            </script>            <div id="divConfigAvance">                                    <fieldset>                        <div class="btn-group btn-group-lg">                            <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_verRegistrarAvance();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span>Guardar</button>                            <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span>Salir</button>                        </div>                    </fieldset>                    <fieldset>                        <legend>Detalle</legend>                        <table width="450px;">                            <tr>                                <td>Observación:</td>                             </tr>                              <tr>                                <td>                                 <textarea class="delUpperCase"  id="txtAvanceDescripcion" maxlength="2000" style="width:100%;height: 80px;"  ></textarea>                                </td>                             </tr>                            <tr>                                <td style='height:150px;overflow:auto;display: block;  '>                                  <table id ="myTableFixed" class="display dataTable no-footer ui-datatable-fixed" width="100%">                                    <thead>                                        <tr class="ui-state-default">                                             <th width="10%">Fecha</th>                                            <th width="90%">Observación:</th>                                        </tr>                                    </thead>                                    <tbody >                                    <c:forEach var="recepAvance" items="${recepAvanceList}" varStatus="status">                                        <tr>                                            <td class="gradeA" >${recepAvance.feAvance}</td>                                            <td class="gradeA" style="white-space: normal;" >${recepAvance.deAvance}</td>                                        </tr>                                    </c:forEach>                                    </tbody>                                 </table>                                </td>                            </tr>                        </table>                        <br/>                                                <input type="hidden" id="tipAvance" value="${tipoAvance}"/>                           <input type="hidden" id="txtpnuAnnAvance" value="${pnuAnn}"/>                            <input type="hidden" id="txtpnuEmiAvance" value="${pnuEmi}"/>                             <input type="hidden" id="txtpnuDesAvance" value="${pnuDes}"/>                                                </fieldset>                            </div>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>