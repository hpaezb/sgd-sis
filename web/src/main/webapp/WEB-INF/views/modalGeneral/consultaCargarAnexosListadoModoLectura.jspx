<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div class="ui-datatable-fixed">        <table id ="tblAnexosDetalle" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th width="200px;">Descripci√≥n</th>                    <th width="220px;">Nombre de anexo</th>                    <th width="30px;">Opciones</th>                </tr>            </thead>				            <tbody>                <c:forEach var="refDocAdm" items="${docAnexoList}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td>${refDocAdm.deDet}</td>                        <td>${refDocAdm.deRutOri}</td>                        <td>                            <button id="btn-VerDocumento" title="Ver documento" class="btn btn-default btn-xs" type="button" onclick="fn_verDocAnexo('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuAne="${refDocAdm.nuAne}" >                                <span class="glyphicon glyphicon-folder-open"><jsp:text/></span>                            </button>                            <button id="btn-CapturarAnexo" title="Traer el anexo al proyecto de documento" class="btn btn-default btn-xs" type="button" onclick="fn_capturarAnexo('${refDocAdm.nuAnn}', '${refDocAdm.nuEmi}', '${refDocAdm.nuAne}');" nuAnn="${refDocAdm.nuAnn}" nuEmi="${refDocAdm.nuEmi}" nuAne="${refDocAdm.nuAne}" >                                <span class="glyphicon glyphicon-export"><jsp:text/></span>                            </button>                        </td>                    </tr>                </c:forEach>            </tbody>			        </table>    </div>    <script>    fu_callEventoTablaAnexosdetalle();    $("#windowConsultaCargarAnexo").show('fast');    jQuery(".tree ul.dynatree-container").css('width', '100%').css('width', '-=20px');    jQuery(".tree ul.dynatree-container").css('height', '100%').css('height', '-=20px');    </script></jsp:root>