<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:500px; height:400px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Consulta de Avances"/>        <tiles:putAttribute name="body">            <script>                //$(document).ready(function() {                 //   $("#divConfigAvance input:radio").prop("disabled", true);                //});            </script>            <div id="divConfigAvanceConsulta">                                                        <fieldset>                        <legend>Detalle</legend>                        <table width="450px;">                                                         <tr>                                <td style='height:150px;overflow:auto;display: block;  '>                                  <table id ="myTableFixed" class="display dataTable no-footer ui-datatable-fixed" width="100%">                                    <thead>                                        <tr class="ui-state-default">                                             <th width="10%">Fecha</th>                                            <th width="90%">Observaci√≥n:</th>                                        </tr>                                    </thead>                                    <tbody >                                    <c:forEach var="recepAvance" items="${recepAvanceList}" varStatus="status">    