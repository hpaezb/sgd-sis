<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:500px; height:250px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Vincular Tema"/>        <tiles:putAttribute name="body">            <script>                $(document).ready(function() {                    $("#divConfigPersonal input:radio").prop("disabled", true);                });            </script>            <div id="divConfigPersonal">                                    <fieldset>                        <div class="btn-group btn-group-lg">                            <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fu_grabarTema();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span>Guardar</button>                            <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span>Salir</button>                        </div>                    </fieldset>                    <fieldset>                        <legend>Detalle</legend>                        <table width="450px;">                            <tr>                                <td>Seleccionar Tema</td>                                <td><form:form modelAttribute="temaBean">                                        <form:select path="coTema"  >                                                <option  value="">.:SELECCIONAR:.</option>                                                  <form:options items="${deListTema}" itemValue="coTema" itemLabel="deTema" />                                         </form:select>                                </form:form>                                </td>                            </tr>                                                     </table>                        <br/>                                              <input type="hidden" id="codigoEmision" value="${codigoEmision}"/>                         <input type="hidden" id="codTipo" value="${codigoTipo}"/>                     </fieldset>                            </div>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>