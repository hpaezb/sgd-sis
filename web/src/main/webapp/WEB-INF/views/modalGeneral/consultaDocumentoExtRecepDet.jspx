    <fieldset>
    <legend>Detalle</legend>  
    <div class="ui-datatable ui-datatable-scrollable">
        <div class="ui-datatable-scrollable-header">
            <table class="ui-state-default" style="width: 100%!important">
                     <thead>
                            <tr>
