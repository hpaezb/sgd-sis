<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fu_iniTblGuiaMesaPartes();});</script>                                    <table cellpadding="0" cellspacing="0" width="100%">            <tr valign="top">                <td width="100%">                    <div class="ui-datatable-fixed">                    <div class="ui-datatable ui-datatable-scrollable">                        <div class="ui-datatable-scrollable-header">                               <table class="ui-state-default" style="width:100%;">                                     <thead>                                            <tr>                                                <th width="5%">Nro.</th>                                                <th width="11%">Tipo Doc.</th>                                                <th width="20%">Nro. Doc.</th>                                                <th width="7%">Fecha</th>                                                <th width="27%">Destinatario</th>                                                                                    <th width="30%">Remitente</th>                                            </tr>                                     </thead>                               </table>                        </div>                        <div class="ui-datatable-scrollable-body" style='height:200px;'>                              <table class="ui-datatable" id="tblDetGuiaMP" style="width:100%;">                                  <tbody>                                      <c:forEach var="detGuiaMp" items="${lDetalleGuia}" varStatus="status">                                        <c:choose>                                            <c:when test="${status.count % 2 == 0}">                                              <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                            </c:when>                                            <c:otherwise>                                              <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                            </c:otherwise>                                                                            </c:choose>                                                                                  <tr class="${rowStyle}">                                              <td style="display:none">${detGuiaMp.nuAnn}</td>                                              <td style="display:none">${detGuiaMp.nuEmi}</td>                                              <td style="display:none">${detGuiaMp.nuDes}</td>                                              <td width="5%">${detGuiaMp.nuCorEmi}</td>                                              <td width="11%">${detGuiaMp.deTipDocAdm}</td>                                                                                              <td width="20%">${detGuiaMp.nuDoc}</td>                                              <td width="7%">${detGuiaMp.feEmiCorta}</td>                                              <td width="27%">${detGuiaMp.deDepDes}</td>                                              <td width="30%">${detGuiaMp.deOriEmi}</td>                                                                                        </tr>                                      </c:forEach>                                    <c:if test="${empty lDetalleGuia}">                                        <td>No se encuentran registros.</td>                                    </c:if>                                                                           </tbody>                              </table>                        </div>                                        </div>                   </div>                      <c:if test="${not empty lDetalleGuia}">                    <div>Registros: ${lDetalleGuia.size()}</div>                </c:if>                                    </td>            </tr>        </table>    <div id="divflotante" class="divTip">&amp;nbsp;</div>    	    </jsp:root>