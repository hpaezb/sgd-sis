<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function(){fu_iniTblDocPendEntrega();});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnnGuia" name="txtpnuAnnGuia"/>        <input type="hidden" id="txtpnuGuia" name="txtpnuGuia"/>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                <table class="ui-state-default" style="width:100%;">                    <thead>                        <tr class="ui-state-default">                            <th width="5%">N° REG.</th>                            <th width="22%">REMITENTE</th>                            <th width="4%">FECHA</th>                            <th width="8%">TIPO DOC.</th>                                                <th width="7%">N° DOC</th>                            <th width="22%">DEPENDENCIA</th>                            <th width="18%">PERSONAL</th>                            <th width="12%">ASUNTO</th>                            <th width="2%">                                <input type="checkbox" id="chkIncluir" name="chkIncluir" onchange="fn_changeChkIncluirDocPedEntrega(this);"/>                            </th>                        </tr>                    </thead>                                    </table>            </div>            <div class="ui-datatable-scrollable-body" style='height:470px;'>                <table class="ui-datatable" id="tblDocPendEntrega" style="width:100%;">                    <tbody>                        <c:forEach var="docPendEntrega" items="${docPendEntregaLs}" varStatus="status">                                                                                                <tr class="gradeA" tabindex="0">                                    <td style="display:none">${docPendEntrega.nuAnn}</td>                                    <td style="display:none">${docPendEntrega.nuEmi}</td>                                    <td style="display:none">${docPendEntrega.nuDes}</td>                                    <td width="5%">${docPendEntrega.nuCorEmi}</td>                                    <td width="22%">${docPendEntrega.deOriEmi}</td>                                    <td width="4%">${docPendEntrega.feEmiCorta}</td>                                    <td width="8%">${docPendEntrega.deTipDocAdm}</td>                                    <td width="7%">${docPendEntrega.nuDoc}</td>                                    <td width="22%">${docPendEntrega.deDepDes}</td>                                    <td width="18%">${docPendEntrega.deEmpDes}</td>                                    <td width="12%">${docPendEntrega.deAsu}</td> <!-- Agregado para que pinte el asunto Posicion 11-->                                    <td width="2%">                                        <input type="checkbox" id="chkTblIncluir" name="chkTblIncluir"/>                                    </td>                                </tr>                        </c:forEach>                        <c:if test="${empty docPendEntregaLs}">                            <tr class="row_selected"> <td>No se encuentran registros.</td> </tr>                        </c:if>                    </tbody>                </table>                            </div>            <c:if test="${not empty docPendEntregaLs}">                <div>Registros: ${docPendEntregaLs.size()}</div>            </c:if>        </div>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	</jsp:root>	