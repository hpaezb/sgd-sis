<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function(){fu_iniTblCargoEntrega();});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnnGuia" name="txtpnuAnnGuia"/>        <input type="hidden" id="txtpnuGuia" name="txtpnuGuia"/>        <table id ="myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"></th>                    <th style="display:none"></th>                                        <th width="4%">NRO</th>                    <th width="5%">FECHA</th>                    <th width="30%">LOCAL</th>                    <th width="52%">DESTINO</th>                                        <th width="9%">ESTADO</th>                </tr>            </thead>				            <tbody>                <c:forEach var="cargo" items="${cargoEntregaLs}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${cargo.nuAnnGuia}</td>                        <td style="display:none">${cargo.nuGuia}</td>                                                <td>${cargo.nuCorGuia}</td>                        <td>${cargo.feGuiaMpCorta}</td>                        <td>${cargo.deLocDes}</td>                        <td>${cargo.deDepDes}</td>                        <td>${cargo.deEstadoGuiaMp}</td>                    </tr>                </c:forEach>            </tbody>			        </table>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	</jsp:root>	