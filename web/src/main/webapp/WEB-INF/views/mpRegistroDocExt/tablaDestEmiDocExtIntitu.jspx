<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventDestEmiDocAdm();});</script>                                    <table cellpadding="0" cellspacing="0" width="100%">            <tr valign="top">                <td>                 <div class="toolbar-left-vertical">                    <button title ="Agregar Destinatario" class="btn btn-default btn-xs" onclick="fn_addDestintarioEmi();" type="button">                      <span class="button.ui-button-icon-only ui-icon ui-icon-circle-plus"/>                    </button><br/><br/>                    <button title ="Eliminar Destinatario" class="btn btn-default btn-xs" onclick="fn_removeDestintarioEmi();" type="button">                      <span class="button.ui-button-icon-only ui-icon ui-icon-circle-minus"/>                    </button>                 </div>                </td>                <td width="100%">                    <input id="txtTblDestEmiFilaWhereButton" name="txtTblDestEmiFilaWhereButton" type="hidden"/>                    <input id="txtTblDestEmiColWhereButton" name="txtTblDestEmiColWhereButton" type="hidden"/>                    <div class="ui-datatable ui-datatable-scrollable">                        <div class="ui-datatable-scrollable-header">                               <table class="ui-state-default" style="width:100%;">                                     <thead>                                            <tr>                                                <th width="25%">Dependencia</th>                                                <th width="14%">Local</th>                                                <th width="25%">Personal</th>                                                <th width="14%">Tr√°mite</th>                                                <th width="12%">Indicaciones</th>                                                                                    <th width="8%">Prioridad</th>                                                <th width="2%"/>                                            </tr>                                     </thead>                               </table>                        </div>                        <div class="bx_sb ui-datatable-scrollable-body" style='height:115px;'>                              <table class="ui-datatable-edit-data" id="tblDestEmiDocAdm" cellpadding="0" cellspacing="0" style="width:100%;">                                  <tbody>                                      <tr class="oculto">                                        <td width="25%">                                            <input type="text" style="width:87.8%;" onkeyup="fu_cambioTxtEmiDoc(this);fu_FiltrarTecladoCadenaFunParam(event, false, public_apenom,this);"/>                                            <button title ="Buscar Dependencia" class="btn btn-default btn-xs" onclick="fn_buscaDependenciaDestEmitbl(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                            </button>                                        </td>                                        <td style="display:none"/>                                        <td width="14%">                                            <input type="text" style="width:77.8%;" onkeyup="fu_cambioTxtEmiDoc(this);"/>                                            <button title ="Buscar Local" class="btn btn-default btn-xs" type="button">                                                <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                            </button>                                        </td>                                        <td style="display:none"/>                                        <td width="25%">                                            <input type="text" style="width:87.8%;" onkeyup="fu_cambioTxtEmiDoc(this);"/>                                            <button title ="Buscar Personal" class="btn btn-default btn-xs" onclick="fn_buscaPersonalDestEmitbl(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                            </button>                                        </td>                                        <td style="display:none"/>                                        <td width="14%">                                            <input type="text" style="width:77.8%;" onkeyup="fu_cambioTxtEmiDoc(this);"/>                                            <button title ="Buscar Accion" class="btn btn-default btn-xs" onclick="fn_buscaAccionDocExtDestEmitbl(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                            </button>                                        </td>                                        <td style="display:none"/>                                        <td width="12%">                                            <textarea style="width:95%; height: 17px" class="delUpperCase" bt="true" maxlength="600"></textarea>                                        </td>                                        <td style="display:none"/>                                        <td width="8%">                                            <select style="width:100%;">                                                <c:forEach var="lPrioriDestEmi" items="${lstPrioridadDestEmi}" varStatus="status">                                                    <c:choose>                                                        <c:when test="${status.count == 1}">                                                            <option selected="selected" value="${lPrioriDestEmi.coPri}">${lPrioriDestEmi.dePri}</option>                                                         </c:when>                                                        <c:otherwise>                                                            <option value="${lPrioriDestEmi.coPri}">${lPrioriDestEmi.dePri}</option>                                                         </c:otherwise>                                                    </c:choose>                                                                                                    </c:forEach>                                            </select>                                        </td>                                        <td style="display:none">INS</td>                                        <td style="display:none">0NP3</td>                                        <td style="display:none">01</td>                                        <td width="2%">                                            <!--button title ="Eliminar Referencia" class="btn btn-default btn-xs" onclick="fn_removeReferenciaEmi(this);" type="button"-->                                            <div class="ui-state-error-text">                                                <span class="ui-icon ui-icon-alert" title="ERROR"/>                                            </div>                                                <!--/button-->                                                                                    </td>                                        <td style="display:none">0</td>                                      </tr>                                      <c:forEach var="lDestinatario" items="${lstDestintarioDocAdmEmi}" varStatus="status">                                          <tr>                                                <td width="25%">                                                    <input type="text" style="width:87.8%;" onkeyup="fu_cambioTxtEmiDoc(this);fu_FiltrarTecladoCadenaFunParam(event, false, public_apenom,this);" value="${lDestinatario.deDependencia}"/>                                                    <button title ="Buscar Dependencia" class="btn btn-default btn-xs" onclick="fn_buscaDependenciaDestEmitbl(this);" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${lDestinatario.coDependencia}</td>                                                <td width="14%">                                                    <input type="text" style="width:77.8%;" onkeyup="fu_cambioTxtEmiDoc(this);" value="${lDestinatario.deLocal}"/>                                                    <button title ="Buscar Local" class="btn btn-default btn-xs" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${lDestinatario.coLocal}</td>                                                <td width="25%">                                                    <input type="text" style="width:87.8%;" onkeyup="fu_cambioTxtEmiDoc(this);" value="${lDestinatario.deEmpleado}"/>                                                    <button title ="Buscar Personal" class="btn btn-default btn-xs" onclick="fn_buscaPersonalDestEmitbl(this);" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${lDestinatario.coEmpleado}</td>                                                <td width="14%">                                                    <input type="text" style="width:77.8%;" onkeyup="fu_cambioTxtEmiDoc(this);" value="${lDestinatario.deTramite}"/>                                                    <button title ="Buscar Accion" class="btn btn-default btn-xs" onclick="fn_buscaAccionDocExtDestEmitbl(this);" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${lDestinatario.coTramite}</td>                                                <td width="12%">                                                    <textarea style="width:95%; height: 17px" onkeyup="changeAccionBDDocEmi(this);" class="delUpperCase" bt="true" maxlength="600">${lDestinatario.deIndicaciones}</textarea>                                                </td>                                                <td style="display:none">${lDestinatario.nuDes}</td>                                                <td width="8%">                                                    <select style="width:100%;" onchange="changeAccionBDDocEmi(this);">                                                        <c:forEach var="lPrioriDestEmi" items="${lstPrioridadDestEmi}" varStatus="status">                                                            <c:choose>                                                                <c:when test="${lPrioriDestEmi.coPri == lDestinatario.coPrioridad}">                                                                    <option selected="selected" value="${lPrioriDestEmi.coPri}">${lPrioriDestEmi.dePri}</option>                                                                 </c:when>                                                                <c:otherwise>                                                                    <option value="${lPrioriDestEmi.coPri}">${lPrioriDestEmi.dePri}</option>                                                                 </c:otherwise>                                                            </c:choose>                                                                                                            </c:forEach>                                                    </select>                                                </td>                                                <td style="display:none">${lDestinatario.accionBD}</td>                                                <td style="display:none">${lDestinatario.deDependencia}$#${lDestinatario.deLocal}$#${lDestinatario.deEmpleado}$#${lDestinatario.deTramite}</td>                                                <td style="display:none">${lDestinatario.coTipoDestino}</td>                                                <td width="2%">                                                    <!--button title ="Eliminar Referencia" class="btn btn-default btn-xs" onclick="fn_removeReferenciaEmi(this);" type="button"-->                                                    <div class="ui-state-highlight-text">                                                        <span class="ui-icon ui-icon-check" title="OK"/>                                                    </div>                                                        <!--/button-->                                                                                            </td>                                                <td style="display:none">1</td>                                                                                        </tr>                                      </c:forEach>                                                                         </tbody>                              </table>                        </div>                                        </div>                                                    </td>            </tr>        </table></jsp:root>