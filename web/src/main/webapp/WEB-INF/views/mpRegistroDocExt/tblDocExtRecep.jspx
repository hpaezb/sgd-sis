<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function(){fu_iniTblDocExtRecep();});</script>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>         <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtpnuEmi" value=""/>        <table id = "myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"></th>                    <th style="display:none"></th>                    <th width="8%"></th>                    <th width="8%">NRO <br>EXPEDIENTE</br></th>                    <th width="5%">FECHA <br>EXP.</br></th>                    <th width="5%">FECHA <br>EMI.</br></th>                    <th width="8%">ESTADO</th>                    <th width="8%">TIPO <br>EXPEDIENTE</br></th>                    <th width="8%">TIPO <br>EMISOR</br></th>                    <th width="8%">REMITENTE</th>                    <th width="13%">ASUNTO</th>                    <th width="8%">TIPO DOC.</th>                    <th width="8%">NÂ° DOC</th>                    <th width="8%">DESTINATARIO</th>                    <th width="8%">ORIGEN REC.</th>                                    </tr>            </thead>            <tbody>                <c:forEach var="docExtRecep" items="${docExtRecepLst}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${docExtRecep.nuAnn}</td>                        <td style="display:none">${docExtRecep.nuEmi}</td>                        <td width="8%">                            <button title ="Editar Documento" class="btn btn-default btn-xs" onclick="fn_goEditarDocExtRecepClick('${docExtRecep.nuAnn}', '${docExtRecep.nuEmi}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                                                                         <c:choose>                                <c:when test='${docExtRecep.existeDoc eq "1"}'>                                    <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${docExtRecep.nuAnn}', '${docExtRecep.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                                                                             </c:when>                            </c:choose>									                        </td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.nuExpediente}</td>                        <td width="5%" class="toolTipVerTodo">${docExtRecep.feExp}</td>                        <td width="5%" class="toolTipVerTodo">${docExtRecep.feEmiCorta}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deEsDocEmiMp}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.coTipoExp}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deTiEmi}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deOriEmiMp}</td>                        <td width="13%" class="toolTipVerTodo">${docExtRecep.deAsu}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deTipDocAdm}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.nuDoc}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deEmpDes}</td>                        <td width="8%" class="toolTipVerTodo">${docExtRecep.deDependencia}</td>                    </tr>                </c:forEach>               </tbody>            </table>            </div>            <c:if test="${not empty docExtRecepLst}">                <div style="display: none">Registros: ${docExtRecepLst.size()}</div>            </c:if>      <div id="divflotante" class="divTip">&amp;nbsp;</div>    	</jsp:root>	