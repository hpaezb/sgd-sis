<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventDestCiudadanoEmiDocAdm();});</script>                                    <table cellpadding="0" cellspacing="0" width="100%">            <tr valign="top">                <td>                 <div class="toolbar-left-vertical">                    <button title ="Agregar Destinatario" class="btn btn-default btn-xs" onclick="fn_addDestintarioEmiCiudadano();" type="button">                      <span class="button.ui-button-icon-only ui-icon ui-icon-circle-plus"/>                    </button><br/><br/>                    <button title ="Eliminar Destinatario" class="btn btn-default btn-xs" onclick="fn_removeDestintarioEmiCiudadano();" type="button">                      <span class="button.ui-button-icon-only ui-icon ui-icon-circle-minus"/>                    </button>                 </div>                </td>                <td width="100%">                    <input id="txtTblDestEmiFilaWhereButton" name="txtTblDestEmiFilaWhereButton" type="hidden"/>                    <input id="txtTblDestEmiColWhereButton" name="txtTblDestEmiColWhereButton" type="hidden"/>                    <input id="txtNroDocumentoCiudadano" name="txtNroDocumentoCiudadano" type="hidden"/>                    <div class="ui-datatable ui-datatable-scrollable">                        <div class="ui-datatable-scrollable-header">                               <table class="ui-state-default" style="width:100%;">                                     <thead>                                    