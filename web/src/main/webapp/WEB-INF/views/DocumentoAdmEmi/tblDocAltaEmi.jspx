<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {            fu_eventoTablaEmiDocAdm();        });</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtnuEmi" value=""/>        <input type="hidden" id="txtpnuDes" name="txtpnuDes" value=""/>        <input type="hidden" id="txtpexisteDoc" name="txtpexisteDoc" value=""/>        <input type="hidden" id="txtpexisteAnexo" name="txtpexisteAnexo" value=""/>        <table id ="myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"></th>                    <th width="7%"></th>                    <th width="3%">N°</th>                    <th width="10%">REFERENCIA ORIGEN</th>                    <th width="4%">FECHA</th>                    <th width="7%">TIPO DOC.</th>                    <th width="8%">N° DOCUMENTO</th>                    <th width="26%">ASUNTO</th>                    <th width="14%">DESTINATARIO</th>                    <th width="8%">ELABORADO POR </th>                    <th width="7%">NRO EXPEDIENTE</th>                    <th width="6%">ESTADO</th>                    <th style="display:none"></th>                    <th style="display:none"></th>                    <th style="display:none"></th>                                                 <th style="display:none"></th>                                                 <th style="display:none"></th>                                                 <th style="display:none"></th>                                                                 </tr>            </thead>				            <tbody>                <c:forEach var="emiDocumAdm" items="${emiDocumAdmList}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${status.count}</td>                        <td>                            <c:choose>                                <c:when test='${emiDocumAdm.esDocEmi eq "7"}'>                                    <c:choose>                                        <c:when test='${emiDocumAdm.coPrioridad eq "3"}'>                                            <button title ="Editar Documento muy Urgente" class="btn btn-danger btn-xs" onclick="editarDocumentoAltaClick('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}','${emiDocumAdm.existeDoc}','${emiDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                     </c:when>                                        <c:when test='${emiDocumAdm.coPrioridad eq "2"}'>                                            <button title ="Editar Documento Urgente" class="btn btn-warning btn-xs" onclick="editarDocumentoAltaClick('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}','${emiDocumAdm.existeDoc}','${emiDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                                                                 </c:when>                                        <c:otherwise>                                            <button title ="Editar Documento" class="btn btn-success btn-xs" onclick="editarDocumentoAltaClick('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}','${emiDocumAdm.existeDoc}','${emiDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                     </c:otherwise>                                    </c:choose>                                </c:when>                                <c:otherwise>                                    <button title ="Editar Documento" class="btn btn-default btn-xs" onclick="editarDocumentoAltaClick('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}','${emiDocumAdm.existeDoc}','${emiDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                                                 </c:otherwise>                            </c:choose>                                                        <c:choose>                                <c:when test='${emiDocumAdm.existeDoc eq "1"}'>                                    <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                                                                             </c:when>                            </c:choose>									                            <c:choose>                                <c:when test='${emiDocumAdm.existeAnexo eq "1"}'>                                    <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexo('${emiDocumAdm.nuAnn}', '${emiDocumAdm.nuEmi}', 'N');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                </c:when>                            </c:choose>                                                            </td>                        <!--<c:choose>                            <c:when test='${emiDocumAdm.coPrioridad eq "3"}'>                                <td align="center" style="background-color: #D9534F; color:white; font-size: 1em;">                                    ${emiDocumAdm.nuCorEmi}                                </td>                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${emiDocumAdm.coPrioridad eq "2"}'>                                <td align="center" style="background-color: #F0AD4E;color:white;font-size: 1em;">                                    ${emiDocumAdm.nuCorEmi}                                </td>                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${emiDocumAdm.coPrioridad eq "1"}'>-->                                <td align="center">${emiDocumAdm.nuCorEmi}</td>                            <!--</c:when>                            </c:choose>-->                        <td>${emiDocumAdm.deEmiRef}</td>                        <td>${emiDocumAdm.feEmiCorta}</td>                        <td>${emiDocumAdm.deTipDocAdm}</td>                        <td>${emiDocumAdm.nuDoc}</td>                        <td>${emiDocumAdm.deAsuM}</td>                        <td>${emiDocumAdm.deEmpPro}</td>                        <td>${emiDocumAdm.deEmpRes}</td>                        <td>${emiDocumAdm.nuExpediente}</td>                        <td>${emiDocumAdm.deEsDocEmi}</td>                        <td style="display:none">${emiDocumAdm.nuAnn}</td>                        <td style="display:none">${emiDocumAdm.nuEmi}</td>                        <td style="display:none">${emiDocumAdm.tiCap}</td>                                                            <td style="display:none">${emiDocumAdm.nuEmi}</td>                                                            <td style="display:none">${emiDocumAdm.existeDoc}</td>                                                            <td style="display:none">${emiDocumAdm.existeAnexo}</td>                                                        </tr>                </c:forEach>            </tbody>			        </table>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	</jsp:root>	