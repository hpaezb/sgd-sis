ia" class="btn btn-default btn-xs" onclick="fn_searchDocReferentbl(this);" type="button">                                            <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                        </button>                                                                                <button title ="Ver Documento Referenciado" class="btn btn-default btn-xs" onclick="fn_verDocumentoRefEmi(this);" type="button">                                            <span class="button.ui-button-icon-only ui-icon icon_pdf"/>                                        </button>                                        <!--SEGDI MVALDERA-->                                        <button title ="Ver Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verDocumentosAnexosRef(this);" type="button">                                            <span class="button.ui-button-icon-only ui-icon icon_clip"/>                                        </button>                                        <!--SEGDI MVALDERA-->                                    </td>                                    <td style="display:none">0NP3</td>                                    <td style="display:none"/>                                    <td style="display:none"/>                                    <td style="display:none"/>                                    <td style="display:none">INS</td>                                    <td style="display:none"/>                                    <td width="2%">                                        <!--button title ="Eliminar Referencia" class="btn btn-default btn-xs" onclick="fn_removeReferenciaEmi(this);" type="button"-->                                        <div class="ui-state-error-text">                                            <span class="ui-icon ui-icon-alert" title="ERROR"/>                                        </div>                                            <!--/button-->                                      </td>                                    <td style="display:none">0</td>                                </tr>                                 <c:forEach var="lReferencia" items="${lstReferenciaDocAdmEmi}" varStatus="status">                                    <tr>                                        <td width="12%">                                            <select style="width:100%;" onchange="fu_cambioTxtTblRefEmiDoc(this);">                                                <c:forEach var="lAnnRefEmi" items="${deAnnioList}">                                                    <c:choose>                                                        <c:when test="${lAnnRefEmi.cano eq lReferencia.nuAnnRef}">                                                            <option selected="selected" value="${lAnnRefEmi.cano}">${lAnnRefEmi.deAnio}</option>                                                         </c:when>                                                        <!-- <c:when test="${deAnnioList.size() eq status.count}"/>-->                                                        <c:otherwise>                                                            <option value="${lAnnRefEmi.cano}">${lAnnRefEmi.deAnio}</option>                                                         </c:otherwise>                                                    </c:choose>                                                                                                    </c:forEach>                                            </select>                                                                                    </td>                                        <td width="25%">                                            <select style="width:100%;" onchange="fu_cambioTxtTblRefEmiDoc(this);">                                                <c:forEach var="lTipDocRefEmi" items="${lstTipDocReferenciaEmi}">                                                    <c:choose>                                                        <c:when test="${lTipDocRefEmi.coTipDoc eq lReferencia.coTipDocAdm}">                                                            <option selected="selected" value="${lTipDocRefEmi.coTipDoc}">${lTipDocRefEmi.deTipDoc}</option>                                                         </c:when>                                                        <c:otherwise>                                                            <option value="${lTipDocRefEmi.coTipDoc}">${lTipDocRefEmi.deTipDoc}</option>                                                         </c:otherwise>                                                    </c:choose>                                                                                                    </c:forEach>                                            </select>                                                                                                                </td>                                        <td width="8%" align="center">                                            <c:choose>                                                    <c:when test="${lReferencia.tipDocRef eq 'emi'}">                                                            <input type="radio" name="group${status.count}" value="emi" onclick="fu_cambioTxtTblRefEmiDoc(this);" checked="true"/>&amp;nbsp;<input type="radio" name="group${status.count}" value="rec" onclick="fu_cambioTxtTblRefEmiDoc(this);"/>                                                    </c:when>                                                    <c:otherwise>                                                            <input type="radio" name="group${status.count}" value="emi" onclick="fu_cambioTxtTblRefEmiDoc(this);"/>&amp;nbsp;<input type="radio" name="group${status.count}" value="rec" checked="true" onclick="fu_cambioTxtTblRefEmiDoc(this);"/>                                                    </c:otherwise>                                            </c:choose>                                                                                                                                                                               </td>                                        <td width="27%"><input type="text" style="width:95%;" onkeyup="fu_cambioTxtTblRefEmiDoc(this);" value="${lReferencia.liNuDoc}"/></td>                                        <td width="12%"><input type="text" style="width:88%;" onkeyup="fu_cambioTxtTblRefEmiDoc(this);" value="${lReferencia.feEmiCorta}"/></td>                                        <td width="16%">                                            <button title ="Buscar Referencia" class="btn btn-default btn-xs" onclick="fn_searchDocReferentbl(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                            </button>                                                                                    <button title ="Ver Documento Referenciado" class="btn btn-default btn-xs" onclick="fn_verDocumentoRefEmi(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon icon_pdf"/>                                            </button>                                            <!--SEGDI MVALDERA-->                                            <button title ="Ver Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verDocumentosAnexosRef(this);" type="button">                                                <span class="glyphicon glyphicon-paperclip"/>                                            </button>                                            <!--SEGDI MVALDERA-->                                        </td>                                        <td style="display:none">${lReferencia.nuAnnRef}$#${lReferencia.coTipDocAdm}$#${lReferencia.tipDocRef}$#${lReferencia.liNuDoc}$#${lReferencia.feEmiCorta}</td>                                        <td style="display:none">${lReferencia.nuAnnRef}</td>                                        <td style="display:none">${lReferencia.nuEmiRef}</td>                                        <td style="display:none">${lReferencia.nuDesRef}</td>                                        <td style="display:none">${lReferencia.accionBd}</td>                                        <td style="display:none">${lReferencia.coRef}</td>                                        <td width="2%">                                            <!--button title ="Eliminar Referencia" class="btn btn-default btn-xs" onclick="fn_removeReferenciaEmi(this);" type="button"-->                                            <div class="ui-state-highlight-text">                                                <span class="ui-icon ui-icon-check" title="OK"/>                                            </div>                                                <!--/button-->                                        </td>                                        <td style="display:none">1</td>                                    </tr>                                                                    </c:forEach>                               </tbody>                          </table>                    </div>                </div>                            </td>        </tr>    </table>    </jsp:root>