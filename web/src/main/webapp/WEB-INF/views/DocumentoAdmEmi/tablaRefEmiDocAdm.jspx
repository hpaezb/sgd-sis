<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventRefEmiDocAdm();});</script>                                <table cellpadding="0" cellspacing="0" width="100%">        <tr valign="top">            <td>             <div class="toolbar-left-vertical">                <button title ="Agregar Referencia" class="btn btn-default btn-xs" onclick="fn_addReferenciaEmi();" type="button">                  <span class="button.ui-button-icon-only ui-icon ui-icon-circle-plus"/>                </button><br/><br/>                <button title ="Eliminar Referencia" class="btn btn-default btn-xs" onclick="fn_removeReferenciaEmi();" type="button">                  <span class="button.ui-button-icon-only ui-icon ui-icon-circle-minus"/>                </button>             </div>            </td>                        <td width="100%">                <input id="txtTblRefEmiFilaWhereButton" name="txtTblRefEmiFilaWhereButton" type="hidden"/>                <input id="txtTblRefEmiColWhereButton" name="txtTblRefEmiColWhereButton" type="hidden"/>                                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                           <table class="ui-state-default" style="width:100%;">                                 <thead>                                        <tr>                                            <th width="12%">AÃ±o</th>                                            <th width="24%">Tipo Documento</th>                                            <th width="10%">Emi/Rec</th>                                            <th width="25%">Nro. Documento</th>                                                                                <th width="12%">Fecha</th>                                            <th width="17%"/>                                        </tr>                                 </thead>                           </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:55px;'>                          <table class="ui-datatable-edit-data" id="tblRefEmiDocAdm"  cellpadding="0" cellspacing="0" style="width:100%;">                              <tbody>                                <tr class="oculto">                                    <td width="12%">                                        <select style="width:100%;" onchange="fu_cambioTxtTblRefEmiDoc(this);">                                            <c:forEach var="lAnnRefEmi" items="${deAnnioList}" varStatus="status">                                                <c:choose>                                                    <c:when test="${status.count == 1}">                                                        <option selected="selected" value="${lAnnRefEmi.cano}">${lAnnRefEmi.deAnio}</option>                                                     </c:when>                                                    <!-- <c:when test="${deAnnioList.size() eq status.count}"/>-->                                                    <c:otherwise>                                                        <option value="${lAnnRefEmi.cano}">${lAnnRefEmi.deAnio}</option>                                                     </c:otherwise>                                                </c:choose>                                                                                                </c:forEach>                                        </select>                    