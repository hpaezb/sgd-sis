<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventPersVoBoDocAdm();});</script>                                <table cellpadding="0" cellspacing="0" width="100%">        <tr valign="top">            <td>             <div class="toolbar-left-vertical">                <button title ="Agregar Empleado" class="btn btn-default btn-xs" onclick="fn_addEmpVoBoDocAdm();" type="button">                  <span class="button.ui-button-icon-only ui-icon ui-icon-circle-plus"/>                </button><br/><br/>                <button title ="Eliminar Empleado" class="btn btn-default btn-xs" onclick="fn_removeEmpVoBoDocAdm();" type="button">                  <span class="button.ui-button-icon-only ui-icon ui-icon-circle-minus"/>                </button>             </div>            </td>                        <td width="100%">                <input id="txtTblPersVoBoFilaWhereButton" name="txtTblPersVoBoFilaWhereButton" type="hidden"/>                <input id="txtTblPersVoBoColWhereButton" name="txtTblPersVoBoColWhereButton" type="hidden"/>                                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                           <table class="ui-state-default" style="width:100%;">                                 <thead>                                        <tr>                                            <th width="25%">Dependencia</th>                                            <th width="75%">Personal</th>                                        </tr>                                 </thead>                           </table>                    </div>                    <div id="containerTblVoBo" class="bx_sb ui-datatable-scrollable-body" style='height:125px;'>                          <table class="ui-datatable-edit-data" id="tblPersVoBoDocAdm"  cellpadding="0" cellspacing="0" style="width:100%;">                              <tbody>                                <tr class="oculto">                                    <td width="25%">                                        <input type="text" style="width:70%;" onkeyup="return filtrarTeclado(event, false, public_apenom,'','',fn_buscarDepVoBoDocAdm);"/>                                        <button title ="Buscar Dependencia" class="btn btn-default btn-xs" onclick="fn_buscarDepVoBoDocAdmButton(this);" type="button">                                            <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                        </button>                                    </td>                                                                        <td style="display:none"/><!--Cod. Dependencia-->                                                                                                                      <td width="71%">                                        <input type="text" style="width:90%;" onkeyup="return filtrarTeclado(event, false, public_apenom,'','',fn_buscarEmpleadoVoBoDocAdm);"/>                                        <button title ="Buscar Personal" class="btn btn-default btn-xs" onclick="fn_buscarEmpleadoVoBoDocAdmButton(this);" type="button">                                            <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                        </button>                                    </td>                                    <td style="display:none"/><!--cod. empleado-->                                                                                                                      <td width="2%" class="label-danger"><span style="color:white;font-weight: bold;font-size: 12px;" title="Sin Visto Bueno.">VB</span></td>                                                                          <td style="display:none">INS</td><!--accionBD-->                                    <td style="display:none">0NP3</td><!--Desc. Dependencia-->                                                                            <td style="display:none">0NP3</td><!--Desc. empleado-->                                                                            <td style="display:none">0</td><!--indicador Sin Visto Bueno-->                                    <td width="2%">                                        <div class="ui-state-error-text">                                            <span class="ui-icon ui-icon-alert" title="ERROR"/>                                        </div>                                        </td>                                    <td style="display:none">0</td><!--NO OK-->                                       </tr>                                 <c:forEach var="objEmpleado" items="${lstEmpVoBoDocAdm}" varStatus="status">                                      <tr>                                          <c:choose>                                              <c:when test="${objEmpleado.inVobo eq '1'}">                                                <td width="25%">                                                    <input type="text" style="width:70%;" value="${fn:escapeXml(objEmpleado.deDependencia)}" readonly="readonly"/>                                                    <button title ="Buscar Dependencia" class="btn btn-default btn-xs" type="button" disabled="disabled">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${objEmpleado.coDependencia}</td><!--Cod. Dependencia-->                                                                                                                                          <td width="71%">                                                    <input type="text" style="width:90%;" value="${fn:escapeXml(objEmpleado.nombre)}" readonly="readonly"/>                                                    <button title ="Buscar Personal" class="btn btn-default btn-xs" type="button" disabled="disabled">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${objEmpleado.coEmpleado}</td><!--cod. empleado-->                                                                                                                                  <td width="2%" class="label-success"><span style="color:white;font-weight: bold;font-size: 12px;" title="Con Visto Bueno.">VB</span></td>                                                                                                                                      </c:when>                                              <c:otherwise>                                                <td width="25%">                                                    <input type="text" style="width:70%;" onkeyup="return filtrarTeclado(event, false, public_apenom,'','',fn_buscarDepVoBoDocAdm);" value="${fn:escapeXml(objEmpleado.deDependencia)}"/>                                                    <button title ="Buscar Dependencia" class="btn btn-default btn-xs" onclick="fn_buscarDepVoBoDocAdmButton(this);" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                </td>                                                <td style="display:none">${objEmpleado.coDependencia}</td><!--Cod. Dependencia-->                                                                                                                                          <td width="71%">                                                    <input type="text" style="width:90%;" onkeyup="return filtrarTeclado(event, false, public_apenom,'','',fn_buscarEmpleadoVoBoDocAdm);" value="${fn:escapeXml(objEmpleado.nombre)}"/>                                                    <button title ="Buscar Personal" class="btn btn-default btn-xs" onclick="fn_buscarEmpleadoVoBoDocAdmButton(this);" type="button">                                                        <span class="button.ui-button-icon-only ui-icon ui-icon-search"/>                                                    </button>                                                    <c:if test="${objEmpleado.inVobo eq '2'}">                                                        <span id="spanObsDocVoboAlert" style="position : relative;margin-left: -70px;vertical-align: text-top;font-size: 90%;border-radius: 0em;" onclick="fn_showObsEmpVoBo('${objEmpleado.coDependencia}','${objEmpleado.coEmpleado}');" class="label label-warning">OBS</span>                                                    </c:if>                                                </td>                                                <td style="display:none">${objEmpleado.coEmpleado}</td><!--cod. empleado-->                                                                                                                                  <td width="2%" class="label-danger"><span style="color:white;font-weight: bold;font-size: 12px;" title="Sin Visto Bueno.">VB</span></td>                                                                                    </c:otherwise>                                          </c:choose>                                        <td style="display:none">BD</td><!--accionBD-->                                        <td style="display:none">${objEmpleado.deDependencia}</td><!--Desc. Dependencia-->                                                <td style="display:none">${objEmpleado.nombre}</td><!--Desc. empleado-->                                                <td style="display:none">${objEmpleado.inVobo}</td><!--indicador Visto Bueno-->                                                <td width="2%">                                            <div class="ui-state-highlight-text">                                                <span class="ui-icon ui-icon-check" title="OK"/>                                            </div>                                          </td>                                        <td style="display:none">1</td><!--OK-->                                          </tr>                                                                    </c:forEach>                               </tbody>                          </table>                    </div>                </div>                            </td>        </tr>    </table>    </jsp:root>