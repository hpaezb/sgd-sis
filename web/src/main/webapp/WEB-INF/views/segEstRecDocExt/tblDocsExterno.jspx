<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fu_iniTblSeguiDocsExternos('${objJson}');});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn"/>        <input type="hidden" id="txtpnuEmi" name="txtpnuEmi"/>        <input type="hidden" id="txtpnuDes" name="txtpnuDes"/>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                <table class="ui-state-default" style="width:100%;">                    <thead>                        <tr class="ui-state-default">                            <th style="display:none"/>                            <th style="display:none"/>                            <th style="display:none"/>                            <th width="4%"/>                            <th width="8%">VENCIMIENTO</th>                            <th width="4%">NÂ°</th>                            <th width="7%">REMITENTE</th>                            <th width="9%">NUMERO<br/>EXPEDIENTE</th>                            <th width="6%">TIPO DOC.</th>                            <th width="7%">NUMERO<br/>DOCUMENTO</th>                            <th width="4%">FECHA<br/>EMI.</th>                            <th width="7%">DESTINO</th>                            <th width="10%">ASUNTO</th>                            <th width="3%">DIAS<br/>PROY.</th>                            <th width="4%">FECHA<br/>LIM.</th>                            <th width="3%">DIAS<br/>EXC.</th>                                                <th width="4%">FECHA<br/>REC.</th>                            <th width="4%">FECHA<br/>ATE.</th>                            <th width="4%">FECHA<br/>ARC.</th>                            <th width="6%">ESTADO</th>                            <th width="6%">MOTIVO</th>                        </tr>                    </thead>                                    </table>            </div>            <div class="ui-datatable-scrollable-body" style='height:470px;'>                <table class="ui-datatable" id="tblDocExtRecepSegui" style="width:100%;">                    <tbody>                        <c:forEach var="docExtRec" items="${emiDocumExtList}" varStatus="status">                            <c:choose>                                <c:when test="${status.count % 2 == 0}">                                  <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                </c:when>                                <c:otherwise>                                  <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                </c:otherwise>                                                                </c:choose>                            <c:choose>                                <c:when test='${docExtRec.coEstVen eq "1"}'>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenProximoVencer"/>                                </c:when>                                <c:when test='${docExtRec.coEstVen eq "2"}'>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenVencido"/>                                </c:when>                                <c:when test='${docExtRec.coEstVen eq "3"}'>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenVenceHoy"/>                                </c:when>                                <c:when test='${docExtRec.coEstVen eq "4"}'>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenPorVencer"/>                                </c:when>                                <c:when test='${docExtRec.coEstVen eq "5"}'>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenAtendido"/>                                </c:when>                                                                    <c:otherwise>                                    <c:set var="rowStyleVen" scope="page" value="estadoVenNormal"/>                                </c:otherwise>                            </c:choose>                                                        <tr class="${rowStyle}" tabindex="0">                                <td style="display:none">${docExtRec.nuAnn}</td>                                <td style="display:none">${docExtRec.nuEmi}</td>                                <td style="display:none">${docExtRec.nuDes}</td>                                <td class="${rowStyleVen}" width="4%">                                    <c:choose>                                        <c:when test='${docExtRec.existeDoc eq "1"}'>                                            <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${docExtRec.nuAnn}', '${docExtRec.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                                                                                     </c:when>                                    </c:choose>									                                    <c:choose>                                        <c:when test='${docExtRec.existeAnexo eq "1"}'>                                            <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexoDocExtConsul('${docExtRec.nuAnn}', '${docExtRec.nuEmi}','${docExtRec.nuDes}');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                        </c:when>                                    </c:choose>                                                                    </td>                                <td class="${rowStyleVen}" width="8%">${docExtRec.deEstVen}</td>                                <td align="center" width="4%">${docExtRec.nuCorEmi}</td>                                <td width="7%">${docExtRec.deOriEmi}</td>                                <td width="9%">${docExtRec.nuExpediente}</td>                                <td width="6%">${docExtRec.deTipDocAdm}</td>                                 <td width="7%">${docExtRec.nuDoc}</td>                                <td width="4%">${docExtRec.feEmiCorta}</td>                                <td width="7%">${docExtRec.deEmpDes}</td>                                <td width="10%">${docExtRec.deAsu}</td>                                <td width="3%">${docExtRec.nuDiaAte}</td>                                <td width="4%">${docExtRec.feLimiteCorta}</td>                                <td width="3%">${docExtRec.nuDiaExc}</td>                                                        <td width="4%">${docExtRec.feRecDocCorta}</td>                                                        <td width="4%">${docExtRec.feAteDocCorta}</td>                                                        <td width="4%">${docExtRec.feArcDocCorta}</td>                                                        <td width="6%">${docExtRec.deEsDocEmi}</td>                                                        <td width="6%">${docExtRec.deMotivo}</td>                                                  </tr>                        </c:forEach>                        <c:if test="${empty emiDocumExtList}">                            <td>No se encuentran registros.</td>                        </c:if>                                                </tbody>                </table>            </div>            <c:if test="${not empty emiDocumExtList}">                <div>Registros: ${emiDocumExtList.size()}</div>            </c:if>                    </div>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	  </jsp:root>	