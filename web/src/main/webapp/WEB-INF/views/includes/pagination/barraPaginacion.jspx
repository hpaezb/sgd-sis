<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <div id="divBarraPaginacion">        <input type="hidden" name="hdnNumeroTotalDePaginas" id="hdnNumeroTotalDePaginas" value="${paginacion.numeroTotalDePaginas}"/>        <table width="100%">            <tr>                <td width="4%"><div id="primeraPag" title="Primero" onClick="mostrarPaginacionDocDependencia('P_PRIMERO')" /></td>                <td width="4%"><div id="atrasPag" title="Atras" onClick="mostrarPaginacionDocDependencia('P_ATRAS')" ></div></td>                <td width="4%"><div id="etiquetaPagina">Pagina</div></td>                <td width="4%"><div id="numeroPaginaActual"><input type="text" name="txtNumeroPaginaActual" id="txtNumeroPaginaActual" size="2" style="font-size: x-small;" value="${paginacion.numeroDePagina}" maxlength="2" onkeypress="return fu_onKeyPressFiltrarTecladoNumero(event, false, public_enteros)"/> </div></td>                <td width="3%"><div id="etiquetaDe"> De </div></td>                <td width="4%"><div id="totalPaginas">${paginacion.numeroTotalDePaginas}</div></td>                <td width="4%"><div id="siguientePag" title="Siguiente" onClick="mostrarPaginacionDocDependencia('P_SIGUIENTE')" ></div></td>                <td width="4%"><div id="ultimaPag" title="Ultimo" onClick="mostrarPaginacionDocDependencia('P_ULTIMO')"></div></td>                <td width="4%"><div id="actualizaPag" title="Actualizar" onClick="mostrarPaginacionDocDependencia('P_ACTUALIZAR')" ></div></td>                <td width="60%"><div id="registrosMostrados" align="right" style="padding-right:5px"> ${paginacion.strRegistrosMostrados} </div></td>            </tr>        </table>    </div>    <script type="text/javascript">pushFocusEvent(true);</script></jsp:root>