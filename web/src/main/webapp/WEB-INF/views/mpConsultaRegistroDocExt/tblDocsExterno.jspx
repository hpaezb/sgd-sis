<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fu_iniTblDocsExternos();});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtpnuEmi" value=""/>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                <table class="ui-state-default" style="width:100%;">                    <thead>                        <tr class="ui-state-default">                            <th style="display:none"/>                            <th style="display:none"/>                            <th width="3%"></th>                            <th width="3%">N°</th>                            <th width="15%">REMITENTE</th>                            <th width="6%">TIPO DOC.</th>                            <th width="6%">N° DOC.</th>                            <th width="3%">FECHA</th>                            <th width="15%">ASUNTO</th>                            <th width="13%">DESTINATARIO</th>                            <th width="1%">DIAS</th>                            <!--    <th width="3%">F. VENCE</th> rbn se comenta para que no figura dato ingresado en campo Fecha de venc-->                            <th width="7%">ESTADO</th>                            <th width="7%">ORIGEN REC.</th>                            <th width="10%">RECEPCIONADO POR</th>                            <th width="8%">N° EXPEDIENTE</th>                        </tr>                    </thead>                                    </table>            </div>            <div class="ui-datatable-scrollable-body" style='height:470px;'>                <table class="ui-datatable" id="tblDocExtRecepConsul" style="width:100%;">                    <tbody>                        <c:forEach var="docExtRec" items="${docExtRecepLst}" varStatus="status">                            <c:choose>                                <c:when test="${status.count % 2 == 0}">                                  <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                </c:when>                                <c:otherwise>                                  <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                </c:otherwise>                                                                </c:choose>                            <tr class="${rowStyle}" tabindex="0">                                <td style="display:none">${docExtRec.nuAnn}</td>                                <td style="display:none">${docExtRec.nuEmi}</td>                                <td width="3%">                                    <c:choose>                                        <c:when test='${docExtRec.existeDoc eq "1"}'>                                            <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${docExtRec.nuAnn}', '${docExtRec.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                                                                                     </c:when>                                    </c:choose>									                                    <c:choose>                                        <c:when test='${docExtRec.existeAnexo eq "1"}'>                                            <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexoDocExtConsul('${docExtRec.nuAnn}', '${docExtRec.nuEmi}','N');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                        </c:when>                                    </c:choose>                                                                    </td>                                <td align="center" width="3%">${docExtRec.nuCorEmi}</td>                                <td width="15%">${docExtRec.deOriEmi}</td>                                <td width="6%">${docExtRec.deTipDocAdm}</td>                                <td width="6%">${docExtRec.nuDoc}</td>                                <td width="3%">${docExtRec.feEmiCorta}</td>                                <td width="15%">${docExtRec.deAsu}</td>                                <td width="13%">${docExtRec.deEmpDes}</td>                                <td width="1%">${docExtRec.nuDiaAte}</td><!--    <td width="3%">${docExtRec.feExpVenceCorta}</td> rbn se comenta para que no figura dato ingresado en campo Fecha de venc-->                                <td width="7%">${docExtRec.deEsDocEmi}</td>                                <!--td width="7%">${docExtRec.deLocEmi}</td-->                                <td width="7%">${docExtRec.deDependencia}</td>                                <td width="10%">${docExtRec.deEmpRec}</td>                                <td width="8%">${docExtRec.nuExpediente}</td>                                                              </tr>                        </c:forEach>                        <c:if test="${empty docExtRecepLst}">                            <td>No se encuentran registros.</td>                        </c:if>                                                </tbody>                </table>            </div>            <c:if test="${not empty docExtRecepLst}">                <div>Registros: ${docExtRecepLst.size()}</div>            </c:if>                    </div>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	  </jsp:root>	