<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>    <div id="divBotonesAccion" class="btn-group btn-group-lg">        <c:choose>                <c:when test="${toolBarConfig.coEstado eq '0'}">                <button title ="Recibir Documento" class="btn btn-primary btn-lg" onclick="fu_grabarRecepDocumentoAdm();" type="button">                    <span class="glyphicon glyphicon-import"><jsp:text/></span> Recibir</button>                </c:when>                <c:otherwise>                <button title ="Grabar Cambios del Documento" class="btn btn-primary " onclick="fu_grabarRecepDocumentoAdm();" type="button">                    <span class="glyphicon glyphicon-floppy-disk"><jsp:text/></span> Grabar</button>                     </c:otherwise>        </c:choose>                              <c:if test="${toolBarConfig.coEstado ne '0'}">        <div class="btn-group btn-group-lg">                <button title ="Atender o Derivar Documento" class="btn btn-primary" onclick="fu_atenderDerivarDocumento();" type="button">                 <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Atender/Derivar</button>                <button type="button" class="btn btn-primary btn-lg dropdown-toggle" data-toggle="dropdown">                        <span class="caret"></span>&amp;nbsp;                        <span class="sr-only">Toggle Dropdown</span>                </button>                <ul id="ullsAtenderDocumento" class="dropdown-menu" role="menu">                        <li><a href="#" onclick="fu_atenderDerivarDocumento();" title ="Con Documento Administrativo">Documento Administrativo</a></li>                        <li><a href="#" onclick="fu_atenderDerivarDocumentoPersonal();" title ="Con Documento del Profesional">Documento del Profesional</a></li>                        <!--<li><a href="#" onclick="fu_atenderDerivarDocumentoInter();" title ="Con Documento Personal">Documento Interinstitucional</a></li>-->                        <c:if test="${toolBarConfig.inOpcionMP eq '1' }">                            <li><a href="#" onclick="fu_atenderDerivarMesaParteDocExterno();" title ="Con Mesa Partes">Mesa Partes</a></li>                        </c:if>                </ul>        </div>               </c:if>        <button title ="Ver Documento" class="btn btn-primary btn-lg" onclick="fn_verDocumentoRecEdit();" type="button">            <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Ver Documento</button>       <button title ="Ver Documento Anexos - Referencias" class="btn btn-primary btn-lg" onclick="fn_verAnexoRecEdit();" type="button">           <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Ver Anexos</button>       <button title ="Seguimiento de Documentos" class="btn btn-primary btn-lg" onclick="fn_verSeguimientoRecEdit();" type="button">           <span class="glyphicon glyphicon-transfer"><jsp:text/></span> Seguimiento</button>               <c:if test="${toolBarConfig.coEstado ne '0'}">       <button title ="Anular Recibido" class="btn btn-primary btn-lg" onclick="fu_anularDocumentoRecepcion();" type="button">           <span class="glyphicon glyphicon-remove"><jsp:text/></span> Anular</button>    </c:if>                  <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary btn-lg" onclick="regresarRecepDocumAdm();" type="button">           <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>       <button title ="Salir Recepci&#243;n de Documentos Administrativos" class="btn btn-primary btn-lg" onclick="cerrarPantalla();" type="button">           <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>    </div>        </jsp:root>      