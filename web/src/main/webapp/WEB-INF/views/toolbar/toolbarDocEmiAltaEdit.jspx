<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>                <div class="btn-group btn-group-lg">            <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Grabar Cambios del Documento" class="btn btn-primary " onclick="fn_grabarDocumentoEmiAdmAlta();" type="button">                       <span class="glyphicon glyphicon-floppy-disk"><jsp:text/></span> Grabar</button>                    <c:if test="${toolBarConfig.coEstado eq '5' }">                        <button title ="Generar Documento seg&#250;n Plantilla" class="btn btn-primary" onclick="fn_generaDocx();" type="button">                            <span class="glyphicon glyphicon-saved"><jsp:text/></span> Generar Doc</button>                    </c:if>                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_abrirDocumentoEmi();" type="button">                     <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Doc.</button>                    <c:if test="${toolBarConfig.coTipoDoc eq '1' }">                        <button title ="Cargar Documento al Repositorio" class="btn btn-primary" onclick="fn_cargaDocEmiAlta();" type="button">                            <span class="glyphicon glyphicon-cloud-upload"><jsp:text/></span> Cargar Doc.</button>                    </c:if>                </c:when>                <c:otherwise>                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_verDocumentoEmi();" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Documento</button>                </c:otherwise>            </c:choose>                                      <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Cargar Documentos Anexos al Repositorio" class="btn btn-primary" onclick="fn_cargarDocumentosAnexos();" type="button">                        <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Cargar Anexos</button>                </c:when>                <c:otherwise>                    <button title ="Abrir Documentos Anexos" class="btn btn-primary" onclick="fn_verAnexoEmiEdit();" type="button">                        <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Abrir Anexos</button>                </c:otherwise>            </c:choose>                                      <c:if test="${toolBarConfig.coEstado eq '0'}">                   <button title ="Seguimiento de Documentos" class="btn btn-primary btn-lg" onclick="fn_verSeguimientoEmiEdit();" type="button">                       <span class="glyphicon glyphicon-transfer"><jsp:text/></span> Seguimiento</button>            </c:if>                               <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary" onclick="regresarEmitDocumAlta('1');" type="button">               <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>           <button title ="Salir Emisi&#243;n de Documentos Administrativos" class="btn btn-primary" onclick="cerrarPantallaModuloEmisionDoc();" type="button">               <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>        </div></jsp:root>      