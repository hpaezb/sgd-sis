<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>                <div class="btn-group btn-group-lg">            <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Grabar Cambios del Documento" class="btn btn-primary " onclick="fn_grabarDocumentoEmiPersonal();" type="button">                       <span class="glyphicon glyphicon-floppy-disk"><jsp:text/></span> Grabar</button>                    <c:if test="${toolBarConfig.coEstado eq '5' }">                        <button title ="Generar Documento seg&#250;n Plantilla" class="btn btn-primary" onclick="fn_generaDocxPersonal();" type="button">                            <span class="glyphicon glyphicon-saved"><jsp:text/></span> Generar Doc</button>                    </c:if>                    <c:choose>                        <c:when test="${toolBarConfig.coEstado eq '5'}">                            <div class="btn-group btn-group-lg">                                <button title ="Firmar Documento para Emisi&#243;n" class="btn btn-primary" onclick="fn_firmarDocumentoEmiPersonal();" type="button">                                 <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Firmar Doc.</button>                                <button type="button" class="btn btn-primary btn-lg dropdown-toggle" data-toggle="dropdown">                                    <span class="caret"></span>&amp;nbsp;                                    <span class="sr-only">Toggle Dropdown</span>                                </button>                                <ul id="ullsAbrirDocumento" class="dropdown-menu" role="menu">                                    <li><a href="#" onclick="fn_abrirDocumentoEmiPersonal();" title ="Abrir Documento del Repositorio">Visualizar PDF</a></li>                                    <li><a href="#" onclick="fn_abrirDocumentoPersonalFromPC();" title ="Abrir Documento Word desde la PC">Abrir Documento</a></li>                                </ul>                            </div>                        </c:when>                        <c:otherwise>                            <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_abrirDocumentoPersonal();" type="button">                             <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Doc.</button>                        </c:otherwise>                    </c:choose>                                              <c:if test="${toolBarConfig.coTipoDoc eq '1' }">                        <button title ="Cargar Documento al Repositorio" class="btn btn-primary" onclick="fn_cargaDocEmiPersonal();" type="button">                            <span class="glyphicon glyphicon-cloud-upload"><jsp:text/></span> Cargar Doc.</button>                    </c:if>                </c:when>                <c:otherwise>                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_verDocumentoEmiPersonalEdit();" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Documento</button>                </c:otherwise>            </c:choose>                                      <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Ver y Cargar Documentos Anexos al Repositorio" class="btn btn-primary" onclick="fn_cargarDocumentosAnexosPersonal();" type="button">                        <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Ver / Cargar Anexos</button>                </c:when>                <c:otherwise>                    <button title ="Ver Documentos Anexos" class="btn btn-primary" onclick="fn_verAnexoEmiPersonalEdit();" type="button">                        <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Ver Anexos</button>                </c:otherwise>            </c:choose>                                      <c:if test="${toolBarConfig.coEstado eq '0'}">                   <button title ="Seguimiento de Documentos" class="btn btn-primary btn-lg" onclick="fn_verSeguimientoEmiPersonalEdit();" type="button">                       <span class="glyphicon glyphicon-transfer"><jsp:text/></span> Seguimiento</button>            </c:if>             <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Anular Documento" class="btn btn-primary" onclick="fn_anularDocPersonalEmi();" type="button">                        <span class="glyphicon glyphicon-remove"><jsp:text/></span> Anular</button>                </c:when>                <!--<c:when test="${toolBarConfig.coEstado eq '9'}">                    <button title ="Eliminar Documento" class="btn btn-primary" onclick="fn_eliminarDocEmiAdm();" type="button">                        <span class="glyphicon glyphicon-trash"><jsp:text/></span> Eliminar Doc.</button>                </c:when>-->            </c:choose>                                  <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary" onclick="backLsDocEmiPersonal('1');" type="button">               <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>           <button title ="Salir Emisi&#243;n de Documentos Personales" class="btn btn-primary" onclick="cerrarPantallaModuloEmisionDocPersonal();" type="button">               <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>        </div></jsp:root>      