<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>                <div class="btn-group btn-group-lg">            <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <c:choose>                        <c:when test="${toolBarConfig.coEstado eq '7'}">                            <div class="btn-group btn-group-lg">                            <button title ="Firmar Documento para Emisi&#243;n" class="btn btn-primary" onclick="fn_firmarDocumentoEmi();" type="button">                                 <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Firmar Doc.</button>                            </div>                        </c:when>                    </c:choose>                                            <!--  <c:if test="${toolBarConfig.coTipoDoc eq '1' }">-->                        <div id="divEmitirDocFir" style="display: none;" class="btn-group btn-group-lg">                        <button  title ="Cargar Documento al Repositorio" class="btn btn-success" onclick="fu_changeEstadoDocEmiAdm('0');" type="button">                            <span class="glyphicon glyphicon glyphicon-ok"><jsp:text/></span> Emitir Doc.</button>                        </div>                  <!--  </c:if>-->                </c:when>                <c:otherwise>                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_verDocumentoEmi();" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Documento</button>                </c:otherwise>            </c:choose>                                      <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Ver y Cargar Documentos Anexos al Repositorio" class="btn btn-primary" onclick="showColInfoDetalle();" data-toggle="modal" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Ver Detalles</button>                </c:when>            </c:choose>                                       <c:choose>                <c:when test="${(toolBarConfig.coEstado eq '5' || toolBarConfig.coEstado eq '7' )}">                    <button title ="Devolver Documento" class="btn btn-danger" onclick="showColInfoObservacion();" type="button">                        <span class="glyphicon glyphicon-share-alt"><jsp:text/></span> Devolver</button>                </c:when>            </c:choose>                               </div>                <div class="btn-group btn-group-lg pull-right">                               <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary" onclick="regresarEmitDocumAdm('7');" type="button">               <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>        <button title ="Salir Emisi&#243;n de Documentos Administrativos" class="btn btn-primary" onclick="cerrarPantallaModuloEmisionDoc();" type="button">               <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>                </div>                                        </jsp:root>      