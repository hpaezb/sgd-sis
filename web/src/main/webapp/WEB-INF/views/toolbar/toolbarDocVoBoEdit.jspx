<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>                <div class="btn-group btn-group-lg">            <c:choose>                <c:when test="${toolBarConfig.coEstado eq '7' or toolBarConfig.coEstado eq '2' or toolBarConfig.coEstado eq '0'}">                    <button title ="Grabar Cambios del Documento" class="btn btn-primary " onclick="fn_grabarDocVoBoObs();" type="button">                       <span class="glyphicon glyphicon-floppy-disk"><jsp:text/></span> Grabar</button>                    <div class="btn-group btn-group-lg">                        <button title ="Visar Documento" class="btn btn-primary" onclick="fn_firmarDocumentoVoBoBean();" type="button">                         <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Visar Doc.</button>                        <button type="button" class="btn btn-primary btn-lg dropdown-toggle" data-toggle="dropdown">                            <span class="caret"></span>&amp;nbsp;                            <span class="sr-only">Toggle Dropdown</span>                        </button>                        <ul id="ullsAbrirDocumento" class="dropdown-menu" role="menu">                            <li><a href="#" onclick="fn_abrirDocumentoVoboBean();" title ="Abrir Documento del Repositorio">Visualizar PDF</a></li>                        </ul>                    </div>                </c:when>                <c:otherwise>                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_verDocumentoVoBoBean();" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Documento</button>                </c:otherwise>            </c:choose>                                          <button title ="Abrir Documentos Anexos" class="btn btn-primary" onclick="fn_verAnexoDocVoboBean();" type="button">                    <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Abrir Anexos</button>                <button title ="Seguimiento de Documentos" class="btn btn-primary btn-lg" onclick="fn_verSeguimientoDocVoboBean();" type="button">                   <span class="glyphicon glyphicon-transfer"><jsp:text/></span> Seguimiento</button>                <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary" onclick="fn_regresarDocsVoBoLs('1');" type="button">                    <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>                <button title ="Salir de Documento Visto Bueno" class="btn btn-primary" onclick="cerrarPantallaModuloEmisionDoc();" type="button">                    <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>        </div></jsp:root>      