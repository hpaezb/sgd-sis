<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <jsp:scriptlet><![CDATA[response.setHeader("Cache-Control", "max-age=43200");]]></jsp:scriptlet>    <div id="divBotonesAccion" class="btn-group btn-group-lg">        <c:choose>                <c:when test="${toolBarConfig.coEstado eq '5' or toolBarConfig.coEstado eq '7' or toolBarConfig.coEstado eq '8'}">                <button title ="Grabar Cambios del Documento" class="btn btn-primary " onclick="fu_grabarRecepDocExt();" type="button">                    <span class="glyphicon glyphicon-floppy-disk"><jsp:text/></span> Grabar</button>                <button title ="Cargar Documento al Repositorio" class="btn btn-primary" onclick="fn_cargarDocExteRecep();" type="button">                    <span class="glyphicon glyphicon-cloud-upload"><jsp:text/></span> Cargar Doc.</button>                                    <button title ="Abrir Documento del Repositorio" class="btn btn-primary" onclick="fn_abrirDocumentoExtRecep();" type="button">                 <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Abrir Doc.</button>                                    <button title ="Ver y Cargar Documentos Anexos al Repositorio" class="btn btn-primary" onclick="fn_cargarAnexosDocExtRec();" type="button">                    <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Ver / Cargar Anexos</button>                                        </c:when>                <c:otherwise>                    <button title ="Ver Documento" class="btn btn-primary btn-lg" onclick="fn_verDocumentoExtRecep();" type="button">                        <span class="glyphicon glyphicon-list-alt"><jsp:text/></span> Ver Doc.</button>                                    <button title ="Ver Documento Anexos - Referencias" class="btn btn-primary btn-lg" onclick="fn_verAnexoDocExtRec();" type="button">                        <span class="glyphicon glyphicon-paperclip"><jsp:text/></span> Ver Anexos</button>                                    <!--button title ="Cargo a Destinatario" class="btn btn-primary btn-lg" onclick="" type="button">                        <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Cargo Destino</button-->                                                          <button title ="Seguimiento de Documentos" class="btn btn-primary btn-lg" onclick="fn_verSeguimientoDocExtRec();" type="button">                        <span class="glyphicon glyphicon-transfer"><jsp:text/></span> Seguimiento</button>                                    </c:otherwise>                        </c:choose>                                  <c:choose>            <c:when test="${toolBarConfig.coEstado eq '5'}">                <button title ="Anular Documento" class="btn btn-primary btn-lg" onclick="fn_anularDocExtRecep();" type="button">                    <span class="glyphicon glyphicon-remove"><jsp:text/></span> Anular</button>                            </c:when>        </c:choose>       <button title ="Voucher Expediente" class="btn btn-primary btn-lg" onclick="fu_generarVoucherExpediente();" type="button">           <span class="glyphicon glyphicon-pencil"><jsp:text/></span> Voucher Exp.</button>                      <button title ="Regresar al M&#243;dulo Anterior" class="btn btn-primary btn-lg" onclick="afterRegresarLstRecepDocumExt();" type="button">           <span class="glyphicon glyphicon-arrow-left"><jsp:text/></span> Cerrar</button>       <button title ="Salir de Recibidos" class="btn btn-primary btn-lg" onclick="afterCerrarPantallaMP();" type="button">           <span class="glyphicon glyphicon-log-out"><jsp:text/></span> Salir</button>    </div></jsp:root>      