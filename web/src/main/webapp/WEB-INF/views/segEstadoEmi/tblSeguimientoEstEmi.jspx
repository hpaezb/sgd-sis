<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fu_eventoTablaRecepDocAdmSeguiEstEmi();});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtnuEmi" value=""/>        <input type="hidden" id="txtpnuDes" name="txtpnuDes" value=""/>        <table id ="myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"/>                    <th style="display:none"/>                    <th style="display:none"/>                    <th style="display:none"/>                    <th width="4%"/>                    <th width="7%">VENCIMIENTO</th>                    <th width="8%">REFERENCIA ORIGEN</th>                    <th width="" style="display:none">REMITENTE</th>                    <th width="7%">N° EXPEDIENTE</th>                    <th width="6%">TIPO DOCUMENTO</th>                    <th width="7%">N° DOCUMENTO</th>                    <th width="4%">FECHA EMISION</th>                    <th width="7%">DESTINATARIO</th>                    <th width="12%">ASUNTO</th>                    <th width="5%">DIAS PROY.</th>                    <th width="5%">FECHA LIMITE</th>                    <th width="" style="display:none">DIAS FALT.</th>                    <th width="3%">DIAS EXC.</th>                                        <th width="5%">FECHA RECEPCION</th>                    <th width="5%">FECHA ATENCION</th>                    <th width="5%">FECHA ARCHIVO</th>                    <th width="6%">ESTADO</th>                    <th width="6%">MOTIVO</th>                </tr>            </thead>				            <tbody>                <c:forEach var="recepDocumAdm" items="${recepDocumAdmList}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${status.count}</td>                        <td style="display:none">${recepDocumAdm.nuAnn}</td>                        <td style="display:none">${recepDocumAdm.nuEmi}</td>                                                <td style="display:none">${recepDocumAdm.nuDes}</td>                                                <td>                            <!--<c:choose>                                <c:when test='${recepDocumAdm.esDocRec eq "0"}'>                                    <c:choose>                                        <c:when test='${recepDocumAdm.coPri eq "3"}'>                                            <button title ="Detalle Documento muy Urgente" class="btn btn-danger btn-xs" onclick="editarDocumentoRecepClickConsul('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:when test='${recepDocumAdm.coPri eq "2"}'>                                            <button title ="Detalle Documento Urgente" class="btn btn-warning btn-xs" onclick="editarDocumentoRecepClickConsul('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:otherwise>                                            <button title ="Detalle Documento" class="btn btn-success btn-xs" onclick="editarDocumentoRecepClickConsul('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:otherwise>                                    </c:choose>                                </c:when>                                <c:otherwise>                                    <button title ="Detalle Documento" class="btn btn-default btn-xs" onclick="editarDocumentoRecepClickConsul('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                    </c:otherwise>                            </c:choose>-->                            <c:choose>                                <c:when test='${recepDocumAdm.existeDoc eq "1"}'>                                    <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                </c:when>                            </c:choose>									                            <c:choose>                                <c:when test='${recepDocumAdm.existeAnexo eq "1"}'>                                    <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexo('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}', '${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                </c:when>                            </c:choose>	                                                            </td>                         <td>${recepDocumAdm.coEstVen}</td>                        <td>${recepDocumAdm.deEmiRef}</td>                        <td style="display:none">${recepDocumAdm.deOriEmi}</td>                        <td>${recepDocumAdm.nuExpediente}</td>                        <td>${recepDocumAdm.deTipDocAdm}</td>                         <td>${recepDocumAdm.nuDoc}</td>                        <td>${recepDocumAdm.feEmiCorta}</td>                        <td>${recepDocumAdm.deEmpDes}</td>                        <td>${recepDocumAdm.deAsuM}</td>                        <td>${recepDocumAdm.nuDiaAte}</td>                        <td>${recepDocumAdm.fLimiteCorta}</td>                        <td style="display:none">FALTA</td>                        <td>${recepDocumAdm.nuDiaExc}</td>                                                <td>${recepDocumAdm.feRecCorta}</td>                                                <td>${recepDocumAdm.feAteDocCorta}</td>                                                <td>${recepDocumAdm.feArcDocCorta}</td>                                                <!--<td>${recepDocumAdm.coEtiquetaRec}_${recepDocumAdm.deEsDocDes}</td>-->                                                <td>${recepDocumAdm.deEsDocDes}</td>                                                <td>${recepDocumAdm.deMotivo}</td>                                            </tr>                </c:forEach>                        </tbody>			        </table>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>            <div id="divflotante" class="divTip">&amp;nbsp;</div>    	      <script>        $(document).ready(function(){            var idTable="myTableFixed";//            var col=21;//            setEtiquetasListaDoc(idTable,col);            pintarEstadosVencimientoSeguiEstEmi();        });    </script></jsp:root>	