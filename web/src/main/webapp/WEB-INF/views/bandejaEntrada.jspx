<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>     <script type="text/javascript">            jQuery(document).ready(function() {        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAFIRMA" name="div_BANDEJAFIRMA" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAVB" name="div_BANDEJAVB" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAPROYECTO" name="div_BANDEJAPROYECTO" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAENTRADA" name="div_BANDEJAENTRADA" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJADELEGADOS" name="div_BANDEJADELEGADOS" style="margin-left:40px;display: none;" class="list-group-item" href="#"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAENVIADOS" name="div_BANDEJAENVIADOS" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAMUYURGENTE" name="div_BANDEJAMUYURGENTE" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJAURGENTE" name="div_BANDEJAURGENTE" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_BANDEJASALIDA" name="div_BANDEJASALIDA" class="list-group-item" style="cursor:pointer;display: none;"></div>'));        $('#divTablaBandejaEntrada').append($('<div id="div_etiquetasSGD" name="div_etiquetasSGD" ></div>'));        refrescarBandejaEntradaJSON();        refrescarBandejaEntradaBuscarExpediente();    });function refrescarBandejaEntradaJSON(){        refrescarBandejaEntradaJson('BANDEJAFIRMA');        refrescarBandejaEntradaJson('BANDEJAVB');        refrescarBandejaEntradaJson('BANDEJAPROYECTO');        refrescarBandejaEntradaJson('BANDEJAENTRADA');        refrescarBandejaEntradaJson('BANDEJADELEGADOS');        refrescarBandejaEntradaJson('BANDEJAENVIADOS');        refrescarBandejaEntradaJson('BANDEJASALIDA');        refrescarBandejaEntradaJson('BANDEJAMUYURGENTE');        refrescarBandejaEntradaJson('BANDEJAURGENTE');        refrescarBandejaEntradaEtiquetasJson();}        </script>    <table>        <tbody>            <tr>                <td id="divBuscarExpediente" style="display: block;margin-left: 15px;margin-top: 15px;margin-bottom: -15px;">                                    </td>                <td style="display: block;">                    <div id="divTablaBandejaEntrada" class="list-group bandeja alineaIzq">                       <a id="titleBandejaEntrada" style="min-width: 296px;min-height: 54px;" title="Actualizar Cambios" onclick="refrescarBandejaEntradaJSON();" class="list-group-item active" href="#">                            <!--<h4 style="text-align: center;font-size:21px" class="list-group-item-heading">Documentos de SG  <span class="glyphicon glyphicon-refresh"></span></h4>                            <p class="list-group-item-text">Resumen de los documentos principales de <strong>SG</strong>.</p>-->                        </a>                                                               </div>                                                      </td>                <td>                                        <div id="carousel-example-generic" class="carousel slide" style="margin-left: 10%;" data-ride="carousel">                                                <!-- Indicators -->                        <ol  class="carousel-indicators">                            <c:forEach var="ImPortada" items="${lstImPortada}" varStatus="status">                              <c:choose>                                        <c:when test='${ImPortada.celeCodele eq "0"}'>                                             <li data-target="#carousel-example-generic" data-slide-to="${ImPortada.celeCodele}" class="active"></li>                                        </c:when>                                        <c:otherwise>                                             <li data-target="#carousel-example-generic" data-slide-to="${ImPortada.celeCodele}"></li>                                        </c:otherwise>                                    </c:choose>                                                  </c:forEach>                        </ol>                        <!-- Wrapper for slides -->                                               <div class="carousel-inner" role="listbox">                            <c:forEach var="ImPortada" items="${lstImPortada}" varStatus="status">                                <c:choose>                                        <c:when test='${ImPortada.celeCodele eq "0"}'>                                              <div data-slide-to="${ImPortada.celeCodele}" class="item active">                                                    <img  class="imagenInicio" src="data:image/jpeg;base64,${ImPortada.celeDesele}" alt="" border="0"/>                                                  </div>                                        </c:when>                                        <c:otherwise>                                              <div data-slide-to="${ImPortada.celeCodele}" class="item">                                                <img  class="imagenInicio" src="data:image/jpeg;base64,${ImPortada.celeDesele}" alt="" border="0"/>                                              </div>                                        </c:otherwise>                                    </c:choose>                                                     </c:forEach>                        </div>                                                                      <!-- Controls -->                        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">                          <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>                          <span class="sr-only">Previous</span>                        </a>                        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">                          <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>                          <span class="sr-only">Next</span>                        </a>                                              <!-- para habilitar si se desea slider-->                    </div>                    <!--<img class="imagenInicio" src="data:image/png;base64,${b64ImgDefault}" border="0" alt=""/>-->                </td>            </tr>            <c:if test="${usuario.diasAntesExpiraClave le 15}">            <tr>                <td colspan="2" align="right">                    <div class="alert alert-danger alert-dismissable" style="margin-bottom:0px;width:40%">                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                        <h4 style="margin-bottom:0px;"><strong>SU CONTRASEÑA CADUCARÁ EN ${usuario.diasAntesExpiraClave} DÍA(S)</strong></h4>                    </div>                                       </td>            </tr>            </c:if>        </tbody>    </table>                        <script type="text/javascript">        /* para habilitar si se desea slider        jQuery(document).ready(function() {            var $numberofSlides = $('.item').length;            var $currentSlide = Math.floor((Math.random() * $numberofSlides));            $('.carousel-inner .item').each(function(){              var $slideValue = $(this).attr('data-slide-to');              if($currentSlide === $slideValue * 1) {                $(this).addClass('active');              } else {                $(this).removeClass('active');              }            });        });*/    </script>                    </jsp:root>