<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <table cellpadding="0" cellspacing="0" width="100%">        <tr valign="top">            <td width="100%">                <input id="txtTblRefEmiFilaWhereButton" name="txtTblRefEmiFilaWhereButton" type="hidden"/>                <input id="txtTblRefEmiColWhereButton" name="txtTblRefEmiColWhereButton" type="hidden"/>                                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                           <table class="ui-state-default" style="width:100%;">                                 <thead>                                        <tr>                                            <th width="12%">AÃ±o</th>                                            <th width="32%">Tipo Documento</th>                                            <th width="37%">Nro. Documento</th>                                                                                <th width="12%">Fecha</th>                                            <th width="5%"/>                                        </tr>                                 </thead>                           </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:55px;'>                          <table class="ui-datatable-edit-data" id="tblRefEmiDocAdm"  cellpadding="0" cellspacing="0" style="width:100%;">                              <tbody>                                <c:forEach var="lReferencia" items="${lstReferenciaDocAdmEmi}" varStatus="status">       