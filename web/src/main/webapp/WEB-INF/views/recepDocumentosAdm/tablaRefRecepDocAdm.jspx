<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                   <table class="ui-state-default" style="width:100%;">                         <thead>                                <tr>                                    <th width="30%">Tipo Documento</th>                                    <th width="40%">Número de Documento</th>                                    <th width="15%">F. Emisión</th>                                    <th width="20%"></th>                                </tr>                         </thead>                   </table>            </div>            <div class="bx_sb ui-datatable-scrollable-body" style='height:55px;'>                  <table class="ui-datatable-data" id="tbListaRefRecDocAdm" style="width:100%;">                      <tbody>                           <c:forEach var="refDocAdm" items="${refRecepDocumAdmList}" varStatus="status">                               <c:choose>                                   <c:when test="${status.count % 2 == 0}">                                     <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                   </c:when>                                   <c:otherwise>                                     <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                   </c:otherwise>                                 </c:choose>                               <tr class="${rowStyle}">                               <c:choose>                                   <c:when test="${status.count eq 1}">                                       <!--td width="16">${status.count}</td-->                                       <td width="31%">${refDocAdm.liTipDoc}</td>                                       <td width="42%">${refDocAdm.liNuDoc}</td>                                       <td width="15%">${refDocAdm.feEmiCorta}</td>                                       <td width="12%"><button title ="Ver Documento" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="fn_verDocumento('${refDocAdm.nuAnnRef}','${refDocAdm.nuEmiRef}');" type="button">                                       <span class="button.ui-button-icon-only ui-icon icon_pdf"/>                                       </button></td>                                                                          </c:when>                                   <c:otherwise>                                       <!--td>${status.count}</td-->                                       <td>${refDocAdm.liTipDoc}</td>                                       <td>${refDocAdm.liNuDoc}</td>                                       <td>${refDocAdm.feEmiCorta}</td>                                       <td><button title ="Ver Documento" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="fn_verDocumento('${refDocAdm.nuAnnRef}','${refDocAdm.nuEmiRef}');" type="button">                                       <span class="button.ui-button-icon-only ui-icon icon_pdf"/>                                       </button></td>                                                                                                                  </c:otherwise>                               </c:choose>                              </tr>                           </c:forEach>                      </tbody>                  </table>            </div>        </div></jsp:root>