<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {            fu_eventoTablaRecepDocAdm();        });</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn" value=""/>        <input type="hidden" id="txtpnuEmi" name="txtnuEmi" value=""/>        <input type="hidden" id="txtpnuDes" name="txtpnuDes" value=""/>        <input type="hidden" id="txtptiCap" name="txtptiCap" value=""/>        <input type="hidden" id="txtpcoPri" name="txtpcoPri" value=""/>                <table id ="myTableFixed" class="display" width="100%">            <thead>                <tr class="ui-state-default">                    <th style="display:none"></th>                     <th style="display:none"></th>                    <th style="display:none"></th>                    <th style="display:none"></th>                                                 <th style="display:none"></th>                                                 <th style="display:none"></th>                    <th width="7%"></th>                    <th width="3%">N°</th>                    <th width="4%">FECHA EMISION</th>                    <th width="15%">REMITENTE</th>                    <th width="8%">TIPO DOC.</th>                    <th width="10%">N° DOCUMENTO</th>                    <th width="12%">DESTINATARIO</th>                    <th width="23%">ASUNTO</th>                    <th width="10%">NRO EXPEDIENTE</th>                    <th width="7%">ESTADO</th>                    <th width="10%"></th>                </tr>            </thead>				            <tbody>                <c:forEach var="recepDocumAdm" items="${recepDocumAdmList}" varStatus="status">                    <tr class="gradeA" tabindex="0">                        <td style="display:none">${status.count}</td>                        <td style="display:none">${recepDocumAdm.nuAnn}</td>                        <td style="display:none">${recepDocumAdm.nuEmi}</td>                        <td style="display:none">${recepDocumAdm.tiCap}</td>                                                            <td style="display:none">${recepDocumAdm.nuDes}</td>                                                            <td style="display:none">${recepDocumAdm.coPri}</td>                                                <td>                            <c:choose>                                <c:when test='${recepDocumAdm.esDocRec eq "0"}'>                                    <c:choose>                                        <c:when test='${recepDocumAdm.coPri eq "3"}'>                                            <button title ="Editar Documento muy Urgente" class="btn btn-danger btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.coPri}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:when test='${recepDocumAdm.coPri eq "2"}'>                                            <button title ="Editar Documento Urgente" class="btn btn-warning btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.coPri}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:when>                                        <c:otherwise>                                            <button title ="Editar Documento" class="btn btn-success btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.coPri}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                            </c:otherwise>                                    </c:choose>                                </c:when>                                <c:otherwise>                                    <button title ="Editar Documento" class="btn btn-default btn-xs" onclick="editarDocumentoRecepClick('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}','${recepDocumAdm.nuDes}','${recepDocumAdm.coPri}','${recepDocumAdm.existeDoc}','${recepDocumAdm.existeAnexo}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                    </c:otherwise>                            </c:choose>                            <c:choose>                                <c:when test='${recepDocumAdm.existeDoc eq "1"}'>                                    <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                </c:when>                            </c:choose>									                            <c:choose>                                <c:when test='${recepDocumAdm.existeAnexo eq "1"}'>                                    <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexo('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}', '${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                </c:when>                            </c:choose>	                                   <c:choose>                                <c:when test='${recepDocumAdm.existeDoc eq "1"}'>                                    <button title ="Ver Documento de Inicio" class="btn btn-default btn-xs" onclick="fn_verDocumentoInicialObj('${recepDocumAdm.nuAnn}', '${recepDocumAdm.nuEmi}', '${recepDocumAdm.nuDes}');" type="button"><span class="glyphicon glyphicon-book" style="color:#388138;"><jsp:text/></span></button>                                </c:when>                            </c:choose>	                        </td>                        <c:choose>                            <c:when test='${recepDocumAdm.coPri eq "3"}'>                                 <td align="center" style="font-size: 1em; color:white;">                                <div style="background-color: #D9534F; border-radius: 25px; width:25px; height:25px; line-height:25px;">                                    ${recepDocumAdm.nuCorDes}                                    </div>                                </td>                                                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${recepDocumAdm.coPri eq "2"}'>                                                                <td align="center" style="font-size: 1em; color:white;">                                <div style="background-color: #F0AD4E;  border-radius: 25px; width:25px; height:25px; line-height:25px;">                                    ${recepDocumAdm.nuCorDes}                                    </div>                                </td>                            </c:when>                        </c:choose>                        <c:choose>                            <c:when test='${recepDocumAdm.coPri eq "1"}'>                                <td align="center">${recepDocumAdm.nuCorDes}</td>                         </c:when>                            </c:choose>                        <td>${recepDocumAdm.feEmiCorta}</td>                        <td>${recepDocumAdm.deOriEmi}</td>                        <td>${recepDocumAdm.deTipDocAdm}</td>                        <td>${recepDocumAdm.nuDoc}</td>                        <td>${recepDocumAdm.deEmpDes}</td>                        <td>${recepDocumAdm.deAsuM}</td>                        <td>${recepDocumAdm.nuExpediente}</td>                        <td>${recepDocumAdm.coEtiquetaRec}_${recepDocumAdm.deEsDocDes}</td>                        <td>${recepDocumAdm.enprocesoatencion}</td>                    </tr>                </c:forEach>            </tbody>			        </table>    </div>    <c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>      </c:if>                             <div id="divflotante" class="divTip">&amp;nbsp;</div>       <script>        $(document).ready(function(){            var idTable="myTableFixed";            var col=15;            setEtiquetasListaDoc(idTable,col);        });    </script></jsp:root>	