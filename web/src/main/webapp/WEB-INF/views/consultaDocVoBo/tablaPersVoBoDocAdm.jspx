<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventPersVoBoDocAdm();});</script>                                <table cellpadding="0" cellspacing="0" width="100%">        <tr>            <td width="100%">                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                           <table class="ui-state-default" style="width:100%;">                                 <thead>                                        <tr>                                            <th width="25%">Dependencia</th>                                            <th width="75%">Personal</th>                                        </tr>                                 </thead>                           </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:180px;'>                          <table class="ui-datatable-edit-data" id="tblPersVoBoDocAdm" cellpadding="0" cellspacing="0" style="width:100%;">                              <tbody>                                <c:forEach var="objEmpleado" items="${lstEmpVoBoDocAdm}" varStatus="status">                                      <tr>                                        <td style="display: none;">${objEmpleado.coEmpleado}</td>                                        <td style="display: none;">${objEmpleado.coDependencia}</td>                                        <td width="25%">                                            <input type="text" style="width:95%;" value="${fn:escapeXml(objEmpleado.deDependencia)}" readonly="true"/>                                        </td>                                        <td width="65%">                                            <input type="text" style="width:98%;" value="${fn:escapeXml(objEmpleado.nombre)}" readonly="true"/>                                        </td>                                                                                  <c:choose>                                            <c:when test="${objEmpleado.inVobo eq '1'}">                                                <td width="10%" class="label-success" align="center"><span id="spanVbTblEdit" style="color:white;font-weight: bold;font-size: 12px;" title="Con Visto Bueno.">V.B.</span></td>                                                                                  </c:when>                                            <c:when test="${objEmpleado.inVobo eq '2'}">                                                <td width="10%" class="label-warning" align="center"><span id="spanVbTblEdit" style="color:white;font-weight: bold;font-size: 12px;" title="Observado.">V.B.</span></td>                                                                                  </c:when>                                                                                            <c:otherwise>                                                <td width="10%" class="label-danger" align="center"><span id="spanVbTblEdit" style="color:white;font-weight: bold;font-size: 12px;" title="Sin Visto Bueno.">V.B.</span></td>                                                                                  </c:otherwise>                                        </c:choose>                                      </tr>                                                                    </c:forEach>                               </tbody>                          </table>                    </div>                </div>                            </td>        </tr>    </table>    </jsp:root>