<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <fieldset>        <legend>CIUDADANO</legend>        <table cellpadding="0" cellspacing="0" width="100%">            <tr valign="top">                <td width="100%">                    <div class="ui-datatable ui-datatable-scrollable">                        <div class="ui-datatable-scrollable-header">                               <table class="ui-state-default">                                     <thead>                                            <tr>                                                <th width="80">DNI</th>                                                <th width="520">Ciudadano</th>                                            </tr>                                     </thead>                               </table>                        </div>                        <div class="bx_sb ui-datatable-scrollable-body">                              <table class="ui-datatable-edit-data" id="tblDestEmiDocAdmCiudadano" cellpadding="0" cellspacing="0">                                  <tbody>                                      <c:forEach var="lDestinatario" items="${lstDestintarioCiudadanoDocAdmEmi}" varStatus="status">                                          <tr>                                                <td width="80">                                                    <input type="text" style="width:80px;" maxlength="8" value="${lDestinatario.nuDni}" readonly="true"/>                                                </td>                                                                                        <td width="540">                                                    <input type="text" style="width:540px;" value="${lDestinatario.deCiudadano}" readonly="true"/>                                                </td>                                                <td width="22">                                                    <div class="ui-state-highlight-text">                                                        <span class="ui-icon ui-icon-check" title="OK"/>                                                    </div>                                                    </td>                                          </tr>                                      </c:forEach>                                   </tbody>                              </table>                        </div>                                        </div>                                                    </td>            </tr>        </table>    </fieldset>        </jsp:root>