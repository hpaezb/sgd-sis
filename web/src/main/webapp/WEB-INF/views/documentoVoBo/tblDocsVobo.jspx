<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fu_iniTblDocsVoBo();});</script>  	    <div class="ui-datatable-fixed">        <input type="hidden" id="txtpnuAnn" name="txtpnuAnn"/>        <input type="hidden" id="txtpnuEmi" name="txtpnuEmi"/>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                <table class="ui-state-default" style="width:100%;">                    <thead>                        <tr class="ui-state-default">                            <th style="display:none"></th>                            <th style="display:none"></th>                            <th width="7%"></th>                            <th width="4%">N°</th>                            <th width="10%">REFERENCIA ORIGEN</th>                            <th width="4%">FECHA</th>                            <th width="7%">TIPO DOC.</th>                            <th width="8%">N° DOCUMENTO</th>                            <th width="22%">ASUNTO</th>                            <th width="14%">DESTINO V.B.</th>                            <th width="8%">ELABORADO POR </th>                            <th width="10%">NRO EXPEDIENTE</th>                            <th width="6%">ESTADO</th>                        </tr>                    </thead>                                    </table>            </div>            <div class="ui-datatable-scrollable-body" style='height:470px;'>                <table class="ui-datatable" id="tblDocsVobo" style="width:100%;">                                    <tbody>                        <c:forEach var="docVobo" items="${lsDocsVobo}" varStatus="status">                            <c:choose>                                <c:when test="${status.count % 2 == 0}">                                    <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                </c:when>                                <c:otherwise>                                    <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                </c:otherwise>                                                                </c:choose>                                                <tr class="${rowStyle}" tabindex="0">                                <td style="display:none">${docVobo.nuAnn}</td>                                <td style="display:none">${docVobo.nuEmi}</td>                                <td width="7%">                                    <button title ="Editar Documento" class="btn btn-success btn-xs" onclick="editarDocumentoVoBoButtonFila('${docVobo.nuAnn}', '${docVobo.nuEmi}');" type="button"><span class="glyphicon glyphicon-edit"><jsp:text/></span></button>                                                                                                                                <c:choose>                                            <c:when test='${docVobo.existeDoc eq "1"}'>                                            <button title ="Ver Documento" class="btn btn-default btn-xs" onclick="fn_verDocumento('${docVobo.nuAnn}', '${docVobo.nuEmi}');" type="button"><span class="glyphicon glyphicon-file"><jsp:text/></span></button>                                                                                                         </c:when>                                        </c:choose>									                                        <c:choose>                                            <c:when test='${docVobo.existeAnexo eq "1"}'>                                            <button title ="Documentos Anexos" class="btn btn-default btn-xs" onclick="fn_verAnexo('${docVobo.nuAnn}', '${docVobo.nuEmi}', 'N');" type="button"><span class="glyphicon glyphicon-paperclip"><jsp:text/></span></button>                                            </c:when>                                        </c:choose>                                                                    </td>                                <td align="center" width="4%">${docVobo.nuCorEmi}</td>                                <td width="10%">${docVobo.deEmiRef}</td>                                <td width="4%">${docVobo.feEmiCorta}</td>                                <td width="7%">${docVobo.deTipDocAdm}</td>                                <td width="8%">${docVobo.nuDoc}</td>                                <td width="22%">${docVobo.deAsu}</td>                                <td width="14%">${docVobo.deEmpVb}</td>                                <td width="8%">${docVobo.deEmpElabora}</td>                                <td width="10%">${docVobo.nroExpediente}</td>                                <td width="6%">${docVobo.deEsDocEmi}</td>                            </tr>                        </c:forEach>                        <c:if test="${empty lsDocsVobo}">                        <td>No se encuentran registros.</td>                    </c:if>                                                </tbody>                </table>            </div>            <c:if test="${not empty lsDocsVobo}">                <div>Registros: ${lsDocsVobo.size()}</div>            </c:if>                       </div>    </div>    <c:if test="${not empty msjEmision}">        <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>            <h4 style="margin-bottom:0px;">${msjEmision}</h4>        </div>                </c:if>    <input id="txtAnnioActual" name="txtAnnioActual" type="hidden" value="${annioActual}"/>    <div id="divflotante" class="divTip">&amp;nbsp;</div>    	</jsp:root>	