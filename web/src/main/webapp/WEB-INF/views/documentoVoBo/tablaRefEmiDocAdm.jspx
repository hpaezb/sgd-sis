<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {/*fn_eventRefEmiDocAdm();*/});</script>                                <table cellpadding="0" cellspacing="0" width="100%">        <tr valign="top">            <td width="100%">                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                           <table class="ui-state-default" style="width:100%;">                                 <thead>                                        <tr>                                            <th width="10%">AÃ±o</th>                                            <th width="34%">Tipo Documento</th>                                            <th width="10%">Emi/Rec</th>                                            <th width="25%">Nro. Documento</th>                                                                                <th width="12%">Fecha</th>                                            <th width="9%"/>                                        </tr>                                 </thead>                           </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:28px;'>                          <table class="ui-datatable-edit-data" id="tblRefEmiDocAdm"  cellpadding="0" cellspacing="0" style="width:100%;">                              <tbody>                                <c:forEach var="lReferencia" items="${lstReferenciaDocAdmEmi}" varStatus="status">                                    <tr>                                        <td width="10%">                                            <input type="text" style="width:90%;" readonly="true" value="${lReferencia.nuAnnRef}"/>                                        </td>                                        <td width="34%">                                            <input type="text" style="width:96%;" readonly="true" value="${lReferencia.deTipoDoc}"/>                                        </td>                                        <td width="10%" align="center">                                            <c:choose>                                                    <c:when test="${lReferencia.tipDocRef eq 'emi'}">                                                            <input type="radio" name="group${status.count}" value="emi" checked="true" disabled="true"/>&amp;nbsp;<input type="radio" name="group${status.count}" value="rec" disabled="true"/>                                                    </c:when>                                                    <c:otherwise>                                                            <input type="radio" name="group${status.count}" value="emi" disabled="true"/>&amp;nbsp;<input type="radio" name="group${status.count}" value="rec" checked="true" disabled="true"/>                                                    </c:otherwise>                                            </c:choose>                                                                                       </td>                                        <td width="27%"><input type="text" style="width:95%;" value="${lReferencia.liNuDoc}" readonly="true"/></td>                                        <td width="12%"><input type="text" style="width:88%;" value="${lReferencia.feEmiCorta}" readonly="true"/></td>                                        <td width="5%">                                            <button title ="Ver Documento Referenciado" class="btn btn-default btn-xs" onclick="fn_verDocumentoRefEmi(this);" type="button">                                                <span class="button.ui-button-icon-only ui-icon icon_pdf"/>                                            </button>                                        </td>                                        <td style="display:none"/>                                        <td style="display:none">${lReferencia.nuAnnRef}</td>                                        <td style="display:none">${lReferencia.nuEmiRef}</td>                                        <td style="display:none">${lReferencia.nuDesRef}</td>                                        <td style="display:none">${lReferencia.accionBd}</td>                                        <td style="display:none">${lReferencia.coRef}</td>                                        <td width="2%">                                            <div class="ui-state-highlight-text">                                                <span class="ui-icon ui-icon-check" title="OK"/>                                            </div>                                            </td>                                        <td style="display:none">1</td>                                    </tr>                                                                    </c:forEach>                               </tbody>                          </table>                    </div>                </div>                            </td>        </tr>    </table>    </jsp:root>