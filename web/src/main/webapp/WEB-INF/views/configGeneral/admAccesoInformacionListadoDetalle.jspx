<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <button id="btn-eliminar" title="Eliminar motivo" class="btn btn-default btn-xs" onclick="fn_eliminarMotDocDep();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <script></script>    <table border="0" width="655px;">        <tr valign="top">            <td style="width: 32px;" >                <div class="toolbar-left-vertical">                    <button id="btn-Cambiar" title="Agregar Usuario a Depencia" class="btn btn-default btn-xs" type="button" onclick="fn_agregarDetallePermiso();" ><span class="glyphicon glyphicon-plus-sign"></span></button>                        <button id="btn-Cambiar" title="Quitar Usuario a Dependencia" class="btn btn-default btn-xs" type="button" onclick="fn_eliminarDetalleGrupo();" ><span class="glyphicon glyphicon-minus-sign"></span></button>                    </div>            </td>            <td>                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                        <table class="ui-state-default">                            <thead>                                <tr>                                    <th width="385px;">Empleado</th>                                    <th width="80px;">Estado</th>                                </tr>                            </thead>                        </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:250px;width: 495px;'>                        <table class="ui-datatable-edit-data" id="tblDestDetalle" cellpadding="0" cellspacing="0">                            <tbody>                                <tr class="oculto">                                       <td width="393px;">                                        <input name="txtEmpleado" type="text" value="" readonly="readonly" style="width:350px;height:21px;"/>                                        <button id="btn-cargar-emp" title="Buscar empleado" class="btn btn-default btn-xs" type="button" onclick="fn_cambiarUsuario(this)" coDep="" coEmp="" ><span class="glyphicon glyphicon-search"></span></button>                                    </td>                                    <td width="100px;" style="text-align: center">                                                                               <input name="txtActivo" type="checkbox" checked="checked"/>                                                                                                                                          <button style="display:none;" id="btn-cargar-act"  class="btn btn-default btn-xs" type="button"  esAct="" ><span class="glyphicon glyphicon-search"></span></button>                                    </td>                                </tr>                                <c:forEach var="refPermisoDet" items="${permisoDetList}" varStatus="status">                                    <tr>                                        <td width="393px;">                                            <input name="txtEmpleado" type="text" value="${refPermisoDet.cdesUser}" readonly="readonly" style="width:350px;height:21px;"/>                                            <button style="display:none;" id="btn-cargar-emp"  class="btn btn-default btn-xs" type="button"  coEmp="${refPermisoDet.coUse}" ><span class="glyphicon glyphicon-search"></span></button>                                        </td>                                        <td width="100px;" style="text-align: center">                                            <c:choose>                                                <c:when test="${refPermisoDet.esAct eq 0}">                                                    <input name="txtActivo" type="checkbox" checked="checked"/>                                                                                                  </c:when>                                                <c:otherwise>                                                    <input name="txtActivo" type="checkbox"/>                                                                                                    </c:otherwise>                                            </c:choose>                                            <button style="display:none;" id="btn-cargar-act"  class="btn btn-default btn-xs" type="button"  esAct="${refPermisoDet.esAct}" ><span class="glyphicon glyphicon-search"></span></button>                                                                                    </td>                                                                                                       </tr>                            </c:forEach>                            </tbody>                        </table>                    </div>                </div>            </td>        </tr>    </table>    <script>        fn_eventGrupoDestino();    </script></jsp:root>