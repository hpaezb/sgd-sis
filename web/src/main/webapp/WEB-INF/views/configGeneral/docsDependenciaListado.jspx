<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <button id="btn-eliminar" title="Eliminar documento" class="btn btn-default btn-xs" onclick="fn_eliminarDocDepen();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <div class="ui-datatable-fixed">        <table id="tblDocDepen" class="display" style="width:40%;">            <thead>                <tr class="ui-state-default">                    <th width="40">CÓDIGO</th>                    <th width="350">DESCRIPCIÓN</th>                    <th width="77">OPCIONES</th>                </tr>            </thead>				            <tbody>                <c:forEach var="refDocDep" items="${docDependenciaList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <td>${refDocDep.coTipDoc}</td>                        <td>${refDocDep.tiDescrip}</td>                        <td>                            <div id="btn-opciones">                                <button id="btn-Cambiar" title="Cambiar o reemplazar documento" class="btn btn-default btn-xs" type="button" onclick="fn_cambiarDoc('${refDocDep.coTipDoc}');" codDoc="${refDocDep.coTipDoc}"  ><span class="glyphicon glyphicon-search"></span></button>                            </div>                        </td>                    </tr>                </c:forEach>            </tbody>			        </table>    </div>    <input type="hidden" id="codDoc"/>    <input type="hidden" id="codDocReempl"/>    <script>fn_eventDocDependencia();</script></jsp:root>