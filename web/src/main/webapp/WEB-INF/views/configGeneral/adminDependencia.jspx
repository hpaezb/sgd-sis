<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <div class="ui-panel ui-corner-all" style="" id="divDependenciaBody">        <div class="ui-panel-titlebar ui-widget-header ui-corner-all">            <span>Administración de Unidades Orgánicas</span>        </div>        <div class="ui-panel-body">            <fieldset>                <div class="btn-group btn-group-lg">                    <button id="btn-nuevo" title="Nuevo" class="btn btn-primary btn-lg" onclick="fu_goNewAdmUUOO();" type="button"><span class="glyphicon glyphicon-file"></span> Nuevo</button>                    <button id="btn-editar" title="Editar" class="btn btn-primary btn-lg" onclick="fu_goEditarAdmUUOO();" type="button"><span class="glyphicon glyphicon-edit"></span> Editar</button>                    <button id="btn-salir" title="Salir" value = "Salir" class="btn btn-primary btn-lg" onclick="cerrarPantallaConfigTabla();" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>            <fieldset>                <legend>Buscar</legend>                    <table>                        <tbody>                            <tr>                                <td width="50px"></td>                                <td>Descripción </td>                                <td>Tipo </td>                            </tr>                            <tr>                                <td width="50px"></td>                                <td><input type="text" id="txtBusDependencia" style="width: 500px;"/></td>                                <td>                                    <select id="cbBusTipo" name="cbBusTipo" >                                        <option value="2" selected="true">Todo</option>                                        <option value="0">Institución</option>                                        <option value="1">Comité especial</option>                                    </select>                                </td>                                <td><button id="btn-buscar" title="Buscar" class="btn btn-default btn-sm" onclick="fn_admBuscarDep();" type="button"><span class="glyphicon glyphicon-search"></span> Buscar</button></td>                            </tr>                        </tbody>                    </table>            </fieldset>            <fieldset>                <legend>Unidades Orgánicas</legend>                <div id="divDependenciaDetalle">                    <jsp:directive.include file="adminDependenciaLista.jspx"/>                </div>            </fieldset>        </div>    </div>    <div id="divDependenciaEdit"><jsp:text/></div>                    </jsp:root>