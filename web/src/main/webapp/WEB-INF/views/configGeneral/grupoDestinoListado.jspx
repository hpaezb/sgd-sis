<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <select id="codGrupoDestino" onchange="fn_listaGrupoDetalle();">        <option value="">Seleccione un GRUPO</option>        <c:forEach var="refGrupoDest" items="${grupoDestinoList}" varStatus="status">            <option value="${refGrupoDest.coGruDes}">${refGrupoDest.deGruDes}</option>        </c:forEach>    </select>    <script>        fn_listaGrupoDetalle();    </script></jsp:root>