<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <button id="btn-eliminar" title="Eliminar motivo" class="btn btn-default btn-xs" onclick="fn_eliminarMotDocDep();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <script>jsonDetGrupo = [];</script>    <table>        <tr>            <td>Nombre del grupo</td>            <td>:</td>            <td><input type="text" id="txtNombreGrupo" style="width: 590px;" onkeyup="fn_cambioNombreGD();" onkeypress="fn_cambioNombreGD();" maxlength="95"/></td>        </tr>    </table>    <table border="0" width="745px;">        <tr valign="top">            <td style="width: 32px;" >                <div class="toolbar-left-vertical">                    <button id="btn-Cambiar" title="Agregar destino dependencia" class="btn btn-default btn-xs" type="button" onclick="fn_agregarDetalleGrupoDepenVar();" ><span class="glyphicon glyphicon-plus-sign"></span></button>                        <button id="btn-Cambiar" title="Quitar destino dependencia" class="btn btn-default btn-xs" type="button" onclick="fn_eliminarDetalleGrupoVar();" ><span class="glyphicon glyphicon-minus-sign"></span></button>                    </div>            </td>            <td>                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                        <table class="ui-state-default">                            <thead>                                <tr>                                    <th width="187px;">Código</th>                                    <th width="530px;">Nombre o Descripción</th>                                                                    </tr>                            </thead>                        </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:250px;width: 735px;'>                        <table class="ui-datatable-edit-data" id="tblDestDetalle" cellpadding="0" cellspacing="0">                            <tbody>                                <tr class="oculto">                                    <td width="160px;">                                        <input name="txtCoDepen" type="text" value="" readonly="readonly" style="width:150px;"/>                                    </td>                                    <td width="540px;">                                        <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:500px;"/>                                        <button id="btn-cargar-dep" title="Buscar dependencia" class="btn btn-default btnPequenoSGD" type="button" onclick="fn_cargarDestinoVar(this);" coDep="" ><span class="glyphicon glyphicon-search"></span></button>                                    </td>                                                                        <td width="33px;">                                        <button id="btn-msg" title="ERROR falta seleccionar una dependencia" class="btn btn-default btnPequenoSGD" type="button" ><span class="glyphicon glyphicon-warning-sign" style="color:red;"></span></button>                                    </td>                                </tr>                                <c:forEach var="refGrupoDestDet" items="${grupoDestinoDetList}" varStatus="status">                                    <tr>                                        <td width="160px;">                                            <input name="txtCoDepen" type="text" value="${refGrupoDestDet.co_emp}" readonly="readonly" style="width:150px;"/>                                        </td>                                        <td width="540px;">                                            <input name="txtDesDepen" type="text" value="${refGrupoDestDet.de_nombre_emp}" readonly="readonly" style="width:500px;"/>                                            <button id="btn-cargar-dep" title="Agregar dependencia" class="btn btn-default btnPequenoSGD" type="button" onclick="fn_cargarDestinoVar(this);" coDep="${refGrupoDestDet.co_emp}"><span class="glyphicon glyphicon-search"></span></button>                                        </td>                                        <td width="33px;">                                            <button id="btn-msg" title="Ok" class="btn btn-default btnPequenoSGD" type="button" ><span class="glyphicon glyphicon-ok" style="color:green;" ></span></button>                                        </td>                                <script>jsonDetGrupo.push({"coDep": "${refGrupoDestDet.co_dep}", "coEmp": "${refGrupoDestDet.co_emp}"});</script>                                </tr>                            </c:forEach>                            </tbody>                        </table>                    </div>                </div>            </td>        </tr>    </table>    <script>        fn_eventGrupoDestino();    </script></jsp:root>