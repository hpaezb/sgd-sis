<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {            fn_iniRegistroCiudadanoNuevo('${ciudadano.nuLem}');            $('[data-toggle="tooltip"]').tooltip();        });</script>              <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarNuevoCiudadano();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar Cambios</button>                <button id="btn-regresar" title="Regresar al listado de ciudadanos" class="btn btn-primary btn-lg" onclick="fn_cerrarCiudadano();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divCiudadanoMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>            <legend>Detalle</legend>            <table>                <tbody>                    <tr>                        <td>Número Documento :</td>                        <td>                            <input type="hidden" id="hidN_nuLem" value="${ciudadano.nuLem}" />                            <input id="txtNuLem" type="text" style="width: 500px;" value="${ciudadano.nuLem}" maxlength="8"/>                        </td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                        </td>                    </tr>                    <tr>                        <td>Apellido Paterno :</td>                        <td><input id="txtDeApp" type="text" style="width: 500px;" value="${ciudadano.deApp}" maxlength="40" onkeypress="return filtrarTeclado(event, false, public_apenom);"/></td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Este campo es obligatorio si la persona no tiene apellido materno.">(!)</a>                         </td>                    </tr>                                        <tr>                        <td>Apellido Materno :</td>                        <td><input id="txtDeApm" type="text" style="width: 500px;" value="${ciudadano.deApm}" maxlength="40" onkeypress="return filtrarTeclado(event, false, public_apenom);"/></td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Este campo es obligatorio si la persona no tiene apellido paterno.">(!)</a>                         </td>                    </tr>                    <tr>                        <td>Nombres :</td>                        <td><input id="txtDeNom" type="text" style="width: 500px;" value="${ciudadano.deNom}" maxlength="35" onkeypress="return filtrarTeclado(event, false, public_apenom);"/></td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                         </td>                    </tr>                    <tr>                        <td>Ubigeo Domicilio :</td>                        <td>                            <div class="input-group anchoTotal">                            <input readonly="readonly" id="txtUbiDomicilio" class="form-control"  type="text" value="${ciudadano.noDep} ${ciudadano.noPrv} ${ciudadano.noDis}" />                            <a id="btn-SelUbi" href="javascript:;"  title="Seleccionar ubigeo del domicilio" class="input-group-addon btn btn-default btnPequenoSGD" onclick="fn_seleccionarUbigeo();"><span class="glyphicon glyphicon-cog"></span> Seleccionar</a>                            </div>                        </td>                        <td>                                                     </td>                    </tr>                    <tr>                        <td>Dirección :</td>                        <td><input id="txtDeDomicil" type="text" style="width: 500px;" value="${ciudadano.deDomicil}" maxlength="100" onkeypress="return filtrarTeclado(event, false, public_apenom);"/></td>                                            </tr>                    <tr>                        <td>Email :</td>                        <td><input id="txtDeEmail" type="text"                                       style="width: 500px;" value="${ciudadano.deEmail}" maxlength="35" /></td>                                            </tr>                    <tr>                        <td>Teléfono :</td>                        <td><input id="txtDeTelefo" type="text" style="width: 500px;" value="${ciudadano.deTelefo}" maxlength="35" onkeypress="return filtrarTeclado(event, false, public_apenom);"/></td>                                            </tr>                </tbody>            </table>            <input type="hidden" id="hNomDepCon" value="${ciudadano.noDep}"/>            <input type="hidden" id="hNomProPa" value="${ciudadano.noPrv}"/>            <input type="hidden" id="hNomDisDep" value="${ciudadano.noDis}"/>                        <input type="hidden" id="hCodDepCon" value="${ciudadano.ubDep}"/>            <input type="hidden" id="hCodProPa" value="${ciudadano.ubPrv}"/>            <input type="hidden" id="hCodDisDep" value="${ciudadano.ubDis}"/>                        <input type="hidden" id="hDeDomicil" value="${ciudadano.deDomicil}"/>            <input type="hidden" id="hDeEmail" value="${ciudadano.deEmail}"/>            <input type="hidden" id="hDeTelefo" value="${ciudadano.deTelefo}"/>        </fieldset></jsp:root>