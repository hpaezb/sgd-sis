<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <div class="ui-panel ui-corner-all" style="">        <div class="ui-panel-titlebar ui-widget-header ui-corner-all">            <span>Registro de Proveedores</span>        </div>        <div class="ui-panel-body">            <fieldset>                <div class="btn-group btn-group-lg">                    <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarConfPersonal();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>                    <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="cerrarPantalla();" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>                        <fieldset>                <legend>Configuración Búsqueda</legend>                <table>                    <tbody>                        <tr>                            <td>NRO RUC:</td>                            <td><input type="text"/></td>                            <td><button id="btn-salir" title="Salir" class="btn btn-default btn-sm" onclick="" type="button"><span class="glyphicon glyphicon-search"></span> BUSCAR</button></td>                        </tr>                        <tr>                            <td>RAZON SOCIAL:</td>                            <td><input type="text"/></td>                            <td></td>                        </tr>                    </tbody>                </table>            </fieldset>            <fieldset>                <table>                    <tbody>                        <tr>                            <td></td>                            <td>Nombre</td>                            <td>Código</td>                        </tr>                        <tr>                            <td>Departamento / Continente</td>                            <td><input type="text"/></td>                            <td><input type="text"/></td>                        </tr>                        <tr>                            <td>Provincia / País</td>                            <td><input type="text"/></td>                            <td>                                <input type="text"/>                                <button>Mostrar</button>                                <button>Limpiar</button>                            </td>                        </tr>                        <tr>                            <td>Distrito / Departamento</td>                            <td><input type="text"/></td>                            <td><input type="text"/></td>                        </tr>                    </tbody>                </table>            </fieldset>            <fieldset>                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                        <table class="ui-state-default">                            <thead>                                <tr>                                    <th width="190px;">Departamento</th>                                    <th width="190px;">Provincia</th>                                    <th width="190px;">Distrito</th>                                    <th width="50px;">Ubdep</th>                                    <th width="50px;">Ubprv</th>                                    <th width="50px;">Ubdis</th>                                </tr>                            </thead>                        </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:200px;width: 902px;'>                        <table class="ui-datatable-edit-data" id="tblDestDetalle" cellpadding="0" cellspacing="0">                            <tbody>                                <tr>                                    <td>                                        <input name="txtCodGru" type="text" value="" readonly="readonly" style="width:200px;"/>                                    </td>                                    <td>                                        <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:200px;"/>                                    </td>                                    <td>                                        <input name="txtEmpleado" type="text" value="" readonly="readonly" style="width:200px;"/>                                    </td>                                    <td>                                        <input name="txtCodGru" type="text" value="" readonly="readonly" style="width:50px;"/>                                    </td>                                    <td>                                        <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:50px;"/>                                    </td>                                    <td>                                        <input name="txtEmpleado" type="text" value="" readonly="readonly" style="width:50px;"/>                                    </td>                                </tr>                            </tbody>                        </table>                    </div>                </div>            </fieldset>            <fieldset>                <div class="ui-datatable ui-datatable-scrollable">                    <div class="ui-datatable-scrollable-header">                        <table class="ui-state-default">                            <thead>                                <tr>                                    <th width="90px;">R.U.C.</th>                                    <th width="90px;">FECHA DE INSCR.</th>                                    <th width="290px;">RAZON SOCIAL</th>                                    <th width="190px;">DIRECCION</th>                                    <th width="90px;">DEPARTAMENTO</th>                                    <th width="90px;">PROVINCIA</th>                                    <th width="90px;">DISTRITO</th>                                    <th width="90px;">TELEFONO</th>                                </tr>                            </thead>                        </table>                    </div>                    <div class="bx_sb ui-datatable-scrollable-body" style='height:200px;width: 1200px;'>                        <table class="ui-datatable-edit-data" id="tblDestDetalle" cellpadding="0" cellspacing="0">                            <tbody>                                <c:forEach var="proveedor" items="${proveedoresList}" varStatus="status">                                    <tr>                                        <td>                                            <input readonly="readonly" id="txtRuc" type="text" style="width: 90px;" value="${proveedor.nuRuc}" />                                        </td>                                        <td>                                            <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:90px;"/>                                        </td>                                        <td>                                            <input readonly="readonly" id="txtRazonSocial" type="text" style="width: 290px;" value="${proveedor.descripcion}" />                                        </td>                                        <td>                                            <input readonly="readonly" id="txtDireccion" type="text" style="width: 190px;" value="${proveedor.cproDomicil}" />                                        </td>                                        <td>                                            <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:90px;"/>                                        </td>                                        <td>                                            <input name="txtEmpleado" type="text" value="" readonly="readonly" style="width:90px;"/>                                        </td>                                        <td>                                            <input name="txtDesDepen" type="text" value="" readonly="readonly" style="width:90px;"/>                                        </td>                                        <td>                                            <input name="txtEmpleado" type="text" value="" readonly="readonly" style="width:90px;"/>                                        </td>                                    </tr>                                </c:forEach>                            </tbody>                        </table>                    </div>                </div>            </fieldset>            <fieldset>                <legend>Detalle</legend>                <table width="1400px;">                    <tbody>                                                    <tr>                                <td>Ruc:</td>                                <td><input readonly="readonly" id="txtRuc" type="text" style="width: 600px;" value="${proveedor.nuRuc}" /></td>                            </tr>                            <tr>                                <td>Razon Social:</td>                                <td><input readonly="readonly" id="txtRazonSocial" type="text" style="width: 600px;" value="${proveedor.descripcion}" /></td>                            </tr>                            <tr>                                <td>Direccion:</td>                                <td><input readonly="readonly" id="txtDireccion" type="text" style="width: 600px;" value="${proveedor.cproDomicil}" /></td>                            </tr>                            <tr>                                <td>Ubigeo domicilio</td>                                <td></td>                            </tr>                            <tr>                                <td>Telefono:</td>                                <td><input id="txtTelefono" type="text" style="width: 600px;" value="${proveedor.cproTelefo}" /></td>                            </tr>                                            </tbody>                </table>                <br/>                <table>                    <tr>                        <td>Tipo de acceso:</td>                        <c:choose>                            <c:when test="${usuarioConf.tiAcceso=='1'}">                                <td><input type="radio" name="rbAcceso" value="0" />Total</td>                                <td><input type="radio" name="rbAcceso" value="1" checked="checked"/>Personal</td>                                </c:when>                                <c:otherwise>                                <td><input type="radio" name="rbAcceso" value="0" checked="checked"/>Total</td>                                <td><input type="radio" name="rbAcceso" value="1" />Personal</td>                                </c:otherwise>                            </c:choose>                    </tr>                    <tr>                        <td>Obliga a cargar el documento MP:</td>                        <c:choose>                            <c:when test="${usuarioConf.inCargaDocMesaPartes=='1'}">                                <td><input type="radio" name="rbCarDoc" value="0" />Si</td>                                <td><input type="radio" name="rbCarDoc" value="1" checked="checked"/>No</td>                                </c:when>                                <c:otherwise>                                <td><input type="radio" name="rbCarDoc" value="0" checked="checked"/>Si</td>                                <td><input type="radio" name="rbCarDoc" value="1" />No</td>                                </c:otherwise>                            </c:choose>                    </tr>                    <tr>                        <td>Obliga a firmar digitalmene:</td>                        <c:choose>                            <c:when test="${usuarioConf.inFirma=='1'}">                                <td><input type="radio" name="rbFirDoc" value="0" />Si</td>                                <td><input type="radio" name="rbFirDoc" value="1" checked="checked"/>No</td>                                </c:when>                                <c:otherwise>                                <td><input type="radio" name="rbFirDoc" value="0" checked="checked"/>Si</td>                                <td><input type="radio" name="rbFirDoc" value="1" />No</td>                                </c:otherwise>                            </c:choose>                    </tr>                    <tr>                        <td>Documento por defecto:</td>                        <c:choose>                            <c:when test="${usuarioConf.inTipoDoc=='1'}">                                <td><input type="radio" name="rbDocDef" value="0" />PDF</td>                                <td><input type="radio" name="rbDocDef" value="1" checked="checked"/>DOC</td>                                </c:when>                                <c:otherwise>                                <td><input type="radio" name="rbDocDef" value="0" checked="checked"/>PDF</td>                                <td><input type="radio" name="rbDocDef" value="1" />DOC</td>                                </c:otherwise>                            </c:choose>                    </tr>                </table>                <input type="hidden" id="codEmp" value="${usuarioConf.cempCodemp}"/>                <input type="hidden" id="codDep" value="${usuarioConf.coDep}"/>            </fieldset>        </div>    </div></jsp:root>