<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"><jsp:directive.page contentType="text/html;charset=UTF-8"/><jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divTupaMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro TUPA"/>        <tiles:putAttribute name="body">        <div id="divTupaBody">            <fieldset>                <div class="btn-group btn-group-lg">                    <button id="btn-nuevo" title="Nuevo TUPA" class="btn btn-primary btn-lg" onclick="fu_goNuevoTupa();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nuevo TUPA</button>                    <button id="btn-edit" title="Editar TUPA" class="btn btn-primary btn-lg" onclick="fu_goEditarTupa();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Editar TUPA</button>                    <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divTupaMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>            <fieldset>                <legend>Configuración Búsqueda</legend>                <table width="70%">                    <tbody>                        <!--                        <tr>                            <td>DEPENDENCIA:</td>                            <td><input type="text" id="txtTupaDependencia" maxlength="5" style="width:120px;" onkeyup="fn_buscarTupa(event, this);"/></td>                            <td><button id="btn-buscar" title="Buscar" class="btn btn-default btn-sm" onclick="fn_cargarBusqTupa();" type="button">                                    <span class="glyphicon glyphicon-search"></span> BUSCAR                                </button>                            </td>                        </tr>                        -->                        <tr>                            <td>NOMBRE :</td>                            <td><input type="text" id="txtTupaCorto" maxlength="20" class="anchoTotal" onkeyup="fn_buscarTupa(event, this);" /></td>                            <td><button id="btn-buscar" title="Buscar" class="btn btn-default btnPequenoSGD" onclick="fn_cargarBusqTupa();" type="button">                                    <span class="glyphicon glyphicon-search"></span> BUSCAR                                </button>                            </td>                        </tr>                        <tr>                            <td>DESCRIPCIÓN:</td>                            <td><input type="text" id="txtTupaDescripcion" maxlength="200" class="anchoTotal" onkeyup="fn_buscarTupa(event, this);" /></td>                            <td></td>                        </tr>                    </tbody>                </table>            </fieldset>            <fieldset>                <div id="divListaTupa">                    <jsp:directive.include file="registroTupaListado.jspx"/>                </div>            </fieldset>        </div>        <div id="divTupaEdit"><jsp:text/></div>            </tiles:putAttribute>        <tiles:putAttribute name="footer">           <jsp:text/>        </tiles:putAttribute>            </tiles:insertTemplate></jsp:root>