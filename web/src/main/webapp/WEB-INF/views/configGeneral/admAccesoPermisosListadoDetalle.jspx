<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <button id="btn-eliminar" title="Eliminar motivo" class="btn btn-default btn-xs" onclick="fn_eliminarMotDocDep();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <script>jsonDetGrupo = [];</script>    <div id="divConfigPersonal">                        <table id="tblDestDetalle">                            <tr>                                <td>Tipo de acceso:</td>                                <c:choose>                                    <c:when test="${usuarioConf.tiAcceso=='1'}">                                        <!--<td><input type="radio" name="rbAcceso" value="3" />Confidencial</td>-->                                        <td><input type="radio" name="rbAcceso" value="0" />Total</td>                                        <td><input type="radio" name="rbAcceso" value="1" checked="checked"/>Personal</td>                                    </c:when>                                    <!--<c:when test="${usuarioConf.tiAcceso=='3'}">                                        <td><input type="radio" name="rbAcceso" value="3" checked="checked" />Confidencial</td>                                        <td><input type="radio" name="rbAcceso" value="0" />Total</td>                                    