<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {fn_eventDepDetalle();});</script>                                    <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                            <th width="30px">Cod</th>                            <th width="200px">Descripci√≥n</th>                            <th width="150px">Empleado Encargado</th>                            <th width="80px">Cargo</th>                            <th width="30px">T.Enc</th>                            <th width="80px">Des. Corta</th>                            <th width="80px">Siglas</th>                            <th width="100px">Cargo Completo</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:300px;'>               <table class="ui-datatable-data" id="tblDepDet" style="width:100%;">            <tbody>                <c:forEach var="admDep" items="${dependenciaList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                <td width="30px">${admDep.coDependencia}</td>                                <td width="200px">${admDep.deDependencia}</td>                                <td width="150px">${admDep.deEmpleado}</td>                                <td width="80px">${admDep.deCargo}</td>                                <td width="30px">${admDep.deTipoEncargatura}</td>                                <td width="80px">${admDep.deCortaDepen}</td>                                <td width="80px">${admDep.deSigla}</td>                                <td width="100px">${admDep.deCargoCompleto}</td>                            </c:when>                            <c:otherwise>                                <td>${admDep.coDependencia}</td>                                <td>${admDep.deDependencia}</td>                                <td>${admDep.deEmpleado}</td>                                <td>${admDep.deCargo}</td>                                <td>${admDep.deTipoEncargatura}</td>                                <td>${admDep.deCortaDepen}</td>                                <td>${admDep.deSigla}</td>                                <td>${admDep.deCargoCompleto}</td>                            </c:otherwise>                        </c:choose>                    </tr>                </c:forEach>            </tbody>			            </table>        </div>    </div>    <input type="hidden" id="pcoDep" /></jsp:root>