<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">        jQuery(document).ready(function() {            $('[data-toggle="tooltip"]').tooltip();        });    </script>           <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarNuevoCargo();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>                <button id="btn-regresar" title="Regresar al listado de Cargo" class="btn btn-primary btn-lg" onclick="fn_cargarListaCargo();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divRequisitoMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>            <legend>Detalle</legend>            <table width="70%">                <tbody>                    <tr>                        <td>Descripci√≥n :</td>                        <td>                            <input id="txtDeDescripcion" type="text" class="anchoTotal" value="${cargo.descripcion}" maxlength="350" />                            <input type="hidden" id="hidN_codCargo" value="${cargo.codigo}" />                        </td>                    </tr>                                      <tr>                        <td>Estado :</td>                        <td>                            <div class="checkbox">                                <label>                                    <c:choose>                                        <c:when test="${cargo.estado eq '0'}">                                            <input type="checkbox" id="chbxEstado" /> Habilitar                                        </c:when>                                        <c:otherwise>                                            <input type="checkbox" id="chbxEstado" checked="true"/> Habilitar                                        </c:otherwise>                                    </c:choose>                                </label>                              </div>                        </td>                    </tr>                </tbody>            </table>        </fieldset></jsp:root>