<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <style>      #tblOtroOrigenDet  tbody {            display:block;             overflow:auto;         }     #tblOtroOrigenDet   thead, #tblOtroOrigenDet tbody tr {            display:table;            width:100%;            table-layout:fixed;        }      #tblOtroOrigenDet  thead {            width: calc( 100% - 1em )        }             </style>    <div class="ui-datatable ui-datatable-scrollable">                 <div class="bx_sb ui-datatable-scrollable-body"  >               <table class="ui-datatable-data" id="tblOtroOrigenDet" style="width:100%;">                 <thead class="ui-datatable-scrollable-header ui-state-default">                        <tr>                            <th  >N. Doc</th>                            <th  >Ap Paterno</th>                            <th  >Ap Materno</th>                            <th >Nombres</th>                            <th >Raz√≥n Social</th>                            <th  >Dep.</th>                            <th >Prov.</th>                            <th  >Dist.</th>                            <th >Direc.</th>                            <th  >Correo</th>                            <th  >Telf.</th>                        </tr>                  </thead>            <tbody style='height:500px;'>                <c:forEach var="origen" items="${otroOrigenList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                <td  ><input type="hidden" name="idOtroOri" value="${origen.coOtrOri}"/> ${origen.nuDocOtrOri}</td>                                <td >${origen.deApePatOtr}</td>                                <td >${origen.deApeMatOtr}</td>                                <td >${origen.deNomOtr}</td>                                <td >${origen.deRazSocOtr}</td>                                <td >${origen.noDep}</td>                                <td >${origen.noPro}</td>                                <td >${origen.noDis}</td>                                <td >${origen.deDirOtroOri}</td>                                <td >${origen.deEmail}</td>                                <td >${origen.deTelefo}</td>                            </c:when>                            <c:otherwise>                                <td  ><input type="hidden" name="idOtroOri" value="${origen.coOtrOri}"/> ${origen.nuDocOtrOri}</td>                                <td >${origen.deApePatOtr}</td>                                <td >${origen.deApeMatOtr}</td>                                <td >${origen.deNomOtr}</td>                                <td >${origen.deRazSocOtr}</td>                                <td >${origen.noDep}</td>                                <td  >${origen.noPro}</td>                                <td >${origen.noDis}</td>                                <td >${origen.deDirOtroOri}</td>                                <td >${origen.deEmail}</td>                                <td >${origen.deTelefo}</td>                            </c:otherwise>                        </c:choose>                    </tr>                </c:forEach>                <c:if test="${empty otroOrigenList}">                    <td>No se encuentran registros.</td>                </c:if>            </tbody>			            </table>        </div>        <c:if test="${not empty otroOrigenList}">            <div>Registros: ${otroOrigenList.size()}</div>        </c:if>          <c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>                    </c:if>     </div>    <input type="hidden" id="codOrigen" />    <script>        fn_eventOtrosOrigenes();    </script></jsp:root>