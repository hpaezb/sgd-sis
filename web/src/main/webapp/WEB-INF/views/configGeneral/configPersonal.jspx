<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Configuración Personal"/>        <tiles:putAttribute name="body">            <script>                $(document).ready(function() {                    $("#divConfigPersonal input:radio").prop("disabled", true);                    $("#lstTipoEnvioCorreo").val($("#inEmailDeriv").val());                });            </script>            <div id="divConfigPersonal">                                    <fieldset>                        <div class="btn-group btn-group-lg">                            <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarConfPersonal();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>                            <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                        </div>                    </fieldset>                    <fieldset>                        <legend>Detalle</legend>                        <table width="960px;">                            <tr>                                <td>Usuario:</td>                                <td><input readonly="readonly" id="txtNombreUsu" type="text" style="width: 600px;" value="${usuario.deApellidoPaterno} ${usuario.deApellidoMaterno} ${usuario.dePrenombres}" /></td>                            </tr>                            <tr>                                <td>Dependencia:</td>                                <td><input readonly="readonly" id="txtDependencia" type="text" style="width: 600px;" value="${usuarioConf.deDep}" /></td>                            </tr>                            <tr>                                <td>Local:</td>                                <td><input readonly="readonly" id="txtLocal" type="text" style="width: 600px;" value="${usuarioConf.deLocal}" /></td>                            </tr>                            <tr>                                <td>Directorio principal:</td>                                <!--<td><input id="txtDirPrincipal" readonly="readonly" type="text" style="width: 600px;" value="${usuarioConf.deDirEmi}"/><button title="Seleccionar carpeta" class="btn btn-primary btn-sm" onclick="fn_selecDirectorio();" type="button"><span class="glyphicon glyphicon-folder-open"></span> Seleccionar directorio</button>-->                                <td><input id="txtDirPrincipal" type="text" style="width: 600px;" value="${fn:replace(usuarioConf.deDirEmi, '|', '\\')}"/>                                </td>                            </tr>                            <tr>                                <td>Pie de página:</td>                                <td><input id="txtPiePag" type="text" style="width: 600px;" value="${usuarioConf.dePiePagina}" class="delUpperCase"/></td>                            </tr>                        </table>                        <br/>                        <table>                            <tr>                                <td>Tipo de acceso:</td>                                <c:choose>                                    <c:when test="${usuarioConf.tiAcceso=='1'}">                                        <td><input type="radio" name="rbAcceso" value="0" />Total</td>                                        <td><input type="radio" name="rbAcceso" value="1" checked="checked"/>Personal</td>                                        </c:when>                                        <c:otherwise>                                        <td><input type="radio" name="rbAcceso" value="0" checked="checked"/>Total</td>                                        <td><input type="radio" name="rbAcceso" value="1" />Personal</td>                                        </c:otherwise>                                    </c:choose>                            </tr>                            <tr>                                <td>Obliga a cargar el documento MP:</td>                                <c:choose>                                    <c:when test="${usuarioConf.inCargaDocMesaPartes=='1'}">                                        <td><input type="radio" name="rbCarDoc" value="0" />Si</td>                                        <td><input type="radio" name="rbCarDoc" value="1" checked="checked"/>No</td>                                        </c:when>                                        <c:otherwise>                                        <td><input type="radio" name="rbCarDoc" value="0" checked="checked"/>Si</td>                                        <td><input type="radio" name="rbCarDoc" value="1" />No</td>                                        </c:otherwise>                                    </c:choose>                            </tr>                            <tr>                                <td>Obliga a firmar digitalmene:</td>                                <c:choose>                                    <c:when test="${usuarioConf.inFirma=='1'}">                                        <td><input type="radio" name="rbFirDoc" value="0" />Si</td>                                        <td><input type="radio" name="rbFirDoc" value="1" checked="checked"/>No</td>                                        </c:when>                                        <c:otherwise>                                        <td><input type="radio" name="rbFirDoc" value="0" checked="checked"/>Si</td>                                        <td><input type="radio" name="rbFirDoc" value="1" />No</td>                                        </c:otherwise>                                    </c:choose>                            </tr>                            <tr>                                <td>Documento por defecto:</td>                                <c:choose>                                    <c:when test="${usuarioConf.inTipoDoc=='1'}">                                        <td><input type="radio" name="rbDocDef" value="0" />PDF</td>                                        <td><input type="radio" name="rbDocDef" value="1" checked="checked"/>DOC</td>                                        </c:when>                                        <c:otherwise>                                        <td><input type="radio" name="rbDocDef" value="0" checked="checked"/>PDF</td>                                        <td><input type="radio" name="rbDocDef" value="1" />DOC</td>                                        </c:otherwise>                                    </c:choose>                            </tr>                             <tr>                                <td>Notificación por Correo:</td>                                <td>                                 <select  style="width:190px;" id="lstTipoEnvioCorreo">                                   <c:forEach items="${lstTipoEnvioCorreo}" var="item">                                           <option value="${item.celeCodele}">                                                     ${item.celeDesele}                                             </option>                                   </c:forEach>                                 </select>                                </td>                            </tr>                        </table>                        <input type="hidden" id="codEmp" value="${usuarioConf.cempCodemp}"/>                        <input type="hidden" id="codDep" value="${usuarioConf.coDep}"/>                        <input type="hidden" id="inEmailDeriv" value="${usuarioConf.inEmailDeriv}"/>                    </fieldset>                            </div>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>