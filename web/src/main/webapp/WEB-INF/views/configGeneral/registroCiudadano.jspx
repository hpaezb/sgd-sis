<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"><jsp:directive.page contentType="text/html;charset=UTF-8"/><spring:eval expression="@applicationProps['resources.version']" var="resourceVersion"/><jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divCiudadanoMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <c:set var="resourceURL" value="resources-${resourceVersion}" scope="request" />        <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro de Ciudadano"/>        <tiles:putAttribute name="body">        <input type="hidden" id="codCiudadano" />            <div id="divCiudadanoBody">                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Nuevo ciudadano" class="btn btn-primary btn-lg" onclick="fu_goNuevoCiudadano();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nuevo ciudadano</button>                        <button id="btn-edit" title="Editar ciudadano" class="btn btn-primary btn-lg" onclick="fu_goEditarCiudadano();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Editar ciudadano</button>                        <button id="btn-salir" title="Salir" value = "Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divCiudadanoMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Configuración Búsqueda</legend>                    <table width="70%">                        <tbody>                            <tr>                                <td>DOCUMENTO:</td>                                <td><input type="text" id="txtCiudDocumento" class="anchoTotal"                                            onkeypress="return fu_onKeyPressFiltrarTecladoNumero(event, false, public_enteros,'','','');"                                           maxlength="8"/></td>                                <td><button id="btn-buscar" title="Buscar" class="btn btn-default btnPequenoSGD" type="button"><span class="glyphicon glyphicon-search"></span> BUSCAR</button></td>                            </tr>                            <tr>                                <td>APELLIDO PATERNO:</td>                                <td><input type="text" id="txtCiudApPaterno" maxlength="40" class="anchoTotal" /></td>                                <td></td>                            </tr>                            <tr>                                <td>APELLIDO MATERNO:</td>                                <td><input type="text" id="txtCiudApMaterno" maxlength="40" class="anchoTotal"  /></td>                                <td></td>                            </tr>                            <tr>                                <td>NOMBRES:</td>                                <td><input type="text" id="txtCiudNombres" maxlength="35" class="anchoTotal" /></td>                                <td></td>                            </tr>                        </tbody>                    </table>                </fieldset>                <fieldset style="margin: 5px 2px;">                    <div id="divListaCiudadano">                        <!--<jsp:directive.include file="registroCiudadanoListado.jspx"/>-->                        <div id="divGrdListCiudadano">                                                    </div>                    </div>                </fieldset>        </div>        <div id="divCiudadanoEdit"><jsp:text/></div>            </tiles:putAttribute>        <tiles:putAttribute name="footer">           <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate>           <script type="text/javascript">                  jQuery(document).ready(function() {                      loadJs("${resourceURL}/js/controller/Ciudadano.Controller.js");                });            </script></jsp:root>