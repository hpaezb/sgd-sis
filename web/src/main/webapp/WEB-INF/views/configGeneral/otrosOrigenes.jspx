<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"><jsp:directive.page contentType="text/html;charset=UTF-8"/><spring:eval expression="@applicationProps['resources.version']" var="resourceVersion"/><jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <c:set var="resourceURL" value="resources-${resourceVersion}" scope="request" />    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro de Otros Or&#237;genes"/>        <tiles:putAttribute name="body">        <input type="hidden" id="codOrigen" />            <div id="divOrigenBody" style="overflow: auto;">                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Nuevo origen" class="btn btn-primary btn-lg" onclick="fu_goNuevoOrigen();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nuevo origen</button>                        <button id="btn-edit" title="Editar origen" class="btn btn-primary btn-lg" onclick="fu_goEditarOrigen();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Editar origen</button>                        <button id="btn-salir" title="Salir" value = "Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Configuración Búsqueda</legend>                    <table>                        <tbody>                            <tr>                                <td>RAZON SOCIAL / NOMBRES :</td>                                <td><input type="text" id="txtBusRazonSocial" style="width: 315px;"  onkeypress="return fu_onKeyPressFiltrarTecladoCadena(event, false, public_letras)" maxlength="100"/></td>                                <td><button id="btn-buscar" title="Buscar" class="btn btn-default btnPequenoSGD" type="button"><span class="glyphicon glyphicon-search"></span> BUSCAR</button></td>                            </tr>                        </tbody>                    </table>                </fieldset>                <fieldset>                    <div id="divListaOtrosOrigenes">                        <!--<jsp:directive.include file="otrosOrigenesListado.jspx"/>-->                        <div id="divGrdResultOtroOrigen">                        </div>                    </div>                </fieldset>        </div>        <div id="divOrigenEdit"><jsp:text/></div>            </tiles:putAttribute>        <tiles:putAttribute name="footer">           <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate>         <script type="text/javascript">              jQuery(document).ready(function() {                  loadJs("${resourceURL}/js/controller/OtrosOrigenes.Controller.js");            });        </script></jsp:root>