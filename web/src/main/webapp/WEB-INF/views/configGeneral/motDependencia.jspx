<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Motivos por Dependencia"/>        <tiles:putAttribute name="body">            <div>                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Agregar nuevo motivo a un documento seleccionado" class="btn btn-primary btn-lg" onclick="fn_cargarNuevoMotDepen();" type="button"><span class="glyphicon glyphicon-file"></span> Agregar</button>                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Dependencia</legend>                    <table width="100%">                        <tr>                            <td>                                <select id="codDepencia" onchange="fn_cargarDocMotivosDependencia();" style="width: 100%;">                                    <c:forEach var="refDepen" items="${dependenciaList}" varStatus="status">                                        <c:choose>                                            <c:when test="${refDepen.coDependencia == buscarDocDependencia.codDepencia}">                                                <option value="${refDepen.coDependencia}" selected="selected">${refDepen.deDependencia}</option>                                            </c:when>                                            <c:otherwise>                                                <option value="${refDepen.coDependencia}">${refDepen.deDependencia}</option>                                            </c:otherwise>                                        </c:choose>                                    </c:forEach>                                </select>                                </td>                        </tr>                    </table>                </fieldset>                <fieldset>                    <legend>Documento</legend>                    <div id="divDetalleDocMotDependencia"><jsp:text/></div>                </fieldset>                <script>                    fn_cargarDocMotivosDependencia();                </script>            </div>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>