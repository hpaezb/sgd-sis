<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <select id="codDoc" onchange="fn_listaMotDependencia();">        <option value="-1">Seleccione un documento</option>        <c:forEach var="refDocDep" items="${docDependenciaList}" varStatus="status">            <option value="${refDocDep.coTipDoc}">${refDocDep.tiDescrip}</option>        </c:forEach>    </select>    <div id="divDetalleMotDependencia"><jsp:text/></div></jsp:root>