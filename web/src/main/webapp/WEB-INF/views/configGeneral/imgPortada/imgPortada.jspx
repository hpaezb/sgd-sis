<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divimgportada" scope="request"/>    <c:set var="posiciona" value="width:500px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Cambiar Imagen Portada"/>        <tiles:putAttribute name="body">            <fieldset>                <div class="btn-group btn-group-lg">                    <button title ="Cargar Imagen" class="btn btn-primary btn-lg" onclick="fn_cargarImgPortadaPrincipal();" type="button">                        <span class="glyphicon glyphicon-cloud-upload"><jsp:text/></span> Cargar Imagen</button>                                                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divimgportada');" type="button">                        <span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>                <div>                    <input id="imgPortada_fileuploadAlta" type="file" name="files[]" style="display: none;"/>                    <div id="imgPortada_progressAlta" class="progress" style="margin-top:-10px;position:absolute;width:400px;display: none;">                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">                            <span style="top: 3px; position: relative;"><jsp:text/></span>                        </div>                    </div>                </div>                </fieldset>            <fieldset>                <legend>Cargar Imagen</legend>                <table width="100%">                    <tr>                        <td>Archivo:</td>                        <td>                            <input id="imgPortada_name" type="text" style="width: 90%;" readonly="true" class="delUpperCase"/>                        </td>                    </tr>                </table>            </fieldset>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>