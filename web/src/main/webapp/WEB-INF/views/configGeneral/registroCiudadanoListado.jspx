<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <style>      #tblCiudadanoDet  tbody {            display:block;             overflow:auto;            overflow-x: hidden;        }      #tblCiudadanoDet  thead, #tblCiudadanoDet tbody tr {            display:table;            width:100%;            table-layout:fixed;        }       #tblCiudadanoDet thead {            width: calc( 100% - 1em )        }             </style>    <div class="ui-datatable ui-datatable-scrollable">                <div class="bx_sb ui-datatable-scrollable-body"  >               <table class="ui-datatable-edit-data " id="tblCiudadanoDet" style="width:100%;display:block;">                 <thead class="ui-datatable-scrollable-header ui-state-default">                        <tr>                            <th  >N. Doc</th>                            <th  >Ap. Paterno</th>                            <th  >Ap. Materno</th>                            <th  >Nombres</th>                            <th  >Dep.</th>                            <th  >Prov.</th>                            <th  >Dist.</th>                                                        <th  >Direc.</th>                                                               <th  >Telf.</th>                                 <th  >Correo</th>                           </tr>                  </thead>            <tbody style='height:500px;'>                <c:forEach var="ciudadano" items="${ciudadanoList}" varStatus="status">                    <c:choose>                        <c:when test="${status.count % 2 == 0}">                            <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                        </c:when>                        <c:otherwise>                            <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                        </c:otherwise>                    </c:choose>                    <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                <td  ><input type="hidden" name="hid_IdCiudadano" value="${ciudadano.nuLem}"/> ${ciudadano.nuLem}</td>                                <td  >${ciudadano.deApp}</td>                                <td  >${ciudadano.deApm}</td>                                <td  >${ciudadano.deNom}</td>                                <td  >${ciudadano.noDep}</td>                                <td  >${ciudadano.noPrv}</td>                                <td  >${ciudadano.noDis}</td>                                                                <td  >${ciudadano.deDomicil}</td>                                     <td  >${ciudadano.deTelefo}</td>                                     <td  >${ciudadano.deEmail}</td>                                 </c:when>                            <c:otherwise>                                <td  ><input type="hidden" name="hid_IdCiudadano" value="${ciudadano.nuLem}"/> ${ciudadano.nuLem}</td>                                <td >${ciudadano.deApp}</td>                                <td >${ciudadano.deApm}</td>                                <td >${ciudadano.deNom}</td>                                <td >${ciudadano.noDep}</td>                                <td >${ciudadano.noPrv}</td>                                <td >${ciudadano.noDis}</td>                                                                <td >${ciudadano.deDomicil}</td>                                     <td >${ciudadano.deTelefo}</td>                                     <td >${ciudadano.deEmail}</td>                             </c:otherwise>                        </c:choose>                    </tr>                </c:forEach>                <c:if test="${empty ciudadanoList}" >                    <td colspan="10">No se encuentran registros.</td>                </c:if>                                </tbody>			            </table>        </div>        <c:if test="${not empty ciudadanoList}">		<div>Registros: ${ciudadanoList.size()}</div>	</c:if>  	<c:if test="${not empty msjEmision}">		<div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>			<h4 style="margin-bottom:0px;">${msjEmision}</h4>		</div>            	</c:if>     </div>    <input type="hidden" id="codCiudadano" />    <script>        fn_eventCiudadano();    </script></jsp:root>