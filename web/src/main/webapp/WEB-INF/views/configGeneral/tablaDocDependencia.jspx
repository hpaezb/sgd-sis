<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <div id="divAddDocDependencia"><table width="100%">            <tr>                <td><a href="#" onclick="f_addDocDependencia();" class="mainField"><strong>Nuevo Documento</strong></a></td>            </tr>        </table></div>        <div class="ui-datatable ui-datatable-scrollable">            <div class="ui-datatable-scrollable-header">                   <table class="ui-state-default">                         <thead>                                <tr>                                    <th width="16">N°</th>                                    <th width="200">Descripción</th>                                    <th width="30">Firma<br>Digital</br></th>                                    <th width="40">Genera<br>Oficio</br></th>                                    <th width="57"/>                                </tr>                         </thead>                   </table>            </div>            <div class="bx_sb ui-datatable-scrollable-body" style='height:280px;'>                  <table class="ui-datatable-data" id="tbListaDocDependencia">                      <tbody>                           <c:forEach var="docDependencia" items="${docDependenciaList}" varStatus="status">                               <c:choose>                                   <c:when test="${status.count % 2 == 0}">                                     <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                                   </c:when>                                   <c:otherwise>                                     <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                                   </c:otherwise>                                 </c:choose>                               <tr class="${rowStyle}">                               <c:choose>                                   <c:when test="${status.count eq 1}">                                       <td width="16">${status.count}</td>                                       <td width="200">${docDependencia.tiDescrip}</td>                                       <c:choose>                                           <c:when test='${docDependencia.esOblFirma eq "1"}'>                                               <!--td width="30" align="center"><input type="checkbox" value="${docDependencia.esOblFirma}" checked="yes"/></td-->                                               <td width="30" align="center">SI</td>                                           </c:when>                                           <c:otherwise>                                               <!--td width="30" align="center"><input type="checkbox" value="${docDependencia.esOblFirma}"/></td-->                                               <td width="30" align="center">NO</td>                                           </c:otherwise>                                       </c:choose>                                       <c:choose>                                           <c:when test='${docDependencia.inGeneOfic eq "1"}'>                                               <!--td width="42" align="center"><input type="checkbox" value="${docDependencia.inGeneOfic}" checked="yes"/></td-->                                               <td width="42" align="center">SI</td>                                           </c:when>                                           <c:otherwise>                                               <!--td width="42" align="center"><input type="checkbox" value="${docDependencia.inGeneOfic}"/></td-->                                               <td width="42" align="center">NO</td>                                           </c:otherwise>                                       </c:choose>                                        <td align="center"><button title ="Editar" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="fu_editarDocXDependencia(${docDependencia.coTipDoc},'1')" type="button">                                        <span class="button.ui-button-icon-only ui-icon icon_edit"/>                                        </button></td>                                              <td align="center"><button title ="Eliminar" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="" type="button">                                        <span class="button.ui-button-icon-only ui-icon icon_delete"/>                                        </button></td>                                                                                </c:when>                                   <c:otherwise>                                       <td>${status.count}</td>                                       <td>${docDependencia.tiDescrip}</td>                                       <c:choose>                                           <c:when test='${docDependencia.esOblFirma eq "1"}'>                                               <!--td width="30" align="center"><input type="checkbox" value="${docDependencia.esOblFirma}" checked="yes"/></td-->                                               <td width="30" align="center">SI</td>                                           </c:when>                                           <c:otherwise>                                               <!--td width="30" align="center"><input type="checkbox" value="${docDependencia.esOblFirma}"/></td-->                                               <td width="30" align="center">NO</td>                                           </c:otherwise>                                       </c:choose>                                       <c:choose>                                           <c:when test='${docDependencia.inGeneOfic eq "1"}'>                                               <!--td width="42" align="center"><input type="checkbox" value="${docDependencia.inGeneOfic}" checked="yes"/></td-->                                               <td width="42" align="center">SI</td>                                           </c:when>                                           <c:otherwise>                                               <!--td width="42" align="center"><input type="checkbox" value="${docDependencia.inGeneOfic}"/></td-->                                               <td width="42" align="center">NO</td>                                           </c:otherwise>                                       </c:choose>                                        <td align="center"><button title ="Editar" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="alert(${docDependencia.coTipDoc})" type="button">                                        <span class="button.ui-button-icon-only ui-icon icon_edit"/>                                        </button></td>                                              <td align="center"><button title ="Eliminar" class="ui-button ui-state-default ui-corner-all button.ui-button-icon-only" onclick="" type="button">                                        <span class="button.ui-button-icon-only ui-icon icon_delete"/>                                        </button></td>                                                                                     </c:otherwise>                               </c:choose>                              </tr>                           </c:forEach>                      </tbody>                  </table>            </div>        </div>        <c:if test="${fn:length(docDependenciaList) gt 0}">            <jsp:directive.include file="../includes/pagination/barraPaginacion.jspx"/>        </c:if></jsp:root>