<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                           <th width="80px;">CÓDIGO</th>                           <!--<th width="60px;">TI_PROC</th>-->                           <!--<th width="80px;">CO_DEP_PRO</th>-->                           <th width="70px;">NOMBRE</th>                           <th width="370px;">DESCRIPCIÓN</th>                           <th width="60px;">PLAZO</th>                           <th width="60px;">ESTADO</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:500px;'>               <table class="ui-datatable-data" id="tblTupaDet" style="width:100%;">                <tbody>                    <c:forEach var="tupa" items="${tupaList}" varStatus="status">                        <c:choose>                            <c:when test="${status.count % 2 == 0}">                                <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                            </c:when>                            <c:otherwise>                                <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                            </c:otherwise>                        </c:choose>                        <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                    <td width="80px;"><input type="hidden" name="hid_CoProceso" value="${tupa.coProceso}"/> ${tupa.coProceso}</td>                                    <!--<td width="60px;">${tupa.tiProceso}</td>-->                                    <!--<td width="80px;">${tupa.coDepPro}</td>-->                                    <td width="70px;">${tupa.deNombre}</td>                                    <td width="370px;">${tupa.deDescripcion}</td>                                    <td width="60px;">${tupa.nuPlazo}</td>                                    <td width="60px;">                                        <c:choose>                                            <c:when test="${tupa.esEstado eq 1}">                                                <span class="label label-success">Habilitado</span>                                            </c:when>                                            <c:otherwise>                                                <span class="label label-danger">Deshabilitado</span>                                            </c:otherwise>                                        </c:choose>                                    </td>                            </c:when>                            <c:otherwise>                                    <td><input type="hidden" name="hid_CoProceso" value="${tupa.coProceso}"/> ${tupa.coProceso}</td>                                    <!--<td>${tupa.tiProceso}</td>-->                                    <!--<td>${tupa.coDepPro}</td>-->                                    <td>${tupa.deNombre}</td>                                    <td>${tupa.deDescripcion}</td>                                    <td>${tupa.nuPlazo}</td>                                    <td>                                        <c:choose>                                            <c:when test="${tupa.esEstado eq 1}">                                                <span class="label label-success">Habilitado</span>                                            </c:when>                                            <c:otherwise>                                                <span class="label label-danger">Deshabilitado</span>                                            </c:otherwise>                                        </c:choose>                                    </td>                            </c:otherwise>                        </c:choose>                      </tr>                    </c:forEach>                    <c:if test="${empty tupaList}">                        <td>No se encuentran registros.</td>                    </c:if>                </tbody>			            </table>        </div>        <c:if test="${not empty tupaList}">		<div>Registros: ${tupaList.size()}</div>	</c:if>  	<c:if test="${not empty msjEmision}">		<div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>			<h4 style="margin-bottom:0px;">${msjEmision}</h4>		</div>            	</c:if>     </div>    <input type="hidden" id="codTupa" />    <script>        fn_eventTupa();    </script></jsp:root>