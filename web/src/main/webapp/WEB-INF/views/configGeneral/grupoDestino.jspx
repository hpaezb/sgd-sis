<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Grupos Destino"/>        <tiles:putAttribute name="body">            <div>                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Crear nuevo grupo" class="btn btn-primary btn-lg" onclick="fn_cargarNuevoGrupoDestino();" type="button"><span class="glyphicon glyphicon-file"></span> Nuevo grupo</button>                        <button id="btn-guardar" title="Guardar los cambios" class="btn btn-primary btn-lg" onclick="fn_guardarGrupoDest();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>                        <button id="btn-cerrar" title="Regresar al M&#243;dulo Anterior" class="btn btn-primary btn-lg" onclick="fn_cancelarGuardarGD();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                        <button id="btn-eliminar" title="Eliminar grupo seleccionado" class="btn btn-primary btn-lg" onclick="fn_eliminarGrupo();" type="button"><span class="glyphicon glyphicon-remove"></span> Eliminar</button>                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Dependencia</legend>                    <table id="tablaDependencia" width="80%">                        <tr>                            <td>Dependencia</td>                            <td>:</td>                            <td><input type="hidden" id="codDependencia" value="${coDep}"/>                                <input readonly="readonly"  type="text" style="width: 100%; display: none;" class="form-control" id="txtDesDependenciaTemp" value="" />                                <div class="input-group" id="busDependenciaGD">                                    <input readonly="readonly"  type="text" class="form-control" id="txtDepEmiteBus" name="txtDepEmiteBus" size="50" value="${deDep}" />                                    <span  class="input-group-btn">                                        <button  class="btn btn-default btnPequenoSGD" type="button" onclick="fn_selecionarDependencia();"><span class="glyphicon glyphicon-search"><jsp:text/></span></button>                                    </span>                                </div>                            </td>                        </tr>                        <tr>                            <td>Seleccionar grupo</td>                            <td>:</td>                            <td><div id="divDetalleGrupoDestino"><jsp:text/></div></td>                        </tr>                    </table>                </fieldset>                <fieldset>                    <legend>Datos</legend>                    <div id="divListaGrupoDestinoDet"><jsp:text/></div>                </fieldset>                <script>                    fn_cargarGruposDestino();                    $("#tblNuevoGrupo").hide();                </script>            </div>          </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>