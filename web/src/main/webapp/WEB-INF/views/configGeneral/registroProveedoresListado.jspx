<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                           <th width="6%;">R.U.C.</th>                           <th width="17%;">Razon Social</th>                           <th width="5%;">Dep.</th>                           <th width="5%;">Prov.</th>                           <th width="5%;">Dist.</th>                           <th width="15%;">Direc.</th>                           <th width="8%;">Correo</th>                           <th width="8%;">Telf.</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:500px;'>               <table class="ui-datatable-data" id="tblProveedorDet" style="width:100%;">                <tbody>                    <c:forEach var="proveedor" items="${proveedoresList}" varStatus="status">                        <c:choose>                            <c:when test="${status.count % 2 == 0}">                                <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                            </c:when>                            <c:otherwise>                                <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                            </c:otherwise>                        </c:choose>                        <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                    <td width="8%;"><input type="hidden" name="hid_NuRuc" value="${proveedor.nuRuc}"/> ${proveedor.nuRuc}</td>                                    <td width="31%;">${proveedor.descripcion}</td>                                    <td width="8%;">${proveedor.noDep}</td>                                    <td width="8%;">${proveedor.noPrv}</td>                                    <td width="8%;">${proveedor.noDis}</td>                                    <td width="20%;">${proveedor.cproDomicil}</td>                                    <td width="8%;">${proveedor.cproEmail}</td>                                    <td width="8%;">${proveedor.cproTelefo}</td>                            </c:when>                            <c:otherwise>                                     <td width="8%;"><input type="hidden" name="hid_NuRuc" value="${proveedor.nuRuc}"/> ${proveedor.nuRuc}</td>                                    <td width="31%;">${proveedor.descripcion}</td>                                    <td width="8%;">${proveedor.noDep}</td>                                    <td width="8%;">${proveedor.noPrv}</td>                                    <td width="8%;">${proveedor.noDis}</td>                                    <td width="20%;">${proveedor.cproDomicil}</td>                                    <td width="8%;">${proveedor.cproEmail}</td>                                    <td width="8%;">${proveedor.cproTelefo}</td>                            </c:otherwise>                        </c:choose>                      </tr>                    </c:forEach>                    <c:if test="${empty proveedoresList}">                       <td>No se encuentran registros.</td>                    </c:if>                </tbody>			            </table>        </div>        <c:if test="${not empty proveedoresList}">            <div>Registros: ${proveedoresList.size()}</div>        </c:if>          <c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px;width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>                    </c:if>      </div>    <input type="hidden" id="codProveedor" />    <script>        fn_eventProveedores();    </script></jsp:root>