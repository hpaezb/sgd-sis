<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:700px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Acceso a Permisos"/>        <tiles:putAttribute name="body">            <div>                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-guardar" title="Guardar los cambios" class="btn btn-primary btn-lg" onclick="fn_guardarTipoPermisosUsuarios();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Empleado</legend>                    <table id="tablaEmpleado" width="90%">                        <tr>                            <td>Empleado</td>                            <td>:</td>                            <td><input type="hidden" id="codEmpleado" value="${coEmp}"/>                                <div class="input-group">                                    <input readonly="readonly" type="text" class="form-control" id="txtEmpleado" name="txtDepEmiteBus" size="50" value="${deEmp}" />                                    <span class="input-group-btn">                                        <button class="btn btn-default btn-xs-bus" type="button" onclick="fn_buscaEmpleadoTipoPermisos();"><span class="glyphicon glyphicon-search"><jsp:text/></span></button>                                    </span>                                </div>                            </td>                        </tr>                        <tr>                            <td>Seleccionar Dependencia</td>                            <td>:</td>                            <td><div id="divDetalleGrupoDestino" ><jsp:text/></div></td>                        </tr>                    </table>                </fieldset>                <fieldset>                    <legend>Datos</legend>                    <div id="divListaGrupoDestinoDet"><jsp:text/></div>                </fieldset>                <script>                    fn_cargarDependenciasEmpleado();                    $("#tblNuevoGrupo").hide();                </script>            </div>          </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>