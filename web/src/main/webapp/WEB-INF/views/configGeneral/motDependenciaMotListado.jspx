<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden;display: none;">        <button id="btn-eliminar" title="Eliminar motivo" class="btn btn-primary btn-xs" onclick="fn_eliminarMotDocDep();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default">                <thead>                    <tr>                        <th width="40">Código</th>                        <th width="350">Descripción</th>                        <th width="77">Opciones</th>                    </tr>                </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:450px;width: 515px;'>            <table class="ui-datatable-edit-data" id="tblMotDepen" cellpadding="0" cellspacing="0">                <tbody>                    <c:forEach var="refMot" items="${motList}" varStatus="status">                        <tr id="" >                            <td width="60">                                <input name="txtCodDoc" type="text" value="${refMot.coMot}" readonly="readonly" style="width:50px;"/>                            </td>                            <td width="370">                                <input name="txtNombreAnexo" type="text" value="${refMot.deMot}" readonly="readonly" style="width:350px;"/>                            </td>                            <td width="71">                                <div id="btn-opciones">                                    <button id="btn-Cambiar" title="Cambiar o reemplazar motivo" class="btn btn-default btn-xs" type="button" onclick="fn_cambiarMotDoc('${refMot.coMot}');" codMot="${refMot.coMot}"  ><span class="glyphicon glyphicon-list-alt"></span></button>                                </div>                            </td>                        </tr>                    </c:forEach>                </tbody>            </table>        </div>        <input type="hidden" id="codMot"/>        <input type="hidden" id="codMotReempl"/>    </div>    <script>fn_eventMotDependencia();</script></jsp:root>