<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="ConfiguraciÃ³n de tablas Maestras"/>        <tiles:putAttribute name="body">            <div>                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Nuevo" class="btn btn-primary btn-lg" onclick="fn_cargarNuevoDocDepen();" type="button"><span class="glyphicon glyphicon-file"></span> Nuevo</button>                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Dependencia</legend>                    <table width="100%">                        <tr>                            <td>                              <select onchange="fn_cargarDetalleTablaMaestra();" style="width:190px;" id="lsTablaMaestra">                                   <c:forEach items="${lsTablaMaestra}" var="item">                                           <option value="${item.celeDescor}">                                                     ${item.celeDesele}                                             </option>                                   </c:forEach>                                 </select>                               </td>                                                    </tr>                    </table>                </fieldset>                <fieldset>                    <legend>Documentos</legend>                    <div id="divDetalleTablaMaestra"><jsp:text/></div>                </fieldset>                <script>                    fn_cargarDetalleTablaMaestra();                </script>            </div>        </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>