<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divOrigenMain" scope="request"/>    <c:set var="posiciona" value="width:800px; height:750px;" scope="request"/>   <script>        fn_iniGrupoDesVar();    </script>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Grupos Destinos Externos"/>        <tiles:putAttribute name="body">            <div>                <fieldset>                    <div class="btn-group btn-group-lg">                        <button id="btn-nuevo" title="Crear nuevo grupo" class="btn btn-primary btn-lg" onclick="fn_cargarNuevoGrupoDestinoVar();" type="button"><span class="glyphicon glyphicon-file"></span> Nuevo grupo</button>                        <button id="btn-guardar" title="Guardar los cambios" class="btn btn-primary btn-lg" onclick="fn_guardarGrupoDestVar();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar</button>                        <button id="btn-cerrar" title="Regresar al M&#243;dulo Anterior" class="btn btn-primary btn-lg" onclick="fn_cancelarGuardarGDVar();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                        <button id="btn-eliminar" title="Eliminar grupo seleccionado" class="btn btn-primary btn-lg" onclick="fn_eliminarGrupoVar();" type="button"><span class="glyphicon glyphicon-remove"></span> Eliminar</button>                        <button id="btn-salir" title="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                    </div>                </fieldset>                <fieldset>                    <legend>Destinos</legend>                    <table id="tablaDependencia" width="80%">                        <tr>                            <td>Tipo Destinatario</td>                            <td>:</td>                            <td>                                <input type="hidden" id="coTipo" name="coTipo" value=""/>                                <input type="text" readonly="readonly" id="tipoDestinatario" name="tipoDestinatario" value="" style="display: none" />                                <select id="sTipoDestinatario"  name="sTipoDestinatario" style="width:160px;" onchange="fn_changeTipoDest(this.value);">                                    <c:forEach var="lTipoDestinatarioEmi" items="${lstTipoDestinatarioEmi}" varStatus="status">                                        <c:choose>                                            <c:when test='${lTipoDestinatarioEmi.coDestinatario!="01"}'>                                                <option value="${lTipoDestinatarioEmi.coDestinatario}">${lTipoDestinatarioEmi.deDestinatario}</option>                                             </c:when>                                        </c:choose>                                                                                        </c:forEach>                                                                                                </select>                            </td>                        </tr>                        <tr>                            <td>Seleccionar grupo</td>                            <td>:</td>                            <td><div id="divDetalleGrupoDestino"><jsp:text/></div></td>                        </tr>                    </table>                </fieldset>                <fieldset>                    <legend>Datos</legend>                    <div id="divListaGrupoDestinoDet"><jsp:text/></div>                </fieldset>                <script>                    fn_cargarGruposDestinoVar();                    $("#tblNuevoGrupo").hide();                </script>            </div>          </tiles:putAttribute>        <tiles:putAttribute name="footer">            <jsp:text/>        </tiles:putAttribute>    </tiles:insertTemplate></jsp:root>