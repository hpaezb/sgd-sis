<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">        jQuery(document).ready(function(){        $('[data-toggle="tooltip"]').tooltip();        fu_iniDepAdmUO();fu_iniTipoUO();    });</script>      <div class="ui-panel ui-corner-all"  style="width: 1180px; margin: 0px auto">        <div class="ui-panel-titlebar ui-widget-header ui-corner-all">            <span>Administración de Unidades Orgánicas</span>        </div>        <div class="ui-panel-body">                   <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarDependenciaAdm();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>                <button id="btn-regresar" title="Regresar al listado de unidades org&#225;nicas" class="btn btn-primary btn-lg" onclick="fn_cerrarEditDepAdm();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="cerrarPantallaConfigTabla();" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>        <legend>Detalle</legend>        <table width="100%">            <tr>                <td width="50%">                        <form:form modelAttribute="dependenciaBean">                        <input type="hidden" id="envDependenciaBean" name="envDependenciaBean" value="0"/>                        <table>                                <tr>                                    <td>Tipo:</td>                                    <td>                                        <input type="hidden" id="pTipoAnt" name="pTipoAnt" value="${dependenciaBean.tiDependencia}" />                                        <c:choose>                                            <c:when test="${dependenciaBean.tiDependencia eq null}">                                                <form:select path="tiDependencia" style="min-width:250px;" >                                                                                                        <option value="0" selected="true">Institución</option>                                                    <option value="1">Comité especial</option>                                                </form:select>                                            </c:when>                                            <c:otherwise>                                                <input type="text" style="min-width:250px;" value="${dependenciaBean.tiDependencia eq 0 ? 'Institución' : 'Comité especial'}" readonly="true" />                                                <form:hidden path="tiDependencia"></form:hidden>                                            </c:otherwise>                                        </c:choose>                                    </td>                                </tr>                                <tr id="rowCoDependencia" style="display:${divstatus};"  >                                    <td>Código:</td>                                                                            <c:choose>                                            <c:when test="${dependenciaBean.coDependencia eq null}">                                                <td>                                                    <form:input path="coDependencia" maxlength="5" style="min-width:250px;" onkeypress="return fu_onKeyPressFiltrarTecladoNumero(event, false, public_enteros, '', '', '');" />                                                                                                    <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                                                </td>                                                <td>                                                </td>                                            </c:when>                                            <c:otherwise>                                                <td>                                                    <form:input path="coDependencia" maxlength="5" style="min-width:250px;" readonly="true" />                                                </td>                                                <td>                                                </td>                                            </c:otherwise>                                        </c:choose>                                                                    </tr>                                <tr>                                    <td>Descripción:</td>                                    <td>                                        <form:input path="deDependencia" maxlength="200" class="anchoTotal"/>                                    </td>                                    <td>                                                                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                                    </td>                                </tr>                                                    <tr>                                    <td>Titulo Dep.:</td>                                    <td>                                        <form:input path="tituloDep" maxlength="200" size="70" class="delUpperCase"/>                                    </td>                                </tr>                                                    <tr>                                    <td>Dep. Padre:</td>                                    <td>                                        <form:input path="coDepenPadre" type="hidden"/>                                        <div class="input-group">                                          <form:input path="deDepenPadre" class="form-control" size="50" readonly="true"/>                                          <span class="input-group-btn">                                            <button class="btn btn-default btn-xs-bus" type="button" onclick="fn_buscaDependenciaPadreUOAdm();"><span class="glyphicon glyphicon-search"><jsp:text/></span></button>                                          </span>                                        </div>                                                                                </td>                                </tr>                                                    <tr>                                    <td>Encargado:</td>                                    <td>                                        <form:input path="coEmpleado" type="hidden"/>                                        <div class="input-group">                                          <form:input path="deEmpleado" class="form-control" size="50" readonly="true"/>                                          <span class="input-group-btn">                                            <button class="btn btn-default btn-xs-bus" type="button" onclick="fn_buscaEmpleadoEncargadoAdm();"><span class="glyphicon glyphicon-search"><jsp:text/></span></button>                                          </span>                                        </div>                                                                                </td>                                </tr>                                <tr>                                    <td>Cargo:</td>                                    <td>                                        <form:input path="coCargo" type="hidden"/>                                        <div class="input-group">                                          <form:input path="deCargo" class="form-control" size="50" readonly="true"/>                                          <span class="input-group-btn">                                            <button class="btn btn-default btn-xs-bus" type="button" onclick="fn_buscaCargoEmpleadoAdm('coCargo','deCargo');"><span class="glyphicon glyphicon-search"><jsp:text/></span></button>                                          </span>                                        </div>                                                                                </td>                                    <td>                                        <form:select path="coTipoEncargatura">                                            <form:options items="${lsTipoEncargatura}" itemValue="celeCodele" itemLabel="celeDesele" />                                        </form:select>                                                                </td>                                </tr>                                <tr>                                    <td>Cargo Desc:</td>                                    <td>                                        <form:input path="deCargoCompleto" maxlength="200" size="70" class="delUpperCase"/>                                    </td>                                </tr>                                <tr>                                    <td>Nivel:</td>                            <td><form:input path="coNivel" maxlength="1" style="min-width:250px;" /><a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a></td>                                </tr>                                                    <tr>                                    <td>Sigla:</td>                            <td><form:input path="deSigla" maxlength="50" style="min-width:250px;" /><a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a></td>                                </tr>                                <tr>                                    <td>Sigla Corta:</td>                            <td><form:input path="deCortaDepen" maxlength="50" style="min-width:250px;"  /><a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a></td>                                </tr>                                                    <tr>                                    <td>Local:</td>                                    <td><select id="coLocal" name="coLocal" style="min-width:250px;">                                            <c:forEach var="lLocal" items="${lsLocal}" varStatus="status">                                                <c:choose>                                                    <c:when test="${lLocal.coLocal eq coLocal}">                                                        <option selected="selected" value="${lLocal.coLocal}">${lLocal.deLocal}</option>                                                     </c:when>                                                    <c:otherwise>                                                        <option value="${lLocal.coLocal}">${lLocal.deLocal}</option>                                                    </c:otherwise>                                                </c:choose>                                                                                  </c:forEach>                                        </select>                                    </td>                                </tr>                                                                   <tr>                                    <td colspan="2">Observación / Descripción:</td>                                                                    </tr>                                   <tr>                                    <td colspan="2"><textarea id="coObservacion" name="coObservacion" class="input-full-row" style="text-transform:lowercase;" maxlength="500" rows="3" >${dependenciaBean.coObservacion}</textarea></td>                                                                    </tr>                                 <form:hidden path="inBaja"/>                        </table>                    </form:form>                </td>                  <c:choose>                    <c:when test="${dependenciaBean.coDependencia eq null}">                        <td id= "tblIntegrantes" valign="top" width="50%" style="display: none;">                    <fieldset>                        <legend>Integrantes</legend>                        <div id="divTablaIntegrantesDepenUO">                            <input id="txtIndexFilaIntegDepUo" name="txtIndexFilaIntegDepUo" type="hidden" value="-1"/>                            <jsp:directive.include file="tblIntegDepenUo.jspx"/>                        </div>                                            </fieldset>                                        </td>                    </c:when>                    <c:otherwise>                        <c:choose>                            <c:when test="${dependenciaBean.tiDependencia eq 1}">                                <td id= "tblIntegrantes" valign="top" width="50%">                                    <fieldset>                                        <legend>Integrantes</legend>                                        <div id="divTablaIntegrantesDepenUO">                                            <input id="txtIndexFilaIntegDepUo" name="txtIndexFilaIntegDepUo" type="hidden" value="-1"/>                                            <jsp:directive.include file="tblIntegDepenUo.jspx"/>                                        </div>                                                            </fieldset>                                                        </td>                            </c:when>                        </c:choose>                    </c:otherwise>                </c:choose>            </tr>        </table>        </fieldset>                                 <c:choose>                              <c:when test="${dependenciaBean.creaUsuario!=null || dependenciaBean.actUsuario!=null ||                                      dependenciaBean.creaFecha!=null || dependenciaBean.actFecha!=null}">                                <fieldset>                                  <legend>Auditorìa</legend>                                   <table style="width: 100%">                                    <tr id="user_form">                                        <td style="width: 30%">Usuario Creación</td>                                        <td style="width: 70%"><input type="text" class="input-full-row" value="${dependenciaBean.creaUsuario}" readonly="true" disabled="true" /></td>                                    </tr>                                       <tr id="user_form">                                        <td style="width: 30%">Fecha Creación</td>                                        <td style="width: 70%"><input type="text" class="input-full-row" value="${dependenciaBean.creaFecha}" readonly="true" disabled="true" /></td>                                    </tr>                                       <tr id="user_form">                                        <td style="width: 30%">Usuario Modificación</td>                                        <td style="width: 70%"><input type="text" class="input-full-row" value="${dependenciaBean.actUsuario}" readonly="true" disabled="true" /></td>                                    </tr>                                       <tr id="user_form">                                        <td style="width: 30%">Fecha Modificación</td>                                        <td style="width: 70%"><input type="text" class="input-full-row" value="${dependenciaBean.actFecha}" readonly="true" disabled="true" /></td>                                    </tr>                                  </table>                             </fieldset>                                                                  </c:when>                                                                    </c:choose>                                                                                                      </div>    </div>                            </jsp:root>