<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form"><jsp:directive.page contentType="text/html;charset=UTF-8"/><jsp:output omit-xml-declaration="yes"/>    <c:set var="divId" value="divRequisitoMain" scope="request"/>    <c:set var="posiciona" value="width:1000px; height:750px;" scope="request"/>    <tiles:insertTemplate template="/WEB-INF/templates/windowmodaldraggable.jspx" flush="true">        <tiles:putAttribute name="title" value="Registro Requisito"/>        <tiles:putAttribute name="body">        <div id="divRequisitoBody">            <fieldset>                <div class="btn-group btn-group-lg">                    <button id="btn-nuevo" title="Nuevo Requisito" class="btn btn-primary btn-lg" onclick="fu_goNuevoRequisito();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Nuevo Requisito</button>                    <button id="btn-edit" title="Editar Requisito" class="btn btn-primary btn-lg" onclick="fu_goEditarRequisito();" type="button"><span class="glyphicon glyphicon-pencil"></span> Editar Requisito</button>                    <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divRequisitoMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>                </div>            </fieldset>            <fieldset>                <legend>Configuración Búsqueda</legend>                <table width="70%">                    <tbody>                        <tr>                            <td>DESCRIPCIÓN:</td>                            <td><input type="text" id="txtRequisitoDescripcion" maxlength="200" class="anchoTotal" onkeyup="fn_buscarRequisito(event, this);" /></td>                            <td>                                <button id="btn-buscar" title="Buscar" class="btn btn-default btnPequenoSGD" onclick="fn_cargarBusqRequisito();" type="button">                                    <span class="glyphicon glyphicon-search"></span> BUSCAR                                </button>                            </td>                        </tr>                    </tbody>                </table>            </fieldset>            <fieldset>                <div id="divListaRequisito">                    <jsp:directive.include file="registroRequisitoListado.jspx"/>                </div>            </fieldset>        </div>        <div id="divRequisitoEdit"><jsp:text/></div>            </tiles:putAttribute>        <tiles:putAttribute name="footer">           <jsp:text/>        </tiles:putAttribute>            </tiles:insertTemplate></jsp:root>