<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">        jQuery(document).ready(function(){            $('[data-toggle="tooltip"]').tooltip();               });                $("#txtPaterno").change(function(){sumRazonSocial()});        $("#txtMaterno").change(function(){sumRazonSocial()});        $("#txtNombres").change(function(){sumRazonSocial()});                function sumRazonSocial()        {          $("#txtRazonSocial").val($("#txtPaterno").val().trim()+ ' '+$("#txtMaterno").val().trim()+' '+ $("#txtNombres" ).val().trim());                       $("#txtRazonSocial").val($("#txtRazonSocial").val().trim());        }    </script>          <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarOtroOrigen();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar Cambios</button>                <button id="btn-regresar" title="Regresar al listado de otros or&#237;genes" class="btn btn-primary btn-lg" onclick="fn_cerrarOtrosOrigenes();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divOrigenMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>            <legend>Detalle</legend>            <table width="70%">                <tbody >                    <tr>                        <td>Tipo Documento</td>                        <td>                            <input type="hidden" id="idOtroOri" value="${origenEditar.coOtrOri}" />                            <select id="cbTipoDoc" class="anchoTotal">                                <option value=""><jsp:text/></option>                                <c:forEach var="tipoDoc" items="${tipoDocList}" varStatus="status">                                    <c:choose>                                        <c:when test="${ origenEditar.coTipOtrOri== tipoDoc.celeCodele }">pizza.                                            <option selected="selected" value="${tipoDoc.celeCodele}">${tipoDoc.celeDesele}</option>                                        </c:when>                                        <c:otherwise>                                            <option value="${tipoDoc.celeCodele}">${tipoDoc.celeDesele}</option>                                        </c:otherwise>                                    </c:choose>                                </c:forEach>                            </select>                        </td>                        <td></td>                    </tr>                    <tr>                        <td>Número:</td>                        <td><input id="txtNumeroDoc" type="text" class="anchoTotal" value="${origenEditar.nuDocOtrOri}" maxlength="10" /></td>                        <td></td>                    </tr>                    <tr>                        <td>Apellido Paterno</td>                        <td><input id="txtPaterno" type="text" class="anchoTotal" value="${origenEditar.deApePatOtr}" maxlength="50" /></td>                        <td></td>                    </tr>                    <tr>                        <td>Apellido Materno</td>                        <td><input id="txtMaterno" type="text" class="anchoTotal" value="${origenEditar.deApeMatOtr}" maxlength="50" /></td>                        <td></td>                    </tr>                    <tr>                        <td>Nombres</td>                        <td><input id="txtNombres" type="text" class="anchoTotal" value="${origenEditar.deNomOtr}" maxlength="100" /></td>                        <td></td>                    </tr>                    <tr>                        <td>Razón Social / Nombre Completo</td>                        <td><input id="txtRazonSocial" type="text" class="anchoTotal" value="${origenEditar.deRazSocOtr}" maxlength="100"/></td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio anchoTotal" title="Campo Obligatorio">(*)</a>                        </td>                    </tr>                                        <tr>                        <td>Ubigeo Domicilio</td>                        <td>                            <div class="input-group anchoTotal">                                <input readonly="readonly" class="form-control" id="txtUbiDomicilio" type="text" value="${origenEditar.noDep} ${origenEditar.noPro} ${origenEditar.noDis}" />                                <a href="javascript:;" id="btn-SelUbi" title="Seleccionar ubigeo del domicilio" class="input-group-addon btn-default btnPequenoSGD" onclick="fn_seleccionarUbigeo();" ><span class="glyphicon glyphicon-cog"></span> Seleccionar</a>                            </div>                                                    </td>                        <td></td>                    </tr>                    <tr>                        <td>Dirección</td>                        <td><input id="txtDireccion" type="text" class="anchoTotal" value="${origenEditar.deDirOtroOri}" maxlength="100"/></td>                        <td></td>                    </tr>                    <tr>                        <td>Email</td>                        <td><input id="txtEmail" type="text" class="anchoTotal" value="${origenEditar.deEmail}" maxlength="100"/></td>                        <td></td>                    </tr>                    <tr>                        <td>Teléfono</td>                        <td><input id="txtTelefo" type="text" class="anchoTotal" value="${origenEditar.deTelefo}" maxlength="30"/></td>                        <td></td>                    </tr>                </tbody>            </table>            <input type="hidden" id="hNomDepCon" value="${origenEditar.noDep}"/>            <input type="hidden" id="hNomProPa" value="${origenEditar.noPro}"/>            <input type="hidden" id="hNomDisDep" value="${origenEditar.noDis}"/>            <input type="hidden" id="hCodDepCon" value="${origenEditar.ubDep}"/>            <input type="hidden" id="hCodProPa" value="${origenEditar.ubPro}"/>            <input type="hidden" id="hCodDisDep" value="${origenEditar.ubDis}"/>        </fieldset></jsp:root>