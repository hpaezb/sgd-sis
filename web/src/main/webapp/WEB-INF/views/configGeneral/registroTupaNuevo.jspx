<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">jQuery(document).ready(function() {            $('[data-toggle="tooltip"]').tooltip();        });</script>           <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarNuevoTupa();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>                <button id="btn-regresar" title="Regresar al listado de Tupa" class="btn btn-primary btn-lg" onclick="fn_cargarListaTupa();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divTupaMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>            <legend>Detalle</legend>            <table width="70%">                <tbody>                    <!--                    <tr>                        <td>Codigo Tupa:</td>                        <td><input id="txtCoProceso" type="text" style="width: 120px;" value="${tupa.coProceso}" maxlength="4" /></td>                    </tr>                    <tr>                        <td>Tipo de Proceso:</td>                        <td><input id="txtTiProceso" type="text" style="width: 120px;" value="${tupa.tiProceso}" maxlength="1"/><span style="color: red;" >(*obligatorio)</span></td>                    </tr>                    <tr>                        <td>Codigo Dependencia:</td>                        <td><input id="txtCoDepPro" type="text" style="width: 120px;" value="${tupa.coDepPro}" maxlength="5"/></td>                    </tr>                    <tr>                        <td>Estado (1/0):</td>                        <td><input id="txtEsEstado" type="text" style="width: 120px;" value="${tupa.esEstado}" maxlength="1" /></td>                    </tr>                    -->                    <tr>                        <td>Nombre :</td>                        <td>                            <input id="txtDeNombre" type="text" class="anchoTotal" value="${tupa.deNombre}" maxlength="20" />                            <input type="hidden" id="hidN_coProceso" value="${tupa.coProceso}" />                        </td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                        </td>                    </tr>                    <tr>                        <td>Descripción :</td>                        <td><input id="txtDeDescripcion" type="text" class="anchoTotal" value="${tupa.deDescripcion}" maxlength="350" /></td>                    </tr>                    <tr>                        <td>Plazo atención en días :</td>                        <td><input id="txtNuPlazo" type="text" class="anchoTotal" value="${tupa.nuPlazo}" maxlength="3" />                         </td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                        </td>                                            </tr>                                        <tr>                        <td>Estado :</td>                        <td>                            <div class="checkbox">                                <label>                                    <c:choose>                                        <c:when test="${tupa.esEstado eq '0'}">                                            <input type="checkbox" id="chbxEstado" /> Habilitar                                        </c:when>                                        <c:otherwise>                                            <input type="checkbox" id="chbxEstado" checked="true"/> Habilitar                                        </c:otherwise>                                    </c:choose>                                </label>                              </div>                        </td>                    </tr>                </tbody>            </table>        </fieldset>            <fieldset id="fielDetalleTupaRequisitos">            <legend>Requisitos</legend>            <div id="divDetalleTupaRequisitos"><jsp:text/></div>        </fieldset>    </jsp:root>