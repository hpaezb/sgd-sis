<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default" style="width:100%;">                  <thead>                        <tr>                           <th width="80px;">C&#243;DIGO</th>                           <th width="370px;">DESCRIPCI&#243;N</th>                           <th width="60px;">ESTADO</th>                        </tr>                  </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:500px;'>               <table class="ui-datatable-data" id="tblCargoDet" style="width:100%;">                <tbody>                    <c:forEach var="cargo" items="${cargoList}" varStatus="status">                        <c:choose>                            <c:when test="${status.count % 2 == 0}">                                <c:set var="rowStyle" scope="page" value="ui-datatable-even"/>                            </c:when>                            <c:otherwise>                                <c:set var="rowStyle" scope="page" value="ui-datatable-odd"/>                            </c:otherwise>                        </c:choose>                        <tr class="${rowStyle}">                        <c:choose>                            <c:when test="${status.count eq 1}">                                    <td width="80px;"><input type="hidden" name="hid_CoCargo" value="${cargo.codigo}"/> ${cargo.codigo}</td>                                    <td width="370px;"><c:out value="${cargo.descripcion}"></c:out></td>                                    <td width="60px;">                                        <c:choose>                                            <c:when test="${cargo.estado eq 1}">                                                <span class="label label-success">Habilitado</span>                                            </c:when>                                            <c:otherwise>                                                <span class="label label-danger">Deshabilitado</span>                                            </c:otherwise>                                        </c:choose>                                    </td>                            </c:when>                            <c:otherwise>                                    <td><input type="hidden" name="hid_CoCargo" value="${cargo.codigo}"/> ${cargo.codigo}</td>                                    <td><c:out value="${cargo.descripcion}"></c:out></td>                                    <td>                                        <c:choose>                                            <c:when test="${cargo.estado eq 1}">                                                <span class="label label-success">Habilitado</span>                                            </c:when>                                            <c:otherwise>                                                <span class="label label-danger">Deshabilitado</span>                                            </c:otherwise>                                        </c:choose>                                    </td>                            </c:otherwise>                        </c:choose>                      </tr>                    </c:forEach>                    <c:if test="${empty cargoList}">                        <td>No se encuentran registros.</td>                    </c:if>                </tbody>			            </table>        </div>        <c:if test="${not empty cargoList}">            <div>Registros: ${cargoList.size()}</div>	</c:if>  	<c:if test="${not empty msjEmision}">            <div class="alert alert-warning alert-dismissable" style="margin-bottom:0px; width:50%">                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>                <h4 style="margin-bottom:0px;">${msjEmision}</h4>            </div>            	</c:if>     </div>    <input type="hidden" id="codCargo" />    <script>        fn_eventCargo();    </script></jsp:root>