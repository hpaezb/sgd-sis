<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core"          xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags"          xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <script languaje="javascript">        jQuery(document).ready(function(){            $('[data-toggle="tooltip"]').tooltip();        });    </script>          <fieldset>            <div class="btn-group btn-group-lg">                <button id="btn-grabar" title="Guardar cambios" class="btn btn-primary btn-lg" onclick="fn_grabarNueProveedor();" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Guardar cambios</button>            <button id="btn-regresar" title="Regresar al Listado de Personas Jur&#237;dicas" class="btn btn-primary btn-lg" onclick="fn_cargarListaProveedores();" type="button"><span class="glyphicon glyphicon-arrow-left"></span> Cerrar</button>                <button id="btn-salir" title="Salir" value="Salir" class="btn btn-primary btn-lg" onclick="removeDomId('divProveedorMain');" type="button"><span class="glyphicon glyphicon-log-out"></span> Salir</button>            </div>        </fieldset>        <fieldset>            <legend>Detalle</legend>            <table width="70%">                <tbody>                    <tr>                        <td>Ruc:</td>                        <td>                            <input type="hidden" id="hidN_Ruc" value="${proveedor.nuRuc}" />                        <c:choose>                            <c:when test="${proveedor==null}">                                <input id="txtRuc" class="anchoTotal" type="text"  value="${proveedor.nuRuc}" maxlength="11" onkeypress="return fu_onKeyPressFiltrarTecladoNumero(event, false, public_enteros, '', '', '');"/>                            </c:when>                            <c:otherwise>                                <input id="txtRuc" class="anchoTotal" type="text"  value="${proveedor.nuRuc}" maxlength="11" readonly="true" onkeypress="return fu_onKeyPressFiltrarTecladoNumero(event, false, public_enteros, '', '', '');"/>                            </c:otherwise>                        </c:choose>                        <!--<input id="txtRuc" type="text" style="width: 100px;" value="${proveedor.nuRuc}" maxlength="11" />-->                        </td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                        </td>                    </tr>                    <tr>                        <td>Razon Social:</td>                        <td><input id="txtRazonSocial" class="anchoTotal" type="text"  value="${proveedor.descripcion}" maxlength="100"/></td>                        <td>                            <a href="#" data-toggle="tooltip" class="tooltipObligatorio" title="Campo Obligatorio">(*)</a>                        </td>                    </tr>                                                           <tr>                        <td>Ubigeo domicilio</td>                        <td>                            <div class="input-group anchoTotal">                                <input readonly="readonly" id="txtUbiDomicilio" type="text" class="form-control"  value="${proveedor.noDep} ${proveedor.noPrv} ${proveedor.noDis}" />                                <a href="javascript:;" id="btn-SelUbi" title="Seleccionar ubigeo del domicilio" class="input-group-addon btn-default btnPequenoSGD" onclick="fn_seleccionarUbigeo();"><span class="glyphicon glyphicon-cog"></span> Seleccionar</a>                            </div>                         </td>                    </tr>                     <tr>                        <td>Direccion:</td>                        <td><input id="txtDireccion" class="anchoTotal" type="text"  value="${proveedor.cproDomicil}" maxlength="100"/></td>                    </tr>                    <tr>                        <td>Email:</td>                        <td><input id="txtEmail" class="anchoTotal"  type="text"  value="${proveedor.cproEmail}" maxlength="100" /></td>                    </tr>                    <tr>                        <td>Tel√©fono:</td>                        <td><input id="txtTelefono" class="anchoTotal"  type="text"  value="${proveedor.cproTelefo}" maxlength="30" /></td>                    </tr>                </tbody>            </table>            <!--Se comenta por tema de Backup            <input type="hidden" id="hNomDepCon" value=""/>            <input type="hidden" id="hNomProPa" value=""/>            <input type="hidden" id="hNomDisDep" value=""/>            <input type="hidden" id="hCodDepCon" value=""/>            <input type="hidden" id="hCodProPa" value=""/>            <input type="hidden" id="hCodDisDep" value=""/>            -->            <input type="hidden" id="hNomDepCon" value="${proveedor.noDep}"/>            <input type="hidden" id="hNomProPa" value="${proveedor.noPrv}"/>            <input type="hidden" id="hNomDisDep" value="${proveedor.noDis}"/>            <input type="hidden" id="hCodDepCon" value="${proveedor.cubiCoddep}"/>            <input type="hidden" id="hCodProPa" value="${proveedor.cubiCodpro}"/>            <input type="hidden" id="hCodDisDep" value="${proveedor.cubiCoddis}"/>                    </fieldset></jsp:root>