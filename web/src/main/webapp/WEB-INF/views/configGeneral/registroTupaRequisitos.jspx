<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>    <spring:eval expression="@applicationProps['application.version']" var="applicationVersion" scope="request"/>    <c:set var="rutactx" value="${pageContext.request.contextPath}/${applicationVersion}" scope="request" />    <div id="btn-defecto" style="visibility: hidden; display: none;">        <button id="btn-eliminar" title="Eliminar Requisito" class="btn btn-primary btn-xs" onclick="fn_eliminarTupaRequisito();" type="button"><span class="glyphicon glyphicon-trash"></span></button>    </div>    <div id="div_agregar_requisito" class="btn-group">        <button id="btn-agregar_requisito" title="Agregar Requisito" class="btn btn-primary" onclick="fn_adicionarTupaRequisito();" type="button"><span class="glyphicon glyphicon-plus"></span> Agregar Requisito</button>            <!--<button id="btn-agregar_requisito" title="Cambiar Estado Obligatorio" class="btn btn-primary" onclick="fn_cambiarEstadoObligatorio();" type="button"><span class="glyphicon glyphicon-pencil"></span> Cambiar Estado Obligatorio</button>-->    </div>    <div><span> </span></div>    <div><span> </span></div>    <div class="ui-datatable ui-datatable-scrollable">        <div class="ui-datatable-scrollable-header">            <table class="ui-state-default">                <thead>                    <tr>                        <th width="40">Código</th>                        <th width="350">Descripción</th>                        <th width="70">Obligatorio</th>                        <th width="45">Opciones</th>                                            </tr>                </thead>            </table>        </div>        <div class="bx_sb ui-datatable-scrollable-body" style='height:450px;width: 585px;'>            <table class="ui-datatable-edit-data" id="tblTupaRequisito" cellpadding="0" cellspacing="0">                <tbody>                    <c:forEach var="requisito" items="${requisitoList}" varStatus="status">                        <tr id="" >                            <td width="60">                                <input name="txtCodDoc" type="text" value="${requisito.codRequisito}" readonly="readonly" style="width:50px;"/>                            </td>                            <td width="370">                                <input name="txtNombreAnexo" type="text" value="${requisito.descripcion}" readonly="readonly" style="width:350px;"/>                            </td>                            <td width="70" style="text-align: center">                                <c:choose>                                    <c:when test="${requisito.indicador == '1'}">                                        <input type="checkbox" name="chkTblObligatorio" checked="checked" onclick="fn_cambiarEstadoObligatorio();"/>                                    </c:when>                                    <c:otherwise>                                        <input type="checkbox" name="chkTblObligatorio" onclick="fn_cambiarEstadoObligatorio();" />                                    </c:otherwise>                                </c:choose>                               </td>                            <td width="71">                                <div id="btn-opciones">                                    <button id="btn-Cambiar" title="Cambiar o reemplazar requisito" class="btn btn-default btn-xs" type="button" onclick="fn_cambiarTupaRequisito('${requisito.codRequisito}');" codMot="${requisito.codRequisito}" indicadorReg="${requisito.indicador}" ><span class="glyphicon glyphicon-list-alt"></span></button>                                </div>                            </td>                                                    </tr>                    </c:forEach>                </tbody>            </table>        </div>        <input type="hidden" id="codReqReempl"/>        <input type="hidden" id="indRegistro"/>    </div>    <script>fn_eventTupaRequisito();</script></jsp:root>