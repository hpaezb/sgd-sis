<jsp:root xmlns:tiles="http://tiles.apache.org/tags-tiles" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:form="http://www.springframework.org/tags/form">    <jsp:directive.page contentType="text/html;charset=UTF-8"/>    <jsp:output omit-xml-declaration="yes"/>        <a href="#" class="list-group-item active" onclick="refrescarBandejaEntrada();" title="Actualizar Cambios">        <h4 class="list-group-item-heading" style="text-align: center;font-size:21px" ><span class="glyphicon glyphicon-refresh"><jsp:text/></span> Documentos de ${siglasDep}</h4>        <p class="list-group-item-text">Resumen de los documentos principales de <strong>${siglasDep}</strong>.</p>    </a>    <c:forEach var="BandEntrada" items="${bandejaEntradaList}" varStatus="status">        <c:if test="${ BandEntrada.coBandeja eq 'P'}">           <div href="#" class="list-group-item" style="margin-left:40px;">            <span   onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');" class="badge" style=" background-color:#4E4E4E !important; font-size:20px; cursor:pointer;">${BandEntrada.nuCan}</span>                                <h4 style="cursor:pointer;"  onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');" class="list-group-item-heading">${BandEntrada.dePen}</h4>            <div class="list-group-item-text" style="margin-bottom: 8px;cursor:pointer;"><a style="cursor:pointer;"  onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');" >${BandEntrada.deResumen}            </a>                           <c:if test="${ BandEntrada.coBandeja eq 'P'}">                    <div style="float:right;">                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','3');" class="badge"  title="Muy Urgentes" style="border-radius: 3px!Important; background-color:#D9534F !important; ">${BandEntrada.nuCanMuyUrgente}</span>                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','2');" class="badge" title="Urgentes" style="margin-left:2px;border-radius: 3px!Important; background-color:#F0AD4E !important;">${BandEntrada.nuCanUrgente}</span>                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','1');" class="badge" title="Normales" style="margin-left:2px;border-radius: 3px!Important; background-color:#428bca;">${BandEntrada.nuCanNormal}</span>                     </div>                </c:if>            </div>        </div>        </c:if>        <c:if test="${ BandEntrada.coBandeja != 'P'}">           <div  style="cursor:pointer;"  class="list-group-item" >            <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');" class="badge" style=" background-color:#4E4E4E !important; font-size:20px; cursor:pointer;">${BandEntrada.nuCan}</span>                                <h4  onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');"  class="list-group-item-heading">${BandEntrada.dePen}</h4>            <div class="list-group-item-text" style="margin-bottom: 8px; cursor:pointer;"><a style="cursor:pointer;"  onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','');" >${BandEntrada.deResumen}            </a>                            <c:if test="${ BandEntrada.coBandeja eq 'O'}">                    <div style="float:right;">                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','3');" class="badge"  title="Muy Urgentes" style="border-radius: 3px!Important; background-color:#D9534F !important; ">${BandEntrada.nuCanMuyUrgente}</span>                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','2');" class="badge" title="Urgentes" style="margin-left:2px;border-radius: 3px!Important; background-color:#F0AD4E !important;">${BandEntrada.nuCanUrgente}</span>                        <span onclick="preloadMostrarVentana('${BandEntrada.tiPen}', '${BandEntrada.coDep}', '${BandEntrada.coBandeja}','1');" class="badge" title="Normales" style="margin-left:2px;border-radius: 3px!Important; background-color:#428bca;">${BandEntrada.nuCanNormal}</span>                     </div>                </c:if>            </div>        </div>        </c:if>            </c:forEach>    <c:forEach var="etiquetas" items="${etiquetasList}" varStatus="status">        <c:set var="auxIco" value="glyphicon glyphicon-tag"/>        <a href="#" class="list-group-item" onclick="mostrarListaEtiquetas('${etiquetas.coEst}','${etiquetas.coDepDes}')">            <span class="badge">${etiquetas.numeroDocumentos}</span>            <h6 class="list-group-item-heading" style="font-size: 18px;font-style: italic;"><span class="${auxIco}"><jsp:text/><input type="hidden" name="icoEtiqueta" value="${etiquetas.coEst}" /></span> ${etiquetas.descripcion}</h6>        </a>    </c:forEach>    <script>        $(document).ready(function(){            mostrarEtiquetas();                    });    </script></jsp:root>      